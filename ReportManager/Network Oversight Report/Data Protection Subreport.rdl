    <DataSet Name="GetEndDate">
      <Fields>
        <Field Name="UpdatedEndDate">
          <DataField>UpdatedEndDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>config</DataSourceName>
        <CommandText>SELECT CONVERT(varchar,DATEADD(SECOND,59,DATEADD(MINUTE,59,DATEADD(HOUR,23,CONVERT(varchar,[dbo].[GetEndDate](@EndDate, GETDATE(), @Subscription),111)))),120) AS UpdatedEndDate</CommandText>
