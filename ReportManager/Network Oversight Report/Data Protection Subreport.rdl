    <DataSet Name="dsCheckIfBackupmanagerOnly">
      <Fields>
        <Field Name="Result">
          <DataField>Result</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>warehouse_database</DataSourceName>
        <CommandText>IF EXISTS (
SELECT 1 
FROM  
	[warehouse].[dbo].[fact_BackupServices] 
WHERE 
	CustomerID = @CustomerID
	AND
	[Job Type] &lt;&gt; @BackupManagerJobType
	)
	SELECT 1 AS Result
ELSE
	SELECT 0 AS Result</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BackupManagerJobType">
            <Value>=Parameters!BackupManagerJobType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
