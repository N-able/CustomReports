  <Code>Public Function BackupStatus(ByVal code As Integer) As String
	Dim Description As String
	Select Case code
		Case 1 
			Description = "Success"
		Case 2 
			Description = "Warnings"
		Case 3 
			Description = "Failed"
		Case Else 
			Description = "Unknown"
	End Select
	Return Description
End Function

Public Function BackupResult(ByVal code As Integer) As String
	Dim Description As String
	Select Case code
		Case 1 
			Description = "Number of successful backups:"
		Case 2 
			Description = "Number of backups completed with warnings:"
		Case 3 
			Description = "Number of failed backups:"
		Case Else 
			Description = "Number of backups with unknown result:"
	End Select
	Return Description
End Function

Public Shared Function TranslateStatus(ByVal s As Integer) As String
	Select case s 
		case 1 
			TranslateStatus = "Job terminated due to cancellation."
		case 2 
			TranslateStatus = "Job has been completed by the engine and is waiting final disposition."
		case 3 
			TranslateStatus = "Job terminated with success but there were some exceptions."
		case 4 
			TranslateStatus = "Job has been sent for execution."
		case 5 
			TranslateStatus = "Job is in a hold state."
		case 6 
			TranslateStatus = "Job terminated with an error."
		case 7 
			TranslateStatus = "The schedule for the task is invalid."
		case 8 
			TranslateStatus = "The time window is mutually exclusive thus job will never run."
		case 9 
			TranslateStatus = "Job is eligible for dispatch and is late."
		case 10 
			TranslateStatus = "Date of job makes it eligible to run, but time is not in window."
		case 11 
			TranslateStatus = "Job is ready, but dispatcher is paused."
		case 12 
			TranslateStatus = "The job needs to be dispositioned to an actual state."
		case 13 
			TranslateStatus = "The system forced recovery of the job."
		case 14 
			TranslateStatus = "Job has been disabled in the system."
		case 15 
			TranslateStatus = "The job will be restarted with check point restart enabled, this value is only set in the job history summary."
		case 16 
			TranslateStatus = "Job is currently running on server."
		case 17 
			TranslateStatus = "Job is eligible for dispatch."
		case 18 
			TranslateStatus = "The job has a due date in the future."
		case 19 
			TranslateStatus = "Job terminated with success."
		case 20 
			TranslateStatus = "Job is ready, but another higher precedence task is eligible to run."
		case 21 
			TranslateStatus = "The job was aborted due to Abort Threshold time-out."
		case 22 
			TranslateStatus = "The job needs to have the due date calculated."
		case 23 
			TranslateStatus = "The job is linked to another job so will not start until the master job is finished."
	End Select
End Function

