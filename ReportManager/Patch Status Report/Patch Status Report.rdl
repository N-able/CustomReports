      <Query>
        <DataSourceName>Warehouse</DataSourceName>
        <CommandText>DECLARE @DevicesGood int
DECLARE @DevicesWithMissing int
DECLARE @DevicesWithMissingAll int
DECLARE @DevicesWithFailed int

CREATE TABLE #result 
( 
    DevicesGood INT, 
    DevicesWithMissing INT,
    DevicesWithFailed INT,
    DevicesGoodPCT Float, 
    DevicesWithMissingPCT Float,
    DevicesWithFailedPCT Float,
    ManagedDevices INT
)

DECLARE @EndDateID int
SET @EndDateID = (SELECT DateID FROM dim_Date WHERE [Date] = @EndDate)
  
DECLARE @DatasourceID as tinyint
SELECT @DatasourceID=DatasourceID FROM dim_Customer WHERE customerid=@CustomerID  

DECLARE @TotalManagedDevices int
SET @TotalManagedDevices = 
	(
		SELECT COUNT(*) 
		FROM [Fact_Device Management History] 
		WHERE 
				([Date Device Management Started On] &lt;= @EndDateID) 
			AND (
					   @EndDateID &lt;= [Date Device Management Ended On] 
					OR [Date Device Management Ended On] IS NULL
				)
			AND CustomerID = @CustomerID
			AND DeviceID IN 
				(	
					SELECT DeviceID 
					FROM dim_Device 
					WHERE 
							(CAST(FLOOR(CAST(CreatedOn AS FLOAT)) AS DATETIME)) &lt;= @EndDate 
						AND (
								   @EndDate &lt;= (CAST(FLOOR(CAST(DeletedOn AS FLOAT)) AS DATETIME)) 
								OR DeletedOn IS NULL
							)
						AND [is managed device indicator] = 'Managed Device' 
						AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
						AND [IsProbe Device Indicator] = 'Not a Probe Device' 
						AND CustomerID = @CustomerID
						AND DataSourceID=@DatasourceID 
				)
			AND DeviceID in 
				(
					SELECT Distinct DeviceID
					FROM 
							[fact_Device Services] fs 
						inner join 
							dim_Service s on fs.datasourceid=s.DataSourceID and fs.serviceid=s.ServiceID
					WHERE 
							fs.customerid=@CustomerID 
						AND fs.DataSourceID=@DatasourceID
						AND s.ServiceName='Patch Status'
				)					
	)

SELECT @DevicesWithMissingAll = COUNT(DISTINCT deviceid)
FROM [fact_cim_missingpatch]
WHERE
	CustomerID = @CustomerID
	AND [Installation Status] in ('Not Installed','Failed')
	AND ScanTime &gt;= @StartDate 
	AND ScanTime &lt;= @EndDate	

SELECT @DevicesWithMissing = COUNT(DISTINCT deviceid)
FROM [fact_cim_missingpatch]
WHERE
	CustomerID = @CustomerID
	AND [Installation Status] = 'Not Installed'
	AND ScanTime &gt;= @StartDate 
	AND ScanTime &lt;= @EndDate	
	
SELECT @DevicesWithFailed = COUNT(DISTINCT deviceid)
FROM [fact_cim_missingpatch]
WHERE
	CustomerID = @CustomerID
	AND [Installation Status] = 'Failed'	
	AND ScanTime &gt;= @StartDate 
	AND ScanTime &lt;= @EndDate	

SET @DevicesGood = @TotalManagedDevices - @DevicesWithMissingAll 	
		
INSERT INTO #result VALUES (
	 @DevicesGood
	,@DevicesWithMissing
	,@DevicesWithFailed
	,((@DevicesGood+0.000001)*100/(@TotalManagedDevices+0.000001))
	,((@DevicesWithMissing+0.000001)*100/(@TotalManagedDevices+0.000001))
	,((@DevicesWithFailed+0.000001)*100/(@TotalManagedDevices+0.000001))
	,@TotalManagedDevices  )

SELECT 
	DevicesGood as 'Devices with no missing patches'
	,DevicesWithMissing as 'Devices with missing patches'
	,DevicesWithFailed as 'Devices with failed patches'
	,DevicesGoodPCT  as 'Devices with no missing patches percent'
	,DevicesWithMissingPCT as 'Devices with missing patches percent'
	,DevicesWithFailedPCT as 'Devices with failed patches percent'
	,ManagedDevices as 'Total managed devices'
FROM #result	
	
DROP TABLE #result</CommandText>
