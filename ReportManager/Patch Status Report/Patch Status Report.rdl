      <Query>
        <DataSourceName>Warehouse</DataSourceName>
        <CommandText>SELECT
   d.[Device Class]
  ,d.[DeviceName]
  ,p.[Patch Name]
  ,p.[Patch Description]
  ,p.Product
  ,p.[Patch Category]
  ,DATEDIFF(dd,p.[Release Date],@EndDate) as 'Age'
  ,(CASE p.[Patch Category]
	WHEN 'Security Updates' THEN 1
	WHEN 'Critical Updates' THEN 2
	WHEN 'Service Packs' THEN 3
	WHEN 'Update Rollups' THEN 4
	WHEN 'Feature Packs' THEN 5
	WHEN 'Updates' THEN 6
	WHEN 'Drivers' THEN 7
	WHEN 'Definition Updates' THEN 8
	WHEN 'Tools' THEN 9
	WHEN 'Office 2007' THEN 10
	ELSE 11
  END) PatchPriority
FROM
  [fact_cim_MissingPatch] p
	INNER JOIN dim_Customer c
		ON p.customerID = c.CustomerID
	INNER JOIN dim_Device d
		ON p.DeviceID = d.DeviceID
WHERE
	d.CustomerID = @CustomerID
	AND p.[Installation Status] = 'Not Installed'
	AND p.ScanTime &gt;= @StartDate 
	AND	p.ScanTime &lt;= @EndDate</CommandText>
        <QueryParameters>
