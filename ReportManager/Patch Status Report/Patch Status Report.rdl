    <DataSet Name="InstalledByDeviceClass">
      <Fields>
        <Field Name="Device_Class">
          <DataField>Device Class</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DeviceName">
          <DataField>DeviceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Installed_Patches">
          <DataField>Installed Patches</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Warehouse</DataSourceName>
        <CommandText>DECLARE @EndDateID int
SET @EndDateID = (SELECT DateID FROM dim_Date WHERE [Date] = @EndDate)
  
DECLARE @DatasourceID as tinyint
SELECT @DatasourceID=DatasourceID FROM dim_Customer WHERE customerid=@CustomerID  

SELECT DISTINCT DeviceID, CustomerID, DatasourceID INTO #ManagedDevices
FROM [Fact_Device Management History] 
WHERE 
		([Date Device Management Started On] &lt;= @EndDateID) 
	AND (
			   @EndDateID &lt;= [Date Device Management Ended On] 
			OR [Date Device Management Ended On] IS NULL
		)
	AND CustomerID = @CustomerID
	AND DeviceID IN 
		(	
			SELECT DeviceID 
			FROM dim_Device 
			WHERE 
					(CAST(FLOOR(CAST(CreatedOn AS FLOAT)) AS DATETIME)) &lt;= @EndDate 
				AND (
						   @EndDate &lt;= (CAST(FLOOR(CAST(DeletedOn AS FLOAT)) AS DATETIME)) 
						OR DeletedOn IS NULL
					)
				AND [is managed device indicator] = 'Managed Device' 
				AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
				AND [IsProbe Device Indicator] = 'Not a Probe Device' 
				AND CustomerID = @CustomerID
				AND DataSourceID=@DatasourceID 
		)
	AND DeviceID in 
		(
			SELECT Distinct DeviceID
			FROM 
					[fact_Device Services] fs 
				inner join 
					dim_Service s on fs.datasourceid=s.DataSourceID and fs.serviceid=s.ServiceID
			WHERE 
					fs.customerid=@CustomerID 
				AND fs.DataSourceID=@DatasourceID
				AND s.ServiceName='Patch Status'
		)				
	
SELECT Distinct d.[Device Class], d.DeviceName, 
	SUM(
		CASE
			WHEN p.DeviceID is null 
				THEN 0 
			ELSE 
				(CASE
					WHEN p.InstalledDate &gt;= @StartDate  AND	p.InstalledDate &lt;= @EndDate 
					THEN 1 
					ELSE 0 
				END)
		end 
	   ) [Installed Patches]
FROM 
		#ManagedDevices m
	INNER JOIN 
		dim_Device d ON 
				m.CustomerID = d.CustomerID
			AND m.DataSourceID = d.DataSourceID
			AND m.DeviceID = d.DeviceID			
	LEFT JOIN
		fact_cim_Patch p ON d.DeviceID = p.DeviceID		
WHERE
		d.CustomerID = @CustomerID 
GROUP BY d.[Device Class],d.DeviceName
		
		
DROP TABLE #ManagedDevices</CommandText>
        <QueryParameters>
