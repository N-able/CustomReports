        </Field>
        <Field Name="DaysFromLastScan">
          <DataField>DaysFromLastScan</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Version">
          <DataField>Version</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ScanTime">
          <DataField>ScanTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="AVDetected">
          <Value>=IIf(Fields!AVSolutionName.Value="Kein AV erkannt" OR IsNothing(Fields!AVSolutionName.Value) OR (Fields!AVSolutionType.Value=1 AND IsNothing(Fields!Version.Value)), 0, 1)</Value>
        </Field>
        <Field Name="AVDisplayName">
          <Value>=IIF(Fields!AVDetected.Value=0 ,"Kein AV installiert",Fields!AVSolutionName.Value)</Value>
        </Field>
        <Field Name="AVDisplayVersion">
          <Value>=IIF(Fields!AVDetected.Value=0 ,"Unbekannt",Fields!Version.Value)</Value>
        </Field>
