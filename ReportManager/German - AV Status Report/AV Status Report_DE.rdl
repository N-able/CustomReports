    <DataSet Name="dsAVActivitySQL">
      <Query>
        <DataSourceName>WarehouseDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DeviceClassID">
            <Value>=Parameters!Device_Class_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RuleID">
            <Value>=Parameters!Rule_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DeviceID">
            <Value>=Parameters!Device_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @UpdatedEndDate   datetime

SET @UpdatedEndDate = DATEADD(DAY, 1, @EndDate)

IF (OBJECT_ID('tempdb..#tmp_AVDevices') IS NOT NULL)
	DROP TABLE #tmp_AVDevices
	
CREATE TABLE #tmp_AVDevices (DeviceID int not null)	

INSERT INTO #tmp_AVDevices (DeviceID)
SELECT DeviceID 
FROM dbo.dim_Device
WHERE 
	[Custom DeviceClassID]	 IN (@DeviceClassID)
	AND CustomerID = @CustomerID
	AND	[Is Managed Device Indicator] = 'Managed Device'
	AND [IsProbe Device Indicator] = 'Not a Probe Device'
	AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
	AND ([Status] = 'Active' OR [Status] = @Status)
	
INSERT INTO #tmp_AVDevices (DeviceID)
SELECT d.DeviceID 
FROM 
	dbo.dim_Device d 
		INNER JOIN
	dbo.[fact_Device Folder] f
			ON d.DeviceID=f.DeviceID
WHERE 
		f.[FolderID] IN (@RuleID)
	AND f.CustomerID = @CustomerID
	AND	[Is Managed Device Indicator] = 'Managed Device'
	AND [IsProbe Device Indicator] = 'Not a Probe Device'
	AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
	AND (d.[Status] = 'Active' OR d.[Status] = @Status)	
	
INSERT INTO #tmp_AVDevices (DeviceID)
SELECT DeviceID 
FROM 
	dbo.dim_Device  
WHERE 
		DeviceID IN (@DeviceID)
	AND CustomerID = @CustomerID
	AND	[Is Managed Device Indicator] = 'Managed Device'
	AND [IsProbe Device Indicator] = 'Not a Probe Device'
	AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
	AND ([Status] = 'Active' OR [Status] = @Status)	

SELECT 
	d.[Device Class]
	,d.DeviceName
	,CASE 
		WHEN ISNULL(f.AVSolutionName, 'Unbekannt') &lt;&gt; 'Unbekannt' THEN ISNULL(f.AVSolutionName, 'Unbekannt')
		ELSE ISNULL(fl.AVSolutionName, 'Unbekannt')
	END	AS AVSolution
	,SUM([Count Of Virus Detections]) AS VirusDetections
	,SUM([COUNT Of Successful Virus Cleans]) AS VirusRemovalSuccess
	,SUM([Count Of Failed Virus Cleans]) AS VirusRemovalFailed
FROM 
	dbo.[fact_SEM_Anti-virus Activity] a
		INNER JOIN  
	dbo.dim_Device d
			ON a.DeviceID=d.DeviceID
		INNER JOIN 
	(SELECT DISTINCT DeviceID FROM #tmp_AVDevices) td
			ON td.DeviceID = d.DeviceID
		INNER JOIN
	dbo.dim_Date dd
			ON dd.DateID = a.DateID
		LEFT OUTER JOIN
	dbo.fact_AVSecurity f
			ON f.DeviceID = a.DeviceID 
			AND f.DateID = a.DateID
			AND f.TimeID = a.TimeID 
		LEFT OUTER JOIN
		-- The latest AVSolutionName within specified period
		(
		SELECT fav.DeviceID, fav.AVSolutionName
		FROM 
			dbo.fact_AVSecurity fav
				INNER JOIN
			(
			SELECT favs.DeviceID, MAX(ScanTime) AS LastScanTime
			FROM 
				dbo.fact_AVSecurity favs
					INNER JOIN 
				(SELECT DISTINCT DeviceID FROM #tmp_AVDevices) tavd
						ON tavd.DeviceID = favs.DeviceID	
			WHERE 
				favs.[ScanTime] &lt; @UpdatedEndDate
				AND favs.AVSolutionName IS NOT NULL
			GROUP BY
				favs.DeviceID
			) faq
					ON fav.DeviceID = faq.DeviceID
					AND fav.ScanTime = faq.LastScanTime
		) fl
			ON fl.DeviceID = d.DeviceID 			
			
WHERE 
	a.CustomerID = @CustomerID
	AND dd.[Date]&gt;=@StartDate
	AND dd.[Date]&lt;@UpdatedEndDate
GROUP BY
	d.[Device Class]
	,d.DeviceName
	,ISNULL(f.AVSolutionName, 'Unbekannt')
	,ISNULL(fl.AVSolutionName, 'Unbekannt')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
