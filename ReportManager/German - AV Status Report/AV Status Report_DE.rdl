        </Field>
        <Field Name="VirusDetections">
          <DataField>VirusDetections</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="VirusRemovalSuccess">
          <DataField>VirusRemovalSuccess</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="VirusRemovalFailed">
          <DataField>VirusRemovalFailed</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
