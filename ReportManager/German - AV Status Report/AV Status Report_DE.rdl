      <Query>
        <DataSourceName>WarehouseDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DeviceClassID">
            <Value>=Parameters!Device_Class_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
