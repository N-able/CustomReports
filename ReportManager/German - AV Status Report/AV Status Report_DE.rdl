          </QueryParameter>
          <QueryParameter Name="@EndTime">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartTime">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF (OBJECT_ID('tempdb..#tmp_AVDevices') IS NOT NULL)
	DROP TABLE #tmp_AVDevices
	
CREATE TABLE #tmp_AVDevices (DeviceID int not null, 
      [Device Class] nvarchar(50) not null
      ,DeviceName nvarchar(100) not null)	

INSERT INTO #tmp_AVDevices (DeviceID, [Device Class], DeviceName)
SELECT DeviceID, [Device Class], DeviceName
FROM dbo.dim_Device
WHERE 
	[Custom DeviceClassID] IN (@DeviceClassID)
	AND CustomerID = @CustomerID
	AND	[Is Managed Device Indicator] = 'Managed Device'
	AND [IsProbe Device Indicator] = 'Not a Probe Device'
	AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
	AND ([Status] = 'Active' OR [Status] = @Status)
	
INSERT INTO #tmp_AVDevices (DeviceID, [Device Class], DeviceName)
SELECT d.DeviceID, [Device Class], DeviceName
FROM 
	dbo.dim_Device d 
		INNER JOIN
	dbo.[fact_Device Folder] f
			ON d.DeviceID=f.DeviceID
WHERE 
		f.[FolderID] IN (@RuleID)
	AND f.CustomerID = @CustomerID
	AND d.[Is Managed Device Indicator] = 'Managed Device'
	AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'
	AND d.[IsProbe Device Indicator] = 'Not a Probe Device'
	AND (d.[Status] = 'Active' OR d.[Status] = @Status)	
	
INSERT INTO #tmp_AVDevices (DeviceID, [Device Class], DeviceName)
SELECT DeviceID, [Device Class], DeviceName
FROM 
	dbo.dim_Device  
	
WHERE 
	DeviceID IN (@DeviceID)
	AND CustomerID = @CustomerID
	AND	[Is Managed Device Indicator] = 'Managed Device'
	AND [IsProbe Device Indicator] = 'Not a Probe Device'
	AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
	AND ([Status] = 'Active' OR [Status] = @Status)	


; with LatestScan(DeviceID, AVSolutionName, LastScantime, RowID)
AS (
	SELECT 
		a.[DeviceID] 
		,[AVSolutionName]
		,MAX([ScanTime]) AS LastScanTime
		,row_number() over (Partition by a.[DeviceID] ORDER BY  MAX([ScanTime]) desc) as RowID
	FROM [dbo].[fact_AVSecurity] a

	WHERE
		a.CustomerID = @CustomerID
		AND a.[ScanTime]&lt;DateAdd(day, 1, @EndTime)
		AND a.[ScanTime] &gt; @StartTime
	GROUP BY 	
		  a.[DeviceID]
		  ,[AVSolutionName]
)

SELECT d.[Device Class]
	,d.DeviceName
	,ISNULL(l.[AVSolutionName], 'Kein AV erkannt') AS [AVSolutionName]
	,a.AVSolutionType
	,isnull(a.DefinitionsUpToDate, 0) as DefinitionsUpToDate
	,isnull(a.ScanningEnabled, 0) as ScanningEnabled
	,a.DaysFromLastScan
	,a.Version
	,l.LastScanTime as ScanTime
FROM #tmp_AVDevices d 
	LEFT JOIN LatestScan l
		on l.DeviceID = d.DeviceID
		and l.RowID = 1
	LEFT JOIN dbo.fact_AVSecurity as a 
		on a.DeviceID = l.DeviceID
		and a.ScanTime = l.LastScanTime
WHERE ((d.[Device Class] like '%Windows%')
		OR (d.[Device Class] not like '%Windows%' AND l.AVSolutionName is not null))

		
			
IF (OBJECT_ID('tempdb..#tmp_AVDevices') IS NOT NULL)
	DROP TABLE #tmp_AVDevices</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
