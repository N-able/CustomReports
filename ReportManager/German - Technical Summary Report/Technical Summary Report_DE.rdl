        <CommandText>SET NOCOUNT ON

	SELECT @StartDate = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
	SELECT @EndDate = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

	DECLARE @StartDateIndex int, @EndDateIndex int
	SELECT @StartDateIndex = dbo.dateIndex(@StartDate)
	SELECT @EndDateIndex   = dbo.dateIndex(@EndDate)

-- Declare @allDayFlag Indicator
DECLARE @allDay bit

-- Set All Day to true if the End Hour equals Start Hour 
IF (@End_Hour = @Start_Hour)
    SET @allday = 1
ELSE
	 SET @allDay = 0
---------------------&gt;&gt;


	-- Create temp. table to store all devices.
	CREATE TABLE #mDeviceList (
		[DeviceID]	int,
		[DeviceName]	nvarchar(100) COLLATE DATABASE_DEFAULT ,
		[Device Class] nvarchar(50) COLLATE DATABASE_DEFAULT ,
		[Deleted]	nvarchar(20)COLLATE DATABASE_DEFAULT )
		
	-- Get all devices from device list and place in the temporary table.
	INSERT INTO #mDeviceList
	SELECT	DeviceID AS [Value], [DeviceName], [Device Class], [Status]
	FROM	[dim_Device] 
	WHERE [DeviceID] IN (@Devices)

	-- Create a table to store the overall device averages.
	CREATE TABLE #mUtil(
		[DeviceID] int,
		[Average Units Consumed] float,
		MaxUnits float) 

	-- Calculate the overall average for each device.
	INSERT INTO #mUtil
	SELECT	[fact_Utilization].[DeviceID], 
			AVG(case
  	           when [Usage Percentage] &lt; 0 then 0.0
  	           when [Usage Percentage] &gt; 100 then 100.0
  	           else [Usage Percentage]
  	         end),
  	         MAX(ISNULL([Maximum Units],0)) AS MaximumUnits
	FROM	[fact_Utilization] INNER JOIN #mDeviceList dev ON fact_Utilization.DeviceID = dev.[DeviceID]
			inner join dim_Date AS dat ON dat.[DateID] = [fact_Utilization].[DateID]

	WHERE	([fact_Utilization].DateID &gt;= @StartDateIndex) AND 
	        ([fact_Utilization].DateID &lt;= @EndDateIndex) AND
		([fact_Utilization].UtilizationTypeID = @UtilizationTypeID) AND
		([fact_Utilization].[Maximum Units] &gt; 0 OR [fact_Utilization].[Usage Percentage] is not null)

		
			and ( 
					--Check if Time SLA Is Included				
					(@IncludeBreakdown = 0)	
					OR -- Check if We are Reporting an All DAY
					(@allDay = 1)			  
					OR  -- Include Time Period
					( dbo.[fact_Utilization].TimeID between @Start_Hour and @End_Hour )					
					
				)
			and (
				 --Check if time Sla is Included
				(@IncludeBreakdown = 0)
				OR -- check if All Days of Week indicated
				(@WeekType = 'Weekday, Weekend')
				OR -- filter by Weekend or WeekDay
				(@WeekType =  dat.[Weekend Indicator])								
			)
	GROUP BY [fact_Utilization].[DeviceID]

	-- Create a temp. table to store the daily average data (for daily high and low).
	CREATE TABLE #mDailyAverages (
		[Date]				datetime,
		[DeviceID]			int,
		[Daily Average Utilization]	float)

	-- Calculate the Daily High and Daily Low Using Daily Averages.
	INSERT INTO #mDailyAverages
	SELECT	dat.[Date],
		dev.[DeviceID],
		AVG(case
  	           when util.[Usage Percentage] &lt; 0 then 0.0
  	           when util.[Usage Percentage] &gt; 100 then 100.0
  	           else util.[Usage Percentage]
  	         end)
	FROM	fact_Utilization AS util INNER JOIN #mDeviceList dev ON util.DeviceID = dev.[DeviceID] INNER JOIN 
			dim_Date AS dat ON dat.[DateID] = util.[DateID]
	WHERE	([UtilizationTypeID] = @UtilizationTypeID) AND
		((util.[DateID] &gt;= @StartDateIndex) AND 
		(util.[DateID] &lt;= @EndDateIndex)) AND
		(util.[Maximum Units] &gt; 0 OR util.[Usage Percentage] is not null)

	
			and ( 
					--Check if Time SLA Is Included				
					(@IncludeBreakdown = 0)	
					OR -- Check if We are Reporting an All DAY
					(@allDay = 1)			  
					OR  -- Include Time Period
					( util.TimeID between @Start_Hour and @End_Hour )					
					
				)
			and (
				 --Check if time Sla is Included
				(@IncludeBreakdown = 0)
				OR -- check if All Days of Week indicated
				(@WeekType = 'Weekday, Weekend')
				OR -- filter by Weekend or WeekDay
										
				(@WeekType = dat.[Weekend Indicator])
				
			)
	GROUP BY  dat.[Date], dev.[DeviceID]

	Declare @CmdString nvarchar(3000)
	Declare @SortBy nvarchar(200)

	If (@IncludeUBR = 0) 
	 begin 
		Select @CmdString = 'Select ' 
		Select @SortBy = 'ORDER BY dev.[DeviceName], a.[Average Units Consumed] '
	 end
	else
	 begin
		Select @CmdString = 'SELECT top (' + convert (nvarchar(14),@TopNDevices)  + ') '
		Select @SortBy = 'ORDER BY a.[Average Units Consumed] desc '
	 end

	-- Bug 1361
	IF (OBJECT_ID('tempdb..#memUtilizationUnits') IS NOT NULL)
		DROP TABLE #memUtilizationUnits
		
	CREATE TABLE #memUtilizationUnits  (
		DeviceID				int,
		MaxDateID				int,
		MaxTimeID				int,
		MaximumUnits			bigint
		)
	CREATE NONCLUSTERED INDEX [IX_tmp_UtilizationUnits] ON #memUtilizationUnits 
	(
		[DeviceID] ASC
	)

	INSERT INTO	#memUtilizationUnits
	SELECT 
			dev.DeviceID,
			MAX(DateID) AS MaxDateID,
			NULL AS MaxTimeID,
			NULL AS MaximumUnits
	FROM 
			warehouse.dbo.fact_Utilization util
				INNER JOIN
			#mDeviceList dev 
					ON util.[DeviceID] = dev.[DeviceID]
	WHERE	
			util.[DateID] &lt;= @EndDateIndex 
			AND util.[UtilizationTypeID]=@UtilizationTypeID
			AND ISNULL(util.[Maximum Units],0) &gt; 0 
	GROUP BY	
			dev.DeviceID
			
	UPDATE #memUtilizationUnits
	SET 	MaxTimeID = util.MaxTimeID
	FROM 	
		#memUtilizationUnits a
			INNER JOIN
		( 
			SELECT 
				DateID, 
				u.DeviceID,
				MAX(TimeID) AS MaxTimeID 
			FROM 
				warehouse.dbo.fact_Utilization u
					INNER JOIN
				#mDeviceList AS dev 
						ON u.[DeviceID] = dev.[DeviceID] 
						AND u.[UtilizationTypeID]=@UtilizationTypeID 
			GROUP BY u.DeviceID, DateID
		) util
				ON  util.DeviceID=a.DeviceID
				AND util.DateID=a.MaxDateID
				
	UPDATE #memUtilizationUnits
	SET 	MaximumUnits = util.[Maximum Units]
	FROM 	
		#memUtilizationUnits a
			INNER JOIN warehouse.dbo.fact_Utilization util
			ON 
				util.DeviceID=a.DeviceID
				AND util.[UtilizationTypeID]=@UtilizationTypeID
				AND util.DateID=a.MaxDateID	
				AND util.TimeID=a.MaxTimeID				

-- Get the data for the period we need to analyze
-- N-compass 3.1 SP1 UTILDTPR-1 extension. Assumption: In multi-CPU systems, all CPUs are identical
	Select  @CmdString =( @CmdString + 'dev.[DeviceName],dev.[Device Class],
		   MIN([Daily Average Utilization]) AS MinUtil,
		   MAX([Daily Average Utilization]) AS MaxUtil,
		   a.[Average Units Consumed] AS Utilization,
		   ISNULL(STDEV([Daily Average Utilization]),0) AS DevUtil,
		   cpn.cpuname AS CPUName,
		   MAX(cpn.ncpus) AS NCPUs,
		   MAX(CONVERT(numeric(15,1),u.MaximumUnits)) AS PhysicalMemory,
		   MAX(a.MaxUnits) AS TotalUnits
	FROM       #mUtil AS a 
		INNER JOIN #mDailyAverages AS daily ON a.DeviceID = daily.DeviceID
		INNER JOIN #mDeviceList AS dev ON a.DeviceID = dev.DeviceID
		LEFT OUTER JOIN 
			(SELECT DISTINCT 
				pa.deviceid,
				pb.cpuname,
				pb.ncpus
			FROM 
				dbo.fact_cim_processor pa 
			INNER JOIN 
				(SELECT deviceid,COUNT(cpuid) AS ncpus, MAX(ISNULL(name,'''')) AS cpuname
				FROM  dbo.fact_cim_processor
				GROUP BY deviceid
				) pb
			ON pa.deviceid=pb.deviceid 
			) cpn
		ON cpn.deviceid=a.DeviceID
				LEFT OUTER JOIN 
			#memUtilizationUnits u -- Bug 1361
					ON  a.DeviceID=u.DeviceID
		GROUP BY dev.[DeviceName], dev.[Device Class],a.[Average Units Consumed],cpn.cpuname ' +
	 @SortBy);

	EXEC (@CmdString)
	
	IF (OBJECT_ID('tempdb..#memUtilizationUnits') IS NOT NULL)
		DROP TABLE #memUtilizationUnits</CommandText>
