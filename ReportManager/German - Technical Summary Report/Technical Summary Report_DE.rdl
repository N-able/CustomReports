		([fact_Utilization].UtilizationTypeID = @UtilizationTypeID) AND
		([fact_Utilization].[Maximum Units] &gt; 0 OR [fact_Utilization].[Usage Percentage] is not null)

		
			and ( 
					--Check if Time SLA Is Included				
					(@IncludeBreakdown = 0)	
					OR -- Check if We are Reporting an All DAY
					(@allDay = 1)			  
					OR  -- Include Time Period
					( dbo.[fact_Utilization].TimeID between @Start_Hour and @End_Hour )					
					
				)
			and (
				 --Check if time Sla is Included
				(@IncludeBreakdown = 0)
				OR -- check if All Days of Week indicated
