    <DataSet Name="ExecutiveSummaryBandwidthIncoming">
      <Query>
        <DataSourceName>warehouse</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Devices">
            <Value>=Parameters!Devices_Traffic.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Start_Hour">
            <Value>=Parameters!Start_Hour.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@End_Hour">
            <Value>=Parameters!End_Hour.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@WeekType">
            <Value>=Parameters!WeekType.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@IncludeBreakdown">
            <Value>=Parameters!IncludeBreakdown.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@IncludeUBR">
            <Value>=Parameters!IncludeUBR.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@TopNDevices">
            <Value>=Parameters!NumTopDevices.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@SortBy">
            <Value>="Incoming"</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET NOCOUNT ON

-- Declare @allDayFlag Indicator
declare @allDay bit
DECLARE @SortingBy nvarchar(300) -- Bug 42170

-- Set All Day to true if the End Hour equals Start Hour 
if (@End_Hour = @Start_Hour)
    set @allday = 1
else
	 set @allDay = 0


	SELECT @StartDate = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
	SELECT @EndDate = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

	-- Create temp. table to store all devices.
	IF object_id('TempDb..#mDeviceList') IS NOT NULL
		DROP TABLE #mDeviceList
	CREATE TABLE #mDeviceList (
		[DeviceID]	int
		)
		
	-- Get all devices from device list and place in the temporary table.
	INSERT INTO #mDeviceList (DeviceID)
	SELECT	DeviceID AS [Value] 
	FROM dbo.dim_Device
	WHERE DeviceID IN (@Devices) 

	
	DECLARE @CmdString nvarchar(3000)
	
-- Bug 42170		
	DECLARE @OrderBy nvarchar(500)	

	IF (@IncludeUBR = 0) 
		 BEGIN 
			SELECT @CmdString = 'SELECT ' 
			SELECT @SortingBy = 'ORDER BY [dim_Device].[DeviceName],[fact_Bandwidth].[Monitored Interface],[dim_Date].[Date] '
			SET @OrderBy = ''
		 END
	ELSE
		 BEGIN
			SELECT @CmdString = 'SELECT TOP (' + CONVERT (nvarchar(14),@TopNDevices)  + ') '
			SET @SortingBy = 'ORDER BY ' + @SortBy + ' DESC, [dim_Device].[DeviceName], [fact_Bandwidth].[Monitored Interface] ASC'  -- Bug 42170 Comments 7-9
			SELECT 	@OrderBy =	CASE @SortBy
											WHEN 'Utilization'	THEN ' ORDER BY [Utilization %] DESC'
											WHEN 'Incoming'		THEN ' ORDER BY [Incoming Bytes Per Second] DESC'
											WHEN 'Outgoing'		THEN ' ORDER BY [Outgoing Bytes Per Second] DESC'
											ELSE ' ORDER BY [Utilization %] DESC'
								END
			SET @OrderBy = @OrderBy	+ ',[DeviceName] ASC,[Monitored Interface] ASC'
		 END

	-- drop table #mBandwidth
	IF object_id('TempDb..#mBandwidth') IS NOT NULL
		DROP TABLE #mBandwidth
	CREATE TABLE #mBandwidth( 
		[DeviceID] int,
		[DeviceName] nvarchar (200),
		[Monitored Interface] nvarchar (250),
		[Utilization %] float,
		[Outgoing Bytes Per Second] float,
		[Incoming Bytes Per Second] float)


	INSERT INTO #mBandwidth
	SELECT	
		[dim_Device].[DeviceID],
		[dim_Device].[DeviceName], 
		[fact_Bandwidth].[Monitored Interface],
		AVG([fact_Bandwidth].[Utilization %]),
		AVG([fact_Bandwidth].[Outgoing Bytes Per Second]),
		AVG([fact_Bandwidth].[Incoming Bytes Per Second])
	FROM	[fact_Bandwidth] INNER JOIN dim_Date ON [fact_Bandwidth].DateID = dim_Date.DateID
				 INNER JOIN dim_Device ON [fact_Bandwidth].DeviceID = dim_Device.DeviceID
				 INNER JOIN #mDeviceList dev ON dev.DeviceID = dim_Device.DeviceID
	WHERE	(dim_Date.[Date] &gt;= @StartDate) AND 
		(dim_Date.[Date] &lt;= @EndDate)
		-------------------------
		
			and ( 
					--Check if Time SLA Is Included				
					(@IncludeBreakdown = 0)	
					OR -- Check if We are Reporting an All DAY
					(@allDay = 1)			  
					OR  -- Include Time Period
					( dbo.fact_Bandwidth.TimeID between @Start_Hour and @End_Hour )					
					
				)
			and (
				 --Check if time Sla is Included
				(@IncludeBreakdown = 0)
				OR -- check if All Days of Week indicated
				(@WeekType = 'Weekday, Weekend')
				OR -- filter by Weekend or WeekDay
				(@WeekType = dbo.GetWeekEndIndicatorFromDateId(dbo.fact_Bandwidth.DateID))								
			)
		-------------------------
	GROUP BY [dim_Device].[DeviceID],[dim_Device].[DeviceName], [fact_Bandwidth].[Monitored Interface]
	--ORDER BY [dim_Device].[DeviceName], [fact_Bandwidth].[Monitored Interface]

--SELECT * FROM #mBandwidth
	
	-- Get the data for the period we need to analyze
	Select  @CmdString =( 'Select [dim_Device].[DeviceName],
		   [fact_Bandwidth].[Monitored Interface] AS Interface,
		   [dim_Date].[Date],
		   SUM(CAST([fact_Bandwidth].[Utilization %] AS float))/COUNT(*) AS Utilization,
		   SUM(CAST([fact_Bandwidth].[Outgoing bytes per second] AS float))/COUNT(*) AS Outgoing,
		   SUM(CAST([fact_Bandwidth].[Incoming bytes per second] AS float))/COUNT(*) AS Incoming
		   ,dev.[Utilization %] AS [Overall Utilization],
		   dev.[Outgoing Bytes Per Second] as [Overall  Outgoing],
		   dev.[Incoming Bytes Per Second] as [Overall Incoming]		
	FROM       [fact_Bandwidth] 
					INNER JOIN 
					(' + @CmdString + ' * FROM #mBandwidth '
					+ @OrderBy +			
					') dev ON fact_Bandwidth.DeviceID = dev.DeviceID 
														AND [fact_Bandwidth].[Monitored Interface] COLLATE Latin1_General_CI_AS = dev.[Monitored Interface] COLLATE Latin1_General_CI_AS -- Bug # 42170, 1356
				    INNER JOIN dim_Date ON [fact_Bandwidth].DateID = dim_Date.DateID 
				    INNER JOIN dim_Device ON [fact_Bandwidth].DeviceID = dim_Device.DeviceID
	WHERE      (dim_Date.[Date] &gt;='+ ''''+convert (nvarchar(14),@StartDate, 101)  +'''' +') AND 
	           (dim_Date.[Date] &lt;='+ ''''+convert (nvarchar(14),@EndDate, 101)  +'''' +')
			and ( 
					--Check if Time SLA Is Included				
					('+ ''''+convert (varchar(10),@IncludeBreakdown)+'''' +' = 0)	
					OR -- Check if We are Reporting an All DAY
					('+ ''''+convert (varchar(10),@allDay)+'''' +'= 1)			  
					OR  -- Include Time Period
					( dbo.fact_Bandwidth.TimeID between '+''''+convert (varchar(10), @Start_Hour)+'''' 
								 +'and '+ ''''+convert (varchar(10),@End_Hour)+'''' +')					
				)
			and (
				 --Check if time Sla is Included
				('+ ''''+convert (varchar(10),@IncludeBreakdown)+''''  +' = 0)
				OR -- check if All Days of Week indicated
				('+ ''''+@WeekType +'''= ''Weekday, Weekend'')
				OR -- filter by Weekend or WeekDay
				('+ ''''+@WeekType +'''= dim_Date.[Weekend Indicator])								
			)
	GROUP BY [dim_Device].[DeviceName],[fact_Bandwidth].[Monitored Interface],[dim_Date].[Date]
	,dev.[Utilization %],dev.[Outgoing Bytes Per Second],dev.[Incoming Bytes Per Second] ' + @SortingBy);  -- Bug 42170

--print @CmdString
EXEC (@CmdString)</CommandText>
