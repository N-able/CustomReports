        <CommandText>SET @StartDate = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
	SET @EndDate = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

	DECLARE @StartDateIndex int
	DECLARE @EndDateIndex int
	
	DECLARE @DiskUtilizationTypeID tinyint
	SET @DiskUtilizationTypeID = (SELECT TOP 1 UtilizationTypeID 
									FROM warehouse.dbo.[dim_Utilization Type]
									WHERE UPPER([Name])='DISK')

	SET @StartDateIndex = dbo.dateIndex(@StartDate)
	SET @EndDateIndex = dbo.dateIndex(@EndDate)

-- &lt;&lt; Newly Added (Abena)-------------

-- Declare @allDayFlag Indicator
declare @allDay bit

-- Set All Day to true if the End Hour equals Start Hour 
if (@End_Hour = @Start_Hour)
    set @allday = 1
else
	 set @allDay = 0
---------------------&gt;&gt;

	-- Create temp. table to store all devices.
	CREATE TABLE #mDeviceList (
		[DeviceID]	int,
		[DeviceName]	nvarchar(100) COLLATE DATABASE_DEFAULT ,
		[Device Class] nvarchar(50) COLLATE DATABASE_DEFAULT ,
		[Deleted]	nvarchar(20) COLLATE DATABASE_DEFAULT )
		
	-- Get all devices from device list and place in the temporary table.
	INSERT INTO #mDeviceList
	SELECT	[DeviceID], [DeviceName], [Device Class], [Status]
	FROM	[dim_Device] 
	WHERE [DeviceID] IN (@Devices)

--select * from #mDeviceList
	-- Create a table to store the overall device averages.
	CREATE TABLE #mUtil(
		[DeviceID] int,
		[Monitored Element] nvarchar(50) COLLATE DATABASE_DEFAULT ,
		[Average Units Consumed] float,
		[MaximumUnits] float
		) 

	-- Calculate the overall average for each device.
	INSERT INTO #mUtil
		SELECT	
			util.[DeviceID], 
			util.[Monitored Element], 
			AVG(
				CASE
  					   WHEN [Usage Percentage] &lt; 0 THEN 0.0
  					   WHEN [Usage Percentage] &gt; 100 THEN 100.0
  					   ELSE [Usage Percentage]
  				END) AS AverageUnitsConsumed,
	  	    MAX(ISNULL(util.[Maximum Units],0)) AS  MaximumUnits   -- N-compass 3.1 SP1 UTILDTPR-1.1: disk capacity in KB
	FROM	[fact_Utilization]  AS util INNER JOIN #mDeviceList dev ON util.DeviceID = dev.[DeviceID]
	WHERE	(util.DateID &gt;= @StartDateIndex) AND 
	        (util.DateID &lt;= @EndDateIndex) AND
		(util.UtilizationTypeID = @DiskUtilizationTypeID) AND
		(util.[Maximum Units] &gt; 0 OR util.[Usage Percentage] is not null)
		-------------------------
		
			and ( 
					--Check if Time SLA Is Included				
					(@IncludeBreakdown = 0)	
					OR -- Check if We are Reporting an All DAY
					(@allDay = 1)			  
					OR  -- Include Time Period
					( util.TimeID between @Start_Hour and @End_Hour )					
					
				)
			and (
				 --Check if time Sla is Included
				(@IncludeBreakdown = 0)
				OR -- check if All Days of Week indicated
				(@WeekType = 'Weekday, Weekend')
				OR -- filter by Weekend or WeekDay
				(@WeekType = dbo.GetWeekEndIndicatorFromDateId(util.DateID))								
			)
		---------------------
	GROUP BY util.[DeviceID], [Monitored Element]
--select * from #mUtil
	-- Create a temp. table to store the daily average data (for daily high and low).
	CREATE TABLE #mDailyAverages (
		[Date]				datetime,
		[DeviceID]			int,
		[Monitored Element] 		nvarchar(50) COLLATE DATABASE_DEFAULT ,
		[Daily Average Utilization]	float)

	-- Calculate the Daily High and Daily Low Using Daily Averages.
	INSERT INTO #mDailyAverages
	SELECT	dat.[Date],
		dev.[DeviceID],
		util.[Monitored Element],
		AVG(case
  	           when util.[Usage Percentage] &lt; 0 then 0.0
  	           when util.[Usage Percentage] &gt; 100 then 100.0
  	           else util.[Usage Percentage]
  	         end)
	FROM	fact_Utilization AS util INNER JOIN #mDeviceList dev ON util.DeviceID = dev.[DeviceID] INNER JOIN 
			dim_Date AS dat ON dat.[DateID] = util.[DateID]
	WHERE	([UtilizationTypeID] = @DiskUtilizationTypeID) AND
		((util.[DateID] &gt;= @StartDateIndex) AND 
		(util.[DateID] &lt;= @EndDateIndex)) AND
		(util.[Maximum Units] &gt; 0 OR util.[Usage Percentage] is not null)
		-------------------------
		
			and ( 
					--Check if Time SLA Is Included				
					(@IncludeBreakdown = 0)	
					OR -- Check if We are Reporting an All DAY
					(@allDay = 1)			  
					OR  -- Include Time Period
					( util.TimeID between @Start_Hour and @End_Hour )					
					
				)
			and (
				 --Check if time Sla is Included
				(@IncludeBreakdown = 0)
				OR -- check if All Days of Week indicated
				(@WeekType = 'Weekday, Weekend')
				OR -- filter by Weekend or WeekDay
				(@WeekType = dbo.GetWeekEndIndicatorFromDateId(util.DateID))								
			)
		---------------------
	GROUP BY  dat.[Date], dev.[DeviceID], util.[Monitored Element]
--select * from #mDailyAverages
	Declare @CmdString nvarchar(3000)
	Declare @SortBy nvarchar(500)

	If (@IncludeUBR = 0) 
	 begin 
		Select @CmdString = 'Select ' 
		Select @SortBy = 'ORDER BY dev.[DeviceName], a.[Monitored Element], a.[Average Units Consumed] '
	 end
	else
	 begin
		Select @CmdString = 'SELECT top (' + convert (nvarchar(14),@topNDevices)  + ') '
		Select @SortBy = 'ORDER BY a.[Average Units Consumed] desc '
	 end

	-- Bug 1361
	IF (OBJECT_ID('tempdb..#dskUtilizationUnits') IS NOT NULL)
		DROP TABLE #dskUtilizationUnits
		
	CREATE TABLE #dskUtilizationUnits  (
		DeviceID				int,
		[Monitored Element] nvarchar(50),
		MaxDateID				int,
		MaxTimeID				int,
		MaximumUnits				bigint
		)
	CREATE NONCLUSTERED INDEX [IX_tmp_dskUtilizationUnits] ON #dskUtilizationUnits 
	(
		[DeviceID] ASC,
		[Monitored Element] ASC
	)

	INSERT INTO	#dskUtilizationUnits
	SELECT 
			dev.DeviceID,
			util.[Monitored Element],
			MAX(DateID) AS MaxDateID,
			NULL AS MaxTimeID,
			NULL AS MaximumUnits
	FROM 
			warehouse.dbo.fact_Utilization util
				INNER JOIN
			#mDeviceList dev 
					ON util.[DeviceID] = dev.[DeviceID]
	WHERE	
			util.[DateID] &lt;= @EndDateIndex 
			AND util.[UtilizationTypeID]=@DiskUtilizationTypeID
			AND ISNULL(util.[Maximum Units],0) &gt; 0
			AND util.[Monitored Element] IS NOT NULL 
	GROUP BY	
			dev.DeviceID, util.[Monitored Element]
			
	UPDATE #dskUtilizationUnits
	SET 	MaxTimeID = util.MaxTimeID
	FROM 	
		#dskUtilizationUnits a
			INNER JOIN
		( 
			SELECT 
				DateID, 
				u.DeviceID,
				u.[Monitored Element],
				MAX(TimeID) AS MaxTimeID 
			FROM 
				warehouse.dbo.fact_Utilization u
					INNER JOIN
				#mDeviceList AS dev 
						ON u.[DeviceID] = dev.[DeviceID] 
						AND u.[UtilizationTypeID]=@DiskUtilizationTypeID
						AND u.[DateID] &lt;= @EndDateIndex 
			GROUP BY u.DeviceID, u.[Monitored Element], DateID
		) util
				ON  util.DeviceID=a.DeviceID
				AND util.[Monitored Element] COLLATE Latin1_General_CI_AS = a.[Monitored Element] COLLATE Latin1_General_CI_AS
				AND util.DateID=a.MaxDateID
				
	UPDATE #dskUtilizationUnits
	SET 	MaximumUnits = util.[Maximum Units]
	FROM 	
		#dskUtilizationUnits a
			INNER JOIN warehouse.dbo.fact_Utilization util
			ON 
				util.DeviceID=a.DeviceID
				AND util.[UtilizationTypeID]=@DiskUtilizationTypeID
				AND util.[Monitored Element] COLLATE Latin1_General_CI_AS = a.[Monitored Element] COLLATE Latin1_General_CI_AS
				AND util.DateID=a.MaxDateID	
				AND util.TimeID=a.MaxTimeID			
	-- Get the data for the period we need to analyze.
-- Case statement is used incase [Monitored Element] is null in which case the concatenation with [DeviceName] will always return null
	Select  @CmdString =( @CmdString + 'dev.[Device Class],(dev.[DeviceName]
			  + Case 
				when( rtrim(a.[Monitored Element]) is null) then ''''
				else + '' - '' + RTRIM(a.[Monitored Element]) end) AS DeviceName,		
		MIN([Daily Average Utilization]) AS MinUtil,
		MAX([Daily Average Utilization]) AS MaxUtil,
		a.[Average Units Consumed] AS Utilization,
		STDEV([Daily Average Utilization]) AS DevUtil,
		MAX(CONVERT(numeric(15,1),u.MaximumUnits)) AS Capacity
	FROM
	#mUtil AS a JOIN #mDailyAverages AS daily ON a.DeviceID = daily.DeviceID
		AND a.[Monitored Element] = daily.[Monitored Element] 
	INNER JOIN #mDeviceList AS dev ON a.DeviceID = dev.DeviceID
	LEFT OUTER JOIN 
				#dskUtilizationUnits u -- Bug 1361
						ON  a.DeviceID = u.DeviceID	
						AND a.[Monitored Element] COLLATE Latin1_General_CI_AS = u.[Monitored Element] COLLATE Latin1_General_CI_AS
	GROUP BY dev.[Device Class],dev.[DeviceName], a.[Monitored Element], a.[Average Units Consumed]' +
	 @Sortby);

	EXEC (@CmdString)

	IF (OBJECT_ID('tempdb..#dskUtilizationUnits') IS NOT NULL)
		DROP TABLE #dskUtilizationUnits</CommandText>
