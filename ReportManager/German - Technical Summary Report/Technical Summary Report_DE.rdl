    <DataSet Name="ExecutiveSummaryDiskUtilization">
      <Query>
        <DataSourceName>warehouse</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Devices">
            <Value>=Parameters!Devices_Disk.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Start_Hour">
            <Value>=Parameters!Start_Hour.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_Hour">
            <Value>=Parameters!End_Hour.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WeekType">
            <Value>=Parameters!WeekType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeBreakdown">
            <Value>=Parameters!IncludeBreakdown.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeUBR">
            <Value>=Parameters!IncludeUBR.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TopNDevices">
            <Value>=Parameters!NumTopDevices.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- Declare @allDayFlag Indicator
DECLARE @allDay bit

-- Set All Day to true if the End Hour equals Start Hour 
IF (@End_Hour = @Start_Hour)
    SET @allday = 1
ELSE
	 SET @allDay = 0
---------------------&gt;&gt;
	SELECT @StartDate = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
	SELECT @EndDate = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

	DECLARE @StartDateIndex int
	DECLARE @EndDateIndex int

	SET @StartDateIndex = dbo.dateIndex(@StartDate)
	SET @EndDateIndex = dbo.dateIndex(@EndDate)
	

-- Create temp. table to store all devices.
	CREATE  TABLE #mDeviceList (
		[DeviceID]	int,
		[DeviceName]	nvarchar(100) COLLATE DATABASE_DEFAULT ,
		[Deleted]	nvarchar(20) COLLATE DATABASE_DEFAULT )
		
	-- Get all devices from device list and place in the temporary table.
	INSERT INTO #mDeviceList
	SELECT	[DeviceID], [DeviceName], [Status]
	FROM	[dim_Device] 
	WHERE [DeviceID] IN (@Devices)

	DECLARE @CmdString nvarchar(3000)
	IF (@IncludeUBR = 0) 
	 BEGIN 
		Select @CmdString = 'Select ' 
	 END
	ELSE
	 BEGIN
		SELECT @CmdString = 'SELECT top (' + CONVERT (nvarchar(14),@topNDevices)  + ') ' 
	 END

	-- Create a table to store the overall device averages.
	CREATE TABLE #mUtil(
		[DeviceID] int,
		[DeviceName] nvarchar(100) COLLATE DATABASE_DEFAULT ,
		[Monitored Element] nvarchar (200) COLLATE DATABASE_DEFAULT,
		[Average Units Consumed] float) 
	
	-- Calculate the overall average for each device.
	SELECT  @CmdString =( 'with OverallAvg as(
	SELECT	util.DeviceID, dev.[DeviceName],util.[Monitored Element],
--			  + Case 
--				when( rtrim([Monitored Element]) is null) then ''''
--				else + '' - '' + RTRIM([Monitored Element]) end) AS DeviceName,
	AVG(case
  	           when [Usage Percentage] &lt; 0 then 0.0
  	           when [Usage Percentage] &gt; 100 then 100.0
  	           else [Usage Percentage]
  	         end) as [Average Units Consumed]
	FROM	[fact_Utilization]  AS util INNER JOIN #mDeviceList dev ON util.DeviceID = dev.[DeviceID]
	WHERE	(util.DateID &gt;= ' + CONVERT (nvarchar(14),@StartDateIndex)  + ') AND 
	        (util.DateID &lt;= ' + CONVERT (nvarchar(14),@EndDateIndex)  + ') AND
			(util.UtilizationTypeID = ' + CONVERT (varchar(10),2)  + ') AND
			(util.[Maximum Units] &gt; 0 OR util.[Usage Percentage] is not null)
			
		-------------------------
		
		and ( 
					--Check if Time SLA Is Included				
					('+ ''''+CONVERT (varchar(10),@IncludeBreakdown)+'''' +' = 0)	
					OR -- Check if We are Reporting an All DAY
					('+ ''''+CONVERT (varchar(10),@allDay)+'''' +'= 1)			  
					OR  -- Include Time Period
					( util.TimeID between'+''''+CONVERT (varchar(10), @Start_Hour)+'''' 
								 +'and '+ ''''+CONVERT (varchar(10),@End_Hour)+'''' +')					
					
				)
			and (
				 --Check if time Sla is Included
				('+ ''''+CONVERT (varchar(10),@IncludeBreakdown)+''''  +' = 0)
				OR -- check if All Days of Week indicated
				('+ ''''+@WeekType +'''= ''Weekday, Weekend'')
				OR -- filter by Weekend or WeekDay
				('+ ''''+@WeekType +'''= dbo.GetWeekEndIndicatorFromDateId(util.DateID))									
			)
		---------------------
	GROUP BY util.[DeviceID], dev.[DeviceName], util.[Monitored Element]
	)INSERT INTO #mUtil ' +
						   @CmdString + '[DeviceID], [DeviceName],[Monitored Element], [Average Units Consumed]  
							from OverallAvg
							Order by [Average Units Consumed] desc'
						);
	EXEC (@CmdString)

	-- Get the data for the period we need to analyze
	SELECT  @CmdString =( 'Select dev.[DeviceName],util.[Monitored Element],
		   [dim_Date].[Date],
		   AVG(case
  	           when util.[Usage Percentage] &lt; 0 then 0.0
  	           when util.[Usage Percentage] &gt; 100 then 100.0
  	           else util.[Usage Percentage]
  	         end)
  	     AS Utilization
		 ,dev.[Average Units Consumed] as OverallAvg
	FROM       [fact_Utilization] AS util JOIN #mUtil Dev ON util.DeviceID = Dev.[DeviceID]
				 and util.[Monitored Element] = Dev.[Monitored Element]
				      INNER JOIN dim_Date ON util.DateID = dim_Date.DateID
	WHERE      (dim_Date.[Date] &gt;='+ ''''+CONVERT (nvarchar(14),@StartDate, 101)  +'''' +') AND 
	           (dim_Date.[Date] &lt;='+ ''''+CONVERT (nvarchar(14), @EndDate, 101)  +'''' +') AND
		   (util.UtilizationTypeID = 2) AND
		   (util.[Maximum Units] &gt; 0 OR util.[Usage Percentage] is not null)
			and ( 
					--Check if Time SLA Is Included				
					('+ ''''+CONVERT (varchar(10),@IncludeBreakdown)+'''' +' = 0)	
					OR -- Check if We are Reporting an All DAY
					('+ ''''+CONVERT (varchar(10),@allDay)+'''' +'= 1)			  
					OR  -- Include Time Period
					( util.TimeID between'+''''+CONVERT (varchar(10), @Start_Hour)+'''' 
								 +'and '+ ''''+CONVERT (varchar(10),@End_Hour)+'''' +')					
					
				)
			and (
				 --Check if time Sla is Included
				('+ ''''+CONVERT (varchar(10),@IncludeBreakdown)+''''  +' = 0)
				OR -- check if All Days of Week indicated
				('+ ''''+@WeekType +'''= ''Weekday, Weekend'')
				OR -- filter by Weekend or WeekDay
				('+ ''''+@WeekType +'''= dim_Date.[Weekend Indicator])								
			)
	GROUP BY Dev.[DeviceName],util.[Monitored Element], [dim_Date].[Date],dev.[Average Units Consumed] 
	 ORDER BY Dev.[DeviceName],util.[Monitored Element], [dim_Date].[Date]' );

EXEC (@CmdString)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DeviceName">
          <DataField>DeviceName</DataField>
        </Field>
        <Field Name="MonitioredElement">
          <DataField>Monitored Element</DataField>
        </Field>
        <Field Name="Date">
          <DataField>Date</DataField>
        </Field>
        <Field Name="Utilization">
          <DataField>Utilization</DataField>
        </Field>
      </Fields>
