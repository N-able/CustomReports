	INSERT INTO #mUtil
	SELECT	[fact_Utilization].[DeviceID], 
			AVG(case
  	           when [Usage Percentage] &lt; 0 then 0.0
  	           when [Usage Percentage] &gt; 100 then 100.0
  	           else [Usage Percentage]
  	         end),
  	         MAX(ISNULL([Maximum Units],0)) AS MaximumUnits
	FROM	[fact_Utilization] INNER JOIN #mDeviceList dev ON fact_Utilization.DeviceID = dev.[DeviceID]
			inner join dim_Date AS dat ON dat.[DateID] = [fact_Utilization].[DateID]

