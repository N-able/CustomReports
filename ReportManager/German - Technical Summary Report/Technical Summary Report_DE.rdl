	SELECT @StartDateIndex = dbo.dateIndex(@StartDate)
	SELECT @EndDateIndex   = dbo.dateIndex(@EndDate)

-- Declare @allDayFlag Indicator
DECLARE @allDay bit

-- Set All Day to true if the End Hour equals Start Hour 
IF (@End_Hour = @Start_Hour)
    SET @allday = 1
ELSE
	 SET @allDay = 0
