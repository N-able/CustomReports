        </Field>
      </Fields>
      <Query>
        <DataSourceName>Warehouse</DataSourceName>
        <CommandText>DECLARE @odsname varchar(500)
DECLARE @sql varchar(8000)

DECLARE @CustomerIDint int
SELECT @CustomerIDint = [N-central CustomerID] from warehouse.dbo.dim_Customer where [Customer Type] = 'Service Organization' and CustomerID = @SOCustomerID


SELECT @odsname = [Operational Data Store] FROM config.dbo.DataSource where Status='Active' order by DataSourceID DESC

SET @sql = 'USE ' + @odsname + ' 
SELECT distinct 
		c.customername as [Customer Name]
		,d.deviceclass as [Device Class]
		,d.longname as [Device Name]
		,d.lastsample as [Last Discovery Date]
		,a.version as [Agent Version]
		,a.lastlogin as [Last Agent Checkin]
		,DATEDIFF(day, a.lastlogin,GETDATE()) as [Days Since Last Login]
		,case when ca.version is null then ''N\A'' else esp.name end as [Endpoint Profile]
		,ca.version as [Endpoint Version]
		,cp.name as [Patch Profile]
FROM device d
	INNER JOIN customer c
		ON d.customerid=c.customerid
	INNER JOIN appliance a 
		ON d.deviceid=a.deviceid
	LEFT JOIN endpointsecurityprofiledevice espd
		ON d.deviceid = espd.deviceid
	LEFT JOIN endpointsecurityprofile esp
		ON espd.profile_uuid = esp.profile_uuid
	LEFT JOIN configprofiledevice cpd
		ON d.deviceid = cpd.deviceid
	LEFT JOIN configprofile cp
		ON cp.profile_uuid= cpd.profileid
	LEFT JOIN cim_application ca
		ON d.deviceid = ca.deviceid
		AND ca.displayname = ''Endpoint Security Manager''
WHERE
	d.deleted=0 AND
	d.ismanagedasset = 1 AND
	d.system = 0 AND
	a.lastlogin &lt; DATEADD(day,-' + @StaleThreshold + ',GETDATE())
	AND (a.customerid IN (' + CONVERT(nvarchar(30),@CustomerIDint) + ') OR a.customerid IN (select customerid from customer where parentid in ('+CONVERT(nvarchar(30),@CustomerIDint)+')))
ORDER BY c.customername,d.deviceclass, d.longname'

EXEC (@sql)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StaleThreshold">
            <Value>=Parameters!StaleThreshold.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SOCustomerID">
            <Value>=Parameters!SOCustomerID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
