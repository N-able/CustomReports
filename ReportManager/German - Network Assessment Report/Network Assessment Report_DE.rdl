    <DataSet Name="FailedPatches">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
   d.[Device Class]
  ,d.[DeviceName]
  ,p.[Patch Name]
  ,p.[Patch Description]
  ,p.Product
  ,p.[Patch Category]
  ,p.[Installation Status]
  ,DATEDIFF(dd,p.[Release Date],@EndDate) as 'Age'

FROM
  fact_cim_MissingPatch p
	INNER JOIN dim_Customer c
		ON p.customerID = c.CustomerID
	INNER JOIN dim_Device d
		ON p.DeviceID = d.DeviceID
WHERE
	d.CustomerID = @CustomerID 
	AND p.[Installation Status] = 'Failed'
	AND (CAST(FLOOR(CAST(p.ScanTime AS FLOAT)) AS DATETIME)) &gt;= @StartDate 
	AND (CAST(FLOOR(CAST(p.ScanTime AS FLOAT)) AS DATETIME)) &lt;= @EndDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
