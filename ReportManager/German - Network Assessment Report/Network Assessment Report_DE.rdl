    <DataSet Name="IntervalBreakdown">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartHour">
            <Value>=Parameters!StartHour.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndHour">
            <Value>=Parameters!EndHour.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WeekType">
            <Value>=Parameters!WeekType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeBreakdown">
            <Value>=Parameters!IncludeBreakdown.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
Parameters:
	@Start_Hour smallint,		-- FK to dim_Time: minutes
	@End_Hour   smallint,		-- FK to dim_Time: minutes
	@WeekType  nvarchar(1000),	-- Must be 'Weekday', 'Weekend' or 'Weekday, Weekend'
	@IncludeBreakdown bit
*/	

DECLARE @Title nvarchar(50)
DECLARE @Start_Hour int
DECLARE @End_Hour int	

	SET @Start_Hour = @StartHour*60+1
	
	IF @EndHour &lt; 24
		SET @End_Hour = @EndHour*60+1
	ELSE
		SET @End_Hour = @EndHour*60	
	
	IF ((@Start_Hour = @End_Hour) OR (@Start_Hour = 1 AND @End_Hour = 1440) OR (@IncludeBreakdown = 0))
	  SELECT @Title = 'All Day'
	ELSE
	  SELECT @Title =
		(SELECT RIGHT(CAST((100+Hour) AS varchar(3)),2)  + ':' +
			RIGHT(CAST((100+Minute) AS varchar(3)),2)                 
		 FROM dim_time 
		 WHERE timeid = @Start_Hour) + ' - ' + 
		(SELECT RIGHT(CAST((100+Hour) AS varchar(3)),2)  + ':' +
			RIGHT(CAST((100+Minute) AS varchar(3)),2)                 
		 FROM dim_time 
		 WHERE timeid = @End_Hour)

	IF (@IncludeBreakdown = 1)
	BEGIN
		IF (@WeekType = 'Weekday')
			SET @Title = @Title + ' Weekdays'
		ELSE IF (@WeekType = 'Weekend')
			SET @Title = @Title + ' Weekends'
		ELSE	
			SET @Title = @Title + ' Weekdays and Weekends'
	END
	ELSE 
		SET @Title = @Title + ' Weekdays and Weekends'

	SELECT @Title AS Title</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPT_CapacityPlanning_Planning">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DeviceClasses">
            <Value>=Parameters!Device_Class_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UtilizationLimit">
            <Value>=Parameters!UtilizationLimit.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeNearLimit">
            <Value>=Parameters!IncludeNearLimit.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProximityToLimit">
            <Value>=Parameters!ProximityToLimit.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DeviceIDList">
            <Value>null</Value>
          </QueryParameter>
          <QueryParameter Name="@Silent">
            <Value>0</Value>
          </QueryParameter>
          <QueryParameter Name="@StartHour">
            <Value>=Parameters!StartHour.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndHour">
            <Value>=Parameters!EndHour.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeBreakdown">
            <Value>=Parameters!IncludeBreakdown.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WeekType">
            <Value>=Parameters!WeekType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Devices">
            <Value>=Parameters!Device_ID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Rules">
            <Value>=Parameters!Rule_ID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET NOCOUNT ON
SET ANSI_WARNINGS OFF
SET @DeviceIDList = ''

	-- Declare @allDayFlag Indicator
	DECLARE @allDay bit
	DECLARE @Start_Hour int
	DECLARE @End_Hour int
	
	-- SET All Day to true IF the END Hour equals Start Hour 
	IF (@EndHour = 24 AND @StartHour = 0)
		BEGIN
			SET @allday = 1	
		END
	ELSE
		BEGIN
			SET @allDay = 0
			SET @Start_Hour = @StartHour * 60 + 1
			IF @EndHour &lt; 24
				SET @End_Hour = @EndHour * 60 + 1
			ELSE
				SET @End_Hour = @EndHour * 60
		END

	-- Get the start and end dates for the report
	SET @StartDate = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
	SET @EndDate = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

	-- Create the date variables for reuse in the database filter.
	DECLARE @StartDateIndex	nvarchar(10)
	DECLARE @EndDateIndex	nvarchar(10)
	SET  @StartDateIndex = [dbo].dateIndex(@StartDate)
	SET  @EndDateIndex = [dbo].dateIndex(@EndDate)

	-- Create temp. table to store device class, id and status information.
	IF (OBJECT_ID('tempdb..#mDeviceList') IS NOT NULL)
		DROP TABLE #mDeviceList
		
	CREATE TABLE #mDeviceList  (
		[Device Class]	nvarchar(50),
		[Device Name]	nvarchar(100),
		[DeviceID]	int,
		[URI]		nvarchar(200),
		[Status]	nvarchar(20))
		
	CREATE NONCLUSTERED INDEX [ix_mDeviceList_DeviceID] ON #mDeviceList 
	(
		[DeviceID] ASC
	)
		
	-- Get all devices from selected device class(es) and place in the temporary table.
	INSERT INTO #mDeviceList
	SELECT	[Device Class], [DeviceName], [DeviceID], [URI], d.[Status]
	FROM	[dim_Device] AS d 
				INNER JOIN
			[dim_Customer] AS c ON 
							c.[N-central CustomerID] = d.[N-central CustomerID] 
						AND	c.[DataSourceID] = d.[DataSourceID] 
						AND c.CustomerID = @CustomerID
	WHERE	(d.[Custom DeviceClassID] IN (@DeviceClasses) 
			OR
			d.DeviceID IN (@Devices)
			OR
			d.DeviceID IN (SELECT DeviceID 
					FROM dbo.[fact_Device Folder] 
					WHERE CustomerID=@CustomerID AND FolderID IN (@Rules)
					)
			)
			-- Include only those devices that are managed and not ignored Managed Device
			AND (d.[Is Managed Device Indicator] = 'Managed Device' AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device')


	-- Get the show deleted devices value from the config database -&gt; config table.
	DECLARE @ShowDeleted nvarchar(200)
	SET @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')

	-- If the user has specified TO SHOW deleted devices, filter as normal.  Otherwise remove deleted devices.
	IF (@ShowDeleted = 'False')
		DELETE FROM #mDeviceList WHERE [Status] != 'Active'

	-- Temp. table for the utilization data.
	IF (OBJECT_ID('tempdb..#mUtilCounts') IS NOT NULL)
		DROP TABLE #mUtilCounts
		
	CREATE TABLE #mUtilCounts (
		[DeviceID]			int,				
		[Monitored Element]		nvarchar(50),
		[CPU Utilization]		float,
		[Disk Utilization]		float,
		[Physical Memory Utilization]	float,
		[Virtual Memory Utilization]	float) 
		
	CREATE NONCLUSTERED INDEX [ix_mUtilCounts_DeviceID] ON #mUtilCounts
	(
		[DeviceID] ASC
	)
	-- Extract the UtilizationTypeID, Average, Counts, and Caculations for Std. Deviations.
	INSERT INTO #mUtilCounts
	SELECT	dev.[DeviceID],
		NULL,
		AVG(CASE WHEN ((util.[Maximum Units] &gt; 0 OR util.[Usage Percentage] is not null) AND util.[UtilizationTypeID] = 1) THEN (case 
          when util.[Usage Percentage] &lt; 0 then 0.0 
          when util.[Usage Percentage] &gt; 100 then 100.0 
          else util.[Usage Percentage] 
        end) ELSE NULL END),
		NULL,
		AVG(CASE WHEN ((util.[Maximum Units] &gt; 0 OR util.[Usage Percentage] is not null) AND util.[UtilizationTypeID] = 4) THEN (case 
          when util.[Usage Percentage] &lt; 0 then 0.0 
          when util.[Usage Percentage] &gt; 100 then 100.0 
          else util.[Usage Percentage]
        end) ELSE NULL END),
		AVG(CASE WHEN ((util.[Maximum Units] &gt; 0 OR util.[Usage Percentage] is not null) AND util.[UtilizationTypeID] = 3) THEN (case 
          when util.[Usage Percentage] &lt; 0 then 0.0 
          when util.[Usage Percentage] &gt; 100 then 100.0 
          else util.[Usage Percentage] 
        end) ELSE NULL END)
	FROM	fact_Utilization AS util INNER JOIN
			#mDeviceList AS dev ON util.[DeviceID] = dev.[DeviceID] INNER JOIN
			dim_Date AS dat ON dat.[DateID] = util.[DateID]
	WHERE util.[DateID] &gt;= @StartDateIndex AND util.[DateID] &lt;= @EndDateIndex AND (util.[Maximum Units] &gt; 0	 OR util.[Usage Percentage] is not null)
		
			and ( 
					--Check if Time SLA Is Included				
					(@IncludeBreakdown = 0)	
					OR -- Check if We are Reporting an All DAY
					(@allDay = 1)			  
					OR  -- Include Time Period
					( util.TimeID BETWEEN @Start_Hour AND @End_Hour )					
					
				)
			and (
				 --Check if time Sla is Included
				(@IncludeBreakdown = 0)
				OR -- check if All Days of Week indicated
				(@WeekType = 'Weekday, Weekend')
				OR -- filter by Weekend or WeekDay
				(@WeekType = dat.[Weekend Indicator])								
			)
		-------------------------
	GROUP BY dev.[Device Class], dev.[DeviceID]

	-- Create the temporary table to store the output.
	DECLARE @mOutput TABLE (
		DeviceID int,
		[CPU Utilization] float,
		[Disk Utilization] nvarchar(1000),
		[Physical Memory Utilization] float,
		[Virtual Memory Utilization] float,
		[Disk Utilization Status] int		-- 0 = Disk util. below limit, 1 = Disk Util. above limit, 2 = Disk Util. above proximity limit
	) 
	-- Create a temp. table to store the average utilizations for each device and monitored element on each device.
	IF (OBJECT_ID('tempdb..#tmp_DiskAverages') IS NOT NULL)
		DROP TABLE #tmp_DiskAverages
		
	CREATE TABLE #tmp_DiskAverages (
		[DeviceID]		int,
		[Monitored Element]	nvarchar(50),
		[Disk Utilization]	float)
		
	CREATE NONCLUSTERED INDEX [ix_tmp_DiskAverages_DeviceID] ON #tmp_DiskAverages 
	(
		[DeviceID] ASC
	)
		
	-- Calculate the average utilizations for each monitored element on each device from the #mUtilCounts table.
	INSERT INTO #tmp_DiskAverages
	SELECT	dev.[DeviceID],				
		util.[Monitored Element],
		AVG(CASE WHEN ((util.[Maximum Units] &gt; 0	OR util.[Usage Percentage] is not null) AND util.[UtilizationTypeID] = 2) THEN (case 
          when util.[Usage Percentage] &lt; 0 then 0.0 
          when util.[Usage Percentage] &gt; 100 then 100.0 
          else util.[Usage Percentage] 
        end) ELSE NULL END)
	FROM	dbo.fact_Utilization AS util INNER JOIN
			#mDeviceList AS dev ON util.[DeviceID] = dev.[DeviceID] INNER JOIN
			dim_Date AS dat ON dat.[DateID] = util.[DateID]
	WHERE util.[DateID] &gt;= @StartDateIndex AND util.[DateID] &lt;= @EndDateIndex AND (util.[Maximum Units] &gt; 0	OR util.[Usage Percentage] is not null)
		
			and ( 
					--Check if Time SLA Is Included				
					(@IncludeBreakdown = 0)	
					OR -- Check if We are Reporting an All DAY
					(@allDay = 1 /*and (util.TimeID between @Start_Hour and @End_Hour)*/)			  
					OR  -- Include Time Period
					( util.TimeID BETWEEN @Start_Hour AND @End_Hour )					
					
				)
			and (
				 --Check if time Sla is Included
				(@IncludeBreakdown = 0)
				OR -- check if All Days of Week indicated
				(@WeekType = 'Weekday, Weekend')
				OR -- filter by Weekend or WeekDay
				(@WeekType = dat.[Weekend Indicator])								
			)

	GROUP BY dev.[DeviceID], util.[Monitored Element]
	INSERT INTO @mOutput
	SELECT  util.DeviceID,
			AVG(util.[CPU Utilization]),
		     Case -- used incase [Monitored Element] is null in which case the concatenation with [Disk Utilization] will always return null
				when( rtrim(davg.[Monitored Element]) is null) then ''
				else RTRIM(davg.[Monitored Element]) end +'  ' +
			  CAST(Round(davg.[Disk Utilization], 2) AS nvarchar(10)),
			AVG(util.[Physical Memory Utilization]),
			AVG(util.[Virtual Memory Utilization]),
		    MAX(CASE 
				WHEN (ROUND(davg.[Disk Utilization],2) &gt;=  @UtilizationLimit) THEN 1   
				WHEN (ROUND(davg.[Disk Utilization],2) &gt;= (@UtilizationLimit-@ProximityToLimit)) THEN 2
				ELSE 0 END)
	FROM #mUtilCounts AS util INNER JOIN #tmp_DiskAverages AS davg ON util.[DeviceID] = davg.[DeviceID]
	GROUP BY util.DeviceID,
		Case 
				when( rtrim(davg.[Monitored Element]) is null) then ''
				else RTRIM(davg.[Monitored Element]) end 
		 + '  ' + CAST(Round(davg.[Disk Utilization], 2) AS nvarchar(10))
	ORDER BY util.DeviceID
	-- Determine the Disk Utilizations and concatenate into a string.
	DECLARE @mDeviceID int, @mDisk nvarchar(1000), @mTmpDisk nvarchar(1000),@mTmpStatus  int
	SELECT TOP 1 @mDeviceID = [DeviceID], @mDisk = [Disk Utilization], @mTmpStatus = [Disk Utilization Status] 
	FROM @mOutput ORDER BY [Disk Utilization Status] ASC

	WHILE (@mTmpStatus &lt; 0)
	BEGIN
	 	-- Create Disk Utilization String and status
		SET @mTmpDisk = '';

		SELECT	@mTmpStatus = CASE 
				WHEN (ROUND([Disk Utilization],2) &gt;=  @UtilizationLimit) THEN 1   
				WHEN (ROUND([Disk Utilization],2) &gt;= (@UtilizationLimit-@ProximityToLimit)) THEN 2
				WHEN (@mTmpStatus &gt; 0) THEN @mTmpStatus
				ELSE 0 END,
			@mTmpDisk = @mTmpDisk + Case 
				when( rtrim([Monitored Element]) is null) then ''
				else RTRIM([Monitored Element]) end  + '  ' + CAST(Round([Disk Utilization], 2) AS nvarchar(10)) + '\n'
		FROM	#tmp_DiskAverages 
		WHERE	[Disk Utilization] IS NOT NULL AND [DeviceID] = @mDeviceID
		ORDER BY [Monitored Element] ASC

		UPDATE @mOutput SET [Disk Utilization] = @mTmpDisk, [Disk Utilization Status] = (CASE WHEN @mTmpStatus = -1 THEN 0 ELSE @mTmpStatus END) WHERE DeviceID = @mDeviceID

		-- Fetch Next DeviceID for Loop
		SELECT TOP 1 @mDeviceID = [DeviceID], @mDisk = [Disk Utilization], @mTmpStatus = [Disk Utilization Status] 
		FROM @mOutput ORDER BY [Disk Utilization Status] ASC

	END

	DELETE FROM @mOutput
	WHERE DeviceID IN 
	(	
		SELECT DeviceID  
		FROM @mOutput
		GROUP BY DeviceID
		HAVING COUNT([Disk Utilization])&gt;0
	)	
	AND [Disk Utilization] IS NULL
	
	-- Return the output table joined with device info.
	SELECT	DISTINCT d.[Device Class], d.[Device Name], d.[URI], o.[CPU Utilization], o.[Disk Utilization], o.[Physical Memory Utilization], o.[Virtual Memory Utilization], o.[Disk Utilization Status]
	FROM	@mOutput AS o INNER JOIN
		#mDeviceList AS d ON o.[DeviceID] = d.[DeviceID]
	ORDER BY [Device Name]
	-- Return a comma delimited list of device names above the threshold.
	
	IF (OBJECT_ID('tempdb..#mDeviceList') IS NOT NULL)
		DROP TABLE #mDeviceList	
		
	IF (OBJECT_ID('tempdb..#mUtilCounts') IS NOT NULL)
		DROP TABLE #mUtilCounts
		
	IF (OBJECT_ID('tempdb..#tmp_DiskAverages') IS NOT NULL)
		DROP TABLE #tmp_DiskAverages
		
RETURN</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Device_Class">
          <DataField>Device Class</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Device_Name">
          <DataField>Device Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="URI">
          <DataField>URI</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CPU_Utilization">
          <DataField>CPU Utilization</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Disk_Utilization">
          <DataField>Disk Utilization</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Physical_Memory_Utilization">
          <DataField>Physical Memory Utilization</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Virtual_Memory_Utilization">
          <DataField>Virtual Memory Utilization</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Disk_Utilization_Status">
          <DataField>Disk Utilization Status</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPT_CapacityPlanning_Summary">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Customer_ID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DeviceClassID">
            <Value>=Parameters!Device_Class_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @StartDateIndex	VARCHAR(10)
SET  @StartDateIndex = warehouse.[dbo].dateIndex(@StartDate)

DECLARE @EndDateIndex	VARCHAR(10)
SET  @EndDateIndex = warehouse.[dbo].dateIndex(@EndDate)

DECLARE @ShowDeleted VARCHAR(20)
SELECT @ShowDeleted = [VALUE] FROM config.dbo.Config WHERE [KEY]='ShowDeletedDevices'

Select deviceid, Right(Max(cpuid), 1) + 1 as NumCPUs, max(name) as Name
into #CPUDetails
from warehouse.dbo.fact_cim_processor
where customerid = @Customer_ID
GROUP BY deviceid

SELECT u.[CustomerID]
	,u.[DeviceID]
	,[UtilizationTypeID]
	,CASE WHEN UtilizationTypeID = 2 THEN [Monitored Element] ELSE NULL END [Monitored Element]
	,AVG([USAGE Percentage]) AS [Average]
into #OverallAverage
FROM [warehouse].[dbo].[fact_Utilization] AS u
	INNER JOIN warehouse.dbo.dim_Device AS d
		ON d.DeviceID = u.DeviceID
WHERE  u.CustomerID = @Customer_ID
	AND ([Maximum Units] &gt; 0
		OR [USAGE Percentage] IS NOT NULL)
	AND DateID BETWEEN @StartDateIndex AND @EndDateIndex
	AND d.[Custom DeviceClassID] IN (@DeviceClassID)
	AND d.[Is Managed Device INDICATOR] = 'Managed Device'
	AND d.[Is Ignored Device INDICATOR] = 'Not an Ignored Device'
	AND ((d.Status = 'Active' AND @ShowDeleted = 'False')
		OR @ShowDeleted = 'True')
GROUP BY u.[CustomerID]
	,u.[DeviceID]
	,[UtilizationTypeID]
	,CASE WHEN UtilizationTypeID = 2 THEN [Monitored Element] ELSE NULL END


SELECT u.[CustomerID]
	,[DateID]
	,warehouse.dbo.GetDateTimeFromDateTimeIds(Dateid, 1) AS 'Date'
	,u.[DeviceID]
	,d.DeviceClassID
	,d.DeviceName
	,d.URI
	,CASE WHEN u.UtilizationTypeID = 1 THEN 'CPU  (' + cast(min(p.NumCPUs) as varchar(1)) + 'x ' + MIN(p.name) + ')' 
		ELSE cast(cast(max(u.[Maximum Units]) as numeric(15,1)) as varchar(17)) END AS 'Details'
	,CASE WHEN u.UtilizationTypeID = 1 THEN 'Prozessor Auslastung'
		WHEN u.UtilizationTypeID = 2 THEN MIN(RTRIM(u.[Monitored Element])) --Drive Letter
		WHEN u.UtilizationTypeID = 3 THEN 'Virtueller Speicher Auslastung'
		ELSE 'Physischer Speicher Auslastung' END AS [Monitored Element]
	,AVG([USAGE Percentage]) AS [Average]
	,MAX(o.Average) as 'OverallAverage'
	,SUM(CASE WHEN [USAGE Percentage] &gt; 80 THEN 1 ELSE 0 END) AS [TIME OVER LIMIT]
	,Case when u.UtilizationTypeID = 1 then 1
		when u.UtilizationTypeID = 2 then 2
		when u.UtilizationTypeID = 4 then 3
		else 4 end as 'SortOrder'
FROM [warehouse].[dbo].[fact_Utilization] AS u
	INNER JOIN warehouse.dbo.dim_Device AS d
		ON d.DeviceID = u.DeviceID
	INNER JOIN #CPUDetails AS p
		ON p.deviceid = d.DeviceID
	INNER JOIN #OverallAverage as o
		on o.CustomerID = u.CustomerID
		and o.DeviceID = u.DeviceID
		--and o.[Monitored Element] = u.[Monitored Element]
		and ((o.UtilizationTypeID = u.UtilizationTypeID and o.[Monitored Element] = u.[Monitored Element])
			OR (o.UtilizationTypeID = u.UtilizationTypeID and o.[Monitored Element] is null))
WHERE  u.CustomerID = @Customer_ID
	AND ([Maximum Units] &gt; 0
		OR [USAGE Percentage] IS NOT NULL)
	AND DateID BETWEEN @StartDateIndex AND @EndDateIndex
	AND d.[Custom DeviceClassID] IN (@DeviceClassID)
	AND d.[Is Managed Device INDICATOR] = 'Managed Device'
	AND d.[Is Ignored Device INDICATOR] = 'Not an Ignored Device'
	AND d.[IsProbe Device Indicator] = 'Not a Probe Device'
	AND ((d.Status = 'Active' AND @ShowDeleted = 'False')
		OR @ShowDeleted = 'True')
GROUP BY u.[CustomerID]
	,[DateID]
	,u.[DeviceID]
	,d.DeviceClassID
    ,d.DeviceName
    ,d.URI
	,u.[UtilizationTypeID]
	,CASE WHEN u.UtilizationTypeID = 2 THEN u.[Monitored Element] ELSE NULL END

drop table #CPUDetails
drop table #OverallAverage</CommandText>
