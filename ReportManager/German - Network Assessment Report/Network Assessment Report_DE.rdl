    <DataSet Name="ManagedDevices">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @EndDateID int
SET @EndDateID = (SELECT DateID FROM dim_Date WHERE [Date] = @EndDate)
  
DECLARE @DatasourceID as tinyint
SELECT @DatasourceID=DatasourceID FROM dim_Customer WHERE customerid=@CustomerID  

SELECT DISTINCT DeviceID, CustomerID, DatasourceID INTO #ManagedDevices
FROM [Fact_Device Management History] 
WHERE 
		([Date Device Management Started On] &lt;= @EndDateID) 
	AND (
			   @EndDateID &lt;= [Date Device Management Ended On] 
			OR [Date Device Management Ended On] IS NULL
		)
	AND CustomerID = @CustomerID
	AND DeviceID IN 
		(	
			SELECT DeviceID 
			FROM dim_Device 
			WHERE 
					(CAST(FLOOR(CAST(CreatedOn AS FLOAT)) AS DATETIME)) &lt;= @EndDate 
				AND (
						   @EndDate &lt;= (CAST(FLOOR(CAST(DeletedOn AS FLOAT)) AS DATETIME)) 
						OR DeletedOn IS NULL
					)
				AND [is managed device indicator] = 'Managed Device' 
				AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
				AND [IsProbe Device Indicator] = 'Not a Probe Device' 
				AND CustomerID = @CustomerID
				AND DataSourceID=@DatasourceID 
		)
	AND DeviceID in 
		(
			SELECT Distinct DeviceID
			FROM 
					[fact_Device Services] fs 
				inner join 
					dim_Service s on fs.datasourceid=s.DataSourceID and fs.serviceid=s.ServiceID
			WHERE 
					fs.customerid=@CustomerID 
				AND fs.DataSourceID=@DatasourceID
				AND s.ServiceName='Patch Status'
		)				
	
SELECT Distinct d.[Device Class], d.DeviceID, d.DeviceName 
	
			,ISNULL((SELECT
			  TOP 1
			  p.[Patch Name]
			FROM
					[fact_cim_MissingPatch] p
				INNER JOIN dim_Customer c
					ON p.customerID = c.CustomerID
				INNER JOIN dim_Device d
					ON p.DeviceID = d.DeviceID
			WHERE
				d.CustomerID = @CustomerID
				AND d.DeviceID=m.DeviceID
				AND p.[Installation Status] = 'Not Installed'
				AND (CAST(FLOOR(CAST(p.ScanTime AS FLOAT)) AS DATETIME)) &gt;= @StartDate 
				AND (CAST(FLOOR(CAST(p.ScanTime AS FLOAT)) AS DATETIME)) &lt;= @EndDate
			ORDER BY DATEDIFF(dd,p.[Release Date],@EndDate) DESC) ,'')

		 [Oldest Missing Patch]
		 Into #ManagedDevices2
FROM 
		#ManagedDevices m
	INNER JOIN 
		dim_Device d ON 
				m.CustomerID = d.CustomerID
			AND m.DataSourceID = d.DataSourceID
			AND m.DeviceID = d.DeviceID			
	LEFT JOIN
		fact_cim_Patch p ON d.DeviceID = p.DeviceID		
WHERE
		d.CustomerID = @CustomerID 
		

SELECT
   d.DeviceID
  ,d.[Device Class]
  ,d.[DeviceName]
  ,p.[Patch Name]
  ,p.[Patch Description]
  ,p.Product
  ,p.[Patch Category]
  ,DATEDIFF(dd,p.[Release Date],@EndDate) as 'Age'
  ,(CASE p.[Patch Category]
	WHEN 'Security Updates' THEN 1
	WHEN 'Critical Updates' THEN 2
	WHEN 'Service Packs' THEN 3
	WHEN 'Update Rollups' THEN 4
	WHEN 'Feature Packs' THEN 5
	WHEN 'Updates' THEN 6
	WHEN 'Drivers' THEN 7
	WHEN 'Definition Updates' THEN 8
	WHEN 'Tools' THEN 9
	WHEN 'Office 2007' THEN 10
	ELSE 11
  END) PatchPriority
INTO #MissingPatch
FROM
  [fact_cim_MissingPatch] p
	INNER JOIN dim_Customer c
		ON p.customerID = c.CustomerID
	INNER JOIN dim_Device d
		ON p.DeviceID = d.DeviceID
WHERE
	d.CustomerID = @CustomerID
	AND p.[Installation Status] = 'Not Installed'
	AND (CAST(FLOOR(CAST(p.ScanTime AS FLOAT)) AS DATETIME)) &gt;= @StartDate 
	AND (CAST(FLOOR(CAST(p.ScanTime AS FLOAT)) AS DATETIME)) &lt;= @EndDate


--select distinct [Patch Category], 0 PatchPriority into #pc from [fact_cim_MissingPatch]
--update #PC set PatchPriority=(CASE [Patch Category]
--	WHEN 'Security Updates' THEN 1
--	WHEN 'Critical Updates' THEN 2
--	WHEN 'Service Packs' THEN 3
--	WHEN 'Update Rollups' THEN 4
--	WHEN 'Feature Packs' THEN 5
--	WHEN 'Updates' THEN 6
--	WHEN 'Drivers' THEN 7
--	WHEN 'Definition Updates' THEN 8
--	WHEN 'Tools' THEN 9
--	WHEN 'Office 2007' THEN 10
--	ELSE 11
--  END) 

CREATE TABLE #pc (
	[Patch Category] VARCHAR(200)
	,NewCategory VARCHAR(200)
	,PatchPriority INT
)


INSERT INTO #pc 
	SELECT 'Security Updates', 'Sicherheit', 1
UNION	SELECT 'Critical Updates', 'Kritisch', 2
UNION	SELECT 'Definition Updates', 'Definition', 3
UNION	SELECT 'Update Rollups', 'Rollup', 4
UNION	SELECT 'Updates', 'Update', 5
UNION	SELECT 'Service Packs', 'Service Pack', 6
UNION	SELECT 'Feature Packs', 'Feature Pack', 7
UNION	SELECT 'Drivers', 'Driver', 8
UNION	SELECT 'Tools', 'Tools', 9
UNION	SELECT 'Office 2007', 'Unbekannt', 10
UNION	SELECT 'Unbekannt', 'Unbekannt', 11

IF (SELECT COUNT(*) FROM #ManagedDevices) &gt; 0
BEGIN
	SELECT  
		 md.[Device Class]
		,md.DeviceID
		,md.DeviceName
		,isnull(md.[Oldest Missing Patch],'') [Oldest Missing Patch]
		,mp.[Patch Name]
		,mp.[Patch Description]
		,mp.Product
		,CASE WHEN pc.NewCategory IS NULL THEN 'Unbekannt' ELSE pc.NewCategory END [Patch Category]
		,mp.Age
		,CASE WHEN pc.PatchPriority IS NULL THEN 11 ELSE pc.PatchPriority END PatchPriority 
	FROM 
			#ManagedDevices2 md 
		LEFT JOIN 
			#MissingPatch mp ON md.DeviceID=mp.DeviceID
		LEFT JOIN
			#pc pc on mp.[Patch Category] COLLATE SQL_Latin1_General_CP1_CI_AS = pc.[Patch Category] COLLATE SQL_Latin1_General_CP1_CI_AS
	UNION
	Select '' [Device Class], 0 DeviceID, '' DeviceName, '' [Oldest Missing Patch], '' [Patch Name], '' [Patch Description], '' Product, NewCategory [Patch Category], 0 Age, PatchPriority from #pc 
END
ELSE
BEGIN
	SELECT  
		 md.[Device Class]
		,md.DeviceID
		,md.DeviceName
		,isnull(md.[Oldest Missing Patch],'') [Oldest Missing Patch]
		,mp.[Patch Name]
		,mp.[Patch Description]
		,mp.Product
		,CASE WHEN pc.NewCategory IS NULL THEN 'Unbekannt' ELSE pc.NewCategory END [Patch Category]
		,mp.Age
		,CASE WHEN pc.PatchPriority IS NULL THEN 11 ELSE pc.PatchPriority END PatchPriority 
	FROM 
			#ManagedDevices2 md 
		LEFT JOIN 
			#MissingPatch mp ON md.DeviceID=mp.DeviceID
		LEFT JOIN
			#pc pc on mp.[Patch Category] COLLATE SQL_Latin1_General_CP1_CI_AS = pc.[Patch Category] COLLATE SQL_Latin1_General_CP1_CI_AS
END

DROP TABLE #ManagedDevices
DROP TABLE #ManagedDevices2
DROP TABLE #MissingPatch
DROP TABLE #PC</CommandText>
