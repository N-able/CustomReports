    <DataSet Name="ds_LicenseData">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Customer_ID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ShowDeleted bit
SELECT @ShowDeleted = value from config.dbo.Config where [key] = 'ShowDeletedDevices'

select dev.DeviceName
	,app.displayname as SoftwareName
	,app.[version]
	,app.ActivationLicensekey
	,app.LicenseKeyType
from warehouse.dbo.fact_cim_application as app
	INNER JOIN warehouse.dbo.Dim_Device as Dev
		ON Dev.DeviceID = app.deviceid
		AND Dev.[Is Managed Device Indicator] = 'Managed Device'
		AND ((@ShowDeleted = 'False' AND Dev.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
		AND Dev.[Is Ignored Device Indicator] = 'Not an Ignored Device'
		AND Dev.[IsProbe Device Indicator] = 'Not a Probe Device'
where app.customerid in (@Customer_ID)
	and app.ActivationLicensekey is not null

UNION ALL

select 
	dev.DeviceName
	,os.reportedos as SoftwareName
	,os.[version]
	,os.ActivationLicensekey
	,os.LicenseKeyType
FROM [warehouse].[dbo].[fact_cim_operating system] as os
	INNER JOIN warehouse.dbo.Dim_Device as Dev
		ON Dev.DeviceID = os.deviceid
		AND Dev.[Is Managed Device Indicator] = 'Managed Device'
		AND ((@ShowDeleted = 'False' AND Dev.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
		AND Dev.[Is Ignored Device Indicator] = 'Not an Ignored Device'
		AND Dev.[IsProbe Device Indicator] = 'Not a Probe Device'
WHERE os.customerid in (@Customer_ID)
	and os.ActivationLicensekey is not null</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DeviceName">
          <DataField>DeviceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SoftwareName">
