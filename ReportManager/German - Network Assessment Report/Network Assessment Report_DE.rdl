    <DataSet Name="dsWarrantyDetails">
      <Query>
        <DataSourceName>WarehouseBD</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Customer_ID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ShowUnknownWarranty">
            <Value>=Parameters!ShowUnknownWarranty.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ShowDeleted nvarchar(30) 
	SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')

SELECT customerid, deviceid, MAX(installdate) as installdate
into #LatestOSInstall
FROM [fact_cim_operating system]
where CustomerID = @Customer_ID	
GROUP BY customerid, deviceid	
		
SELECT
	d.DeviceName
	,d.[Device Class]
	,d.[Warranty Expiry Date]
	,CASE WHEN datediff(day, GETDATE(),d.[Warranty Expiry Date]) is null THEN 1 ELSE 0 END AS 'Devices with unknown warranty expiry'
	,CASE WHEN datediff(day, GETDATE(),d.[Warranty Expiry Date]) &gt; 90 THEN 1 ELSE 0 END AS 'Devices with valid warranties'
	,CASE WHEN datediff(day, GETDATE(),d.[Warranty Expiry Date]) &lt; 0 THEN 1 ELSE 0 END AS 'Devices with expired warranties'
	,CASE WHEN datediff(day, GETDATE(),d.[Warranty Expiry Date]) between 0 and 90 THEN 1 ELSE 0 END as 'Devices with warranties that will expire soon'
	,cs.serialnumber
	,os.installdate as 'OSInstallDate'
	,cs.manufacturer + ' ' + cs.model as 'Make and Model'
FROM dim_Device d
	LEFT JOIN [fact_cim_computer system] cs
		ON cs.customerid = d.CustomerID
		AND cs.deviceid = d.DeviceID
	LEFT JOIN #LatestOSInstall os
		ON os.customerid = d.CustomerID
		AND os.deviceid = d.DeviceID
		AND os.installdate &lt;= GETDATE()
WHERE d.CustomerID = @Customer_ID
	AND d.[Is Managed Device Indicator] = 'Managed Device'
	AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
	AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'
	AND ((@ShowUnknownWarranty = 'False' and d.[Warranty Expiry Date] is not null) OR @ShowUnknownWarranty = 'True')
	AND d.[IsProbe Device Indicator] = 'Not a Probe Device'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DeviceName">
          <DataField>DeviceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Device_Class">
          <DataField>Device Class</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Warranty_Expiry_Date">
          <DataField>Warranty Expiry Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Devices_with_unknown_warranty_expiry">
          <DataField>Devices with unknown warranty expiry</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Devices_with_valid_warranties">
          <DataField>Devices with valid warranties</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="serialnumber">
          <DataField>serialnumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Devices_with_expired_warranties">
          <DataField>Devices with expired warranties</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="OSInstallDate">
          <DataField>OSInstallDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Devices_with_warranties_that_will_expire_soon">
          <DataField>Devices with warranties that will expire soon</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Make_and_Model">
          <DataField>Make and Model</DataField>
          <rd:TypeName>System.String</rd:TypeName>
