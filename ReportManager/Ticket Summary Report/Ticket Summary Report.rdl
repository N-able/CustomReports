    <DataSet Name="dsResolutionStatus">
      <Fields>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Tickets">
          <DataField>Tickets</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Warehouse</DataSourceName>
        <CommandText>DECLARE @ManualResolved INT
DECLARE @ManualUnresolved INT
DECLARE @AutomatedResolved INT
DECLARE @AutomatedUnresolved INT

SELECT @ManualResolved= COUNT(*)
FROM fact_manualticket
WHERE customerid = @CustomerID
AND [timeopened] &gt;= @StartDate
AND [timeopened] &lt;= @EndDate
AND ticketresolved = 1
SELECT @ManualUnresolved= COUNT(*)
FROM fact_manualticket
WHERE customerid = @CustomerID
AND [timeopened] &gt;= @StartDate
AND [timeopened] &lt;= @EndDate
AND ticketresolved = 0

SELECT @AutomatedResolved= COUNT(*)
FROM fact_incidentticket
WHERE customerid = @CustomerID
AND [timeopened] &gt;= @StartDate
AND [timeopened] &lt;= @EndDate
AND ticketresolved = 1
SELECT @AutomatedUnresolved= COUNT(*)
FROM fact_incidentticket
WHERE customerid = @CustomerID
AND [timeopened] &gt;= @StartDate
AND [timeopened] &lt;= @EndDate
AND ticketresolved = 0

Create Table #ResolutionStatus
(
    Status VARCHAR(32) NULL, 
    Tickets INT NULL, 
)

Insert Into #ResolutionStatus
SELECT 'Unresolved', @ManualUnresolved+@AutomatedUnresolved

Insert Into #ResolutionStatus
SELECT 'Resolved', @ManualResolved+@AutomatedResolved

Select * from #ResolutionStatus</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsTicketDetails">
      <Fields>
        <Field Name="Ticket_Number">
          <DataField>Ticket Number</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TicketResolved">
          <DataField>TicketResolved</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="Time_Opened">
          <DataField>Time Opened</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Incident_Severity">
          <DataField>Incident Severity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Profile_Name">
          <DataField>Profile Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Ticket_Status">
          <DataField>Ticket Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Incident_Devices">
          <DataField>Incident Devices</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Incident_Services">
          <DataField>Incident Services</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Incident_Device_Names">
          <DataField>Incident Device Names</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Incident_Service_Names">
          <DataField>Incident Service Names</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Details">
          <DataField>Details</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Warehouse</DataSourceName>
        <CommandText>Select 
	 it.TicketID [Ticket Number]
	,it.TicketResolved
	,it.TimeOpened [Time Opened]
	,i.[Incident Severity]
	,i.[Profile Name]
	,Case when it.TicketResolved= 1 then 'Resolved' else 'Unresolved' end [Ticket Status]
	,[Incident Devices]
	,[Incident Services]
	,isnull( (SELECT STUFF((SELECT ',' + d.DeviceName FROM dbo.fact_IncidentDevice id inner join dbo.dim_Device d on id.DeviceID=d.DeviceID
				WHERE id.DataSourceID=it.DataSourceID and id.[N-central IncidentID]=it.[N-central IncidentID]
				ORDER BY d.DeviceName
				FOR XML PATH('')),1, 1, '')),'')  [Incident Device Names]
	, isnull( (SELECT STUFF((SELECT ',' + s.ServiceName FROM dbo.fact_IncidentService incs inner join dbo.dim_Service s on incs.ServiceID=s.ServiceID
							  WHERE incs.DataSourceID=it.DataSourceID and incs.[N-central IncidentID]=it.[N-central IncidentID]
							  ORDER BY s.ServiceName
							  FOR XML PATH('')),1, 1, '')),'') [Incident Service Names]
	,'&lt;b&gt;Incident Profile:&lt;/b&gt; '+isnull(i.[Profile Name],'')
	+'&lt;br&gt;&lt;b&gt;Incident Devices:&lt;/b&gt; '+isnull( (SELECT STUFF((SELECT ',' + d.DeviceName FROM dbo.fact_IncidentDevice id inner join dbo.dim_Device d on id.DeviceID=d.DeviceID
				WHERE id.DataSourceID=it.DataSourceID and id.[N-central IncidentID]=it.[N-central IncidentID]
				ORDER BY d.DeviceName
				FOR XML PATH('')),1, 1, '')),'')
	+'&lt;br&gt;&lt;b&gt;Incident Services:&lt;/b&gt; '+ isnull( (SELECT STUFF((SELECT ',' + s.ServiceName FROM dbo.fact_IncidentService incs inner join dbo.dim_Service s on incs.ServiceID=s.ServiceID
							  WHERE incs.DataSourceID=it.DataSourceID and incs.[N-central IncidentID]=it.[N-central IncidentID]
							  ORDER BY s.ServiceName
							  FOR XML PATH('')),1, 1, '')),'')
	+'&lt;br&gt;&lt;b&gt;Incident Severity:&lt;/b&gt; '+ isnull(i.[Incident Severity],'') [Details]
FROM 
		fact_incidentticket it 
	left join dbo.fact_Incident i
		on it.[N-central IncidentID]=i.[N-central IncidentID] and it.DataSourceID=i.DatasourceID
WHERE
	    it.customerid=@Customerid 
	AND (it.TimeOpened between @StartDate and @EndDate)

union

Select 
	 mt.TicketID [Ticket Number]
	,mt.TicketResolved
	,mt.TimeOpened [Time Opened]
	,'' [Incident Severity]
	,'' [Profile Name]
	,Case when mt.TicketResolved= 1 then 'Resolved' else 'Unresolved' end [Ticket Status]
	,cast(d.deviceID as varchar(30)) [Incident Devices]
	,cast(s.serviceID as varchar(30)) [Incident Services]
	,d.DeviceName [Incident Device Names]
	,s.ServiceName [Incident Service Names]				
	,'&lt;b&gt;Incident Devices:&lt;/b&gt; ' + isnull(d.DeviceName,'')
	+'&lt;br&gt;&lt;b&gt;Incident Services:&lt;/b&gt; ' + isnull(s.ServiceName,'') [Details]
FROM 
		fact_manualticket mt 
	left join
		dbo.dim_Device d on mt.DeviceID=d.DeviceID		
	left join 
		dbo.dim_Service s on mt.ServiceID=s.ServiceID
WHERE 
	    mt.customerid=@Customerid 
	AND (mt.TimeOpened between @StartDate and @EndDate)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Customerid">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsDateInterval">
      <Fields>
        <Field Name="Interval">
          <DataField>Interval</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Warehouse</DataSourceName>
        <CommandText>IF EXISTS(
SELECT 	TimeOpened
FROM [dbo].[fact_IncidentTicket]
WHERE CustomerID=@CustomerID AND TimeOpened BETWEEN @StartDate AND @EndDate
UNION
SELECT TimeOpened
FROM	dbo.fact_ManualTicket
WHERE CustomerID=@CustomerID AND TimeOpened BETWEEN @StartDate AND @EndDate
)
SELECT COUNT(DateOpened) AS Interval
FROM
(
SELECT 	CONVERT(varchar,TimeOpened,112) AS DateOpened
FROM [dbo].[fact_IncidentTicket]
WHERE CustomerID=@CustomerID AND TimeOpened BETWEEN @StartDate AND @EndDate
UNION
SELECT CONVERT(varchar,TimeOpened,112) AS DateOpened
FROM	dbo.fact_ManualTicket
WHERE CustomerID=@CustomerID AND TimeOpened BETWEEN @StartDate AND @EndDate
) a
ELSE
SELECT 0 AS Interval</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
