    <DataSet Name="RPTES_ServerPerformance_ServerDeltaDetails">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeServerPerformance.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndHour">
            <Value>=IIf(Parameters!EndHour.Value&lt;24, Parameters!EndHour.Value*60 + 1, Parameters!EndHour.Value*60)</Value>
          </QueryParameter>
          <QueryParameter Name="@StartHour">
            <Value>=Parameters!StartHour.Value*60+1</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeBreakdown">
            <Value>=Parameters!IncludeBreakdown.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WeekType">
            <Value>=Parameters!WeekType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PeriodType">
            <Value>=Parameters!PeriodType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UtilizationDeltaLimit">
            <Value>=Parameters!ServerPerformanceDeltaLimit.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NumberOfDevices">
            <Value>=Parameters!ServerPerformanceNumberOfDevices.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ScoreOnly">
            <Value>0</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT 
				NULL AS [DeviceID], 
				NULL AS [DeviceName], 
				NULL AS [Utilization_Detail_Name], 
				NULL AS [Lowest_Daily_Average], 
				NULL AS [Highest_Daily_Average], 
				NULL AS [Average], 
				NULL AS [Standard_Deviation],
				NULL AS [Delta_Change]
			WHERE 1=2
		END
	ELSE
		BEGIN
			-- Declare @allDayFlag Indicator
			DECLARE @allDay bit

			-- Set All Day to true if the End Hour equals Start Hour 
			IF (@EndHour = @StartHour)
				SET @allday = 1
			ELSE
				 SET @allDay = 0
				 
				DECLARE @Start_Date datetime
				DECLARE @End_Date datetime
				-- Get the start and end dates for the report
				SET @Start_Date = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
				SET @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

				-- Create the date variables for reuse in the database filter.
				DECLARE @Start_DateIndex int, @End_DateIndex int
				SELECT @Start_DateIndex = dbo.dateIndex(@Start_Date)
				SELECT @End_DateIndex = dbo.dateIndex(@End_Date)

				DECLARE @mCustomer TABLE(DataSourceID int, CustomerID int)
				
				-- Create a list of N-central CustomerID based on a list of dim_CustomerIDs
				INSERT INTO @mCustomer 
				SELECT [DataSourceID], [N-central CustomerID] CustomerID FROM dim_Customer 
				WHERE CustomerID IN (SELECT Value FROM inline_split_me(@CustomerID))

				-- Check if showing deleted devices
					DECLARE @ShowDeleted nvarchar(30) 	 
					SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')

				DECLARE @mDailyUtil TABLE (
					[DateID]		int,
					[DeviceID]		int,
					[DeviceName]		nvarchar(100),
					[Monitored Element]	nvarchar(100),
					[UtilizationTypeID]	tinyint,
					[UtilizationTypeName]	nvarchar(100),
					[Daily Average]		float)

				DECLARE @mAverageUtil TABLE (
					[DeviceID]		int,
					[UtilizationTypeID]	tinyint,
					[Monitored Element]	nvarchar(100) COLLATE Latin1_General_CI_AS,
					[Average]		float)

				DECLARE @mTopUtil TABLE (
					[DeviceID]		int,
					[MaxDeltaAverage]	float)

				DECLARE @mUtilization TABLE (
					[DeviceID]		int,
					[DeviceName]		nvarchar(100),
					[Monitored Element]	nvarchar(100) COLLATE Latin1_General_CI_AS,
					[UtilizationTypeID]	tinyint,
					[UtilizationTypeName]	nvarchar(100) COLLATE Latin1_General_CI_AS,
					[Lowest Daily Average]	float,
					[Highest Daily Average]	float,
					[Average]		float,
					[Standard Deviation]	float,
					[Delta Change]		float)

				-- Utilization Type
				-- 1	CPU
				-- 2	Disk
				-- 3	Virtual Memory
				-- 4	Physical Memory
				
				-- Get Utilization Data for CPU, Virtual Memory, and Physical Memory
				INSERT INTO @mDailyUtil
				SELECT u.DateID, d.DeviceID, d.DeviceName, '', t.[UtilizationTypeID], t.[Name], 
						 AVG(case
						   when u.[Usage Percentage] &lt; 0 then 0.0
						   when u.[Usage Percentage] &gt; 100 then 100.0
						   else u.[Usage Percentage]
						 end)
				FROM [fact_Utilization] u 
				INNER JOIN [dim_Utilization Type] t ON t.[UtilizationTypeID] = u.[UtilizationTypeID]
				INNER JOIN [dim_Device] d ON u.DeviceID = d.DeviceID
				INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
				WHERE d.[Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows') 
					AND u.[UtilizationTypeID] != 2
					AND (u.[Maximum Units] &gt; 0 OR u.[Usage Percentage] is not null)
					AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
					AND d.[Device Class] IN ( 'Windows Server', 'Generic Server' )
					AND (u.DateID &gt;= @Start_DateIndex) AND (u.DateID &lt;= @End_DateIndex)
					AND (d.[Is Managed Device Indicator] = 'Managed Device')
					 AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device' )
					-------------------------
					
						AND ( 
								--Check if Time SLA Is Included				
								(@IncludeBreakdown = 0)	
								OR -- Check if We are Reporting an All DAY
								(@allDay = 1)			  
								OR  -- Include Time Period
								( u.TimeID BETWEEN @StartHour AND @EndHour )					
								
							)
						AND (
							 --Check if time Sla is Included
							(@IncludeBreakdown = 0)
							OR -- check if All Days of Week indicated
							(@WeekType = 'Weekday, Weekend')
							OR -- filter by Weekend or WeekDay
							(@WeekType = dbo.GetWeekEndIndicatorFromDateId(u.DateID))											
						  )
					-------------------------&gt;&gt;
				GROUP BY u.DateID, d.DeviceID, d.DeviceName, t.[UtilizationTypeID], t.[Name]

				INSERT INTO @mUtilization
				SELECT DeviceID, DeviceName, '', [UtilizationTypeID], [UtilizationTypeName], 
					MIN([Daily Average]), MAX([Daily Average]),
					0, STDEV([Daily Average]), 0
				FROM @mDailyUtil 
				GROUP BY DeviceID, DeviceName, [UtilizationTypeID], [UtilizationTypeName]


				INSERT INTO @mAverageUtil
				SELECT u1.DeviceID, u1.[UtilizationTypeID], '',
				  AVG(CASE
						   when u1.[Usage Percentage] &lt; 0 THEN 0.0
						   when u1.[Usage Percentage] &gt; 100 THEN 100.0
						   else u1.[Usage Percentage]
						 END)
				FROM [fact_Utilization] u1 
				INNER JOIN @mUtilization u2 ON u1.DeviceID = u2.DeviceID AND u1.[UtilizationTypeID] = u2.[UtilizationTypeID]
				WHERE u1.[UtilizationTypeID] != 2 
				AND (u1.[Maximum Units] &gt; 0 OR u1.[Usage Percentage] is not null)
				AND (u1.DateID &gt;= @Start_DateIndex) AND (u1.DateID &lt;= @End_DateIndex)
				-------------------------
					
						AND ( 
								--Check if Time SLA Is Included				
								(@IncludeBreakdown = 0)	
								OR -- Check if We are Reporting an All DAY
								(@allDay = 1)			  
								OR  -- Include Time Period
								( u1.TimeID between @StartHour AND @EndHour )					
								
							)
						AND (
							 --Check if time Sla is Included
							(@IncludeBreakdown = 0)
							OR -- check if All Days of Week indicated
							(@WeekType = 'Weekday, Weekend')
							OR -- filter by Weekend or WeekDay
							(@WeekType = dbo.GetWeekEndIndicatorFromDateId(u1.DateID))							
						  )
					-------------------------&gt;&gt;
				GROUP BY u1.DeviceID, u1.[UtilizationTypeID]

				UPDATE @mUtilization
				SET [Average] = a.[Average]
				FROM @mUtilization u
				INNER JOIN @mAverageUtil a ON a.DeviceID = u.DeviceID AND a.[UtilizationTypeID] = u.[UtilizationTypeID]
				WHERE u.[UtilizationTypeID] != 2
				
				
				-- Get Utilization Data for Disk
				DELETE FROM @mDailyUtil
				DELETE FROM @mAverageUtil

				INSERT INTO @mDailyUtil
				SELECT u.DateID, d.DeviceID, d.DeviceName, u.[Monitored Element], t.[UtilizationTypeID] , t.[Name], 
						 AVG(CASE
						   WHEN u.[Usage Percentage] &lt; 0 THEN 0.0
						   WHEN u.[Usage Percentage] &gt; 100 THEN 100.0
						   ELSE u.[Usage Percentage]
						 END)
				FROM [fact_Utilization] u 
				INNER JOIN [dim_Utilization Type] t ON t.[UtilizationTypeID] = u.[UtilizationTypeID]
				INNER JOIN [dim_Device] d ON u.DeviceID = d.DeviceID
				INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
				WHERE d.[Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows') 
					AND u.[UtilizationTypeID] = 2
					AND (u.[Maximum Units] &gt; 0 OR u.[Usage Percentage] is not null)
					AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
					AND d.[Device Class] IN ( 'Windows Server', 'Generic Server' )
					AND (u.DateID &gt;= @Start_DateIndex) AND (u.DateID &lt;= @End_DateIndex)
					AND (d.[Is Managed Device Indicator] = 'Managed Device')
					 AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
					-------------------------
						AND ( 
								--Check if Time SLA Is Included				
								(@IncludeBreakdown = 0)	
								OR -- Check if We are Reporting an All DAY
								(@allDay = 1)			  
								OR  -- Include Time Period
								( u.TimeID BETWEEN @StartHour AND @EndHour )					
								
							)
						AND (
							 --Check if time Sla is Included
							(@IncludeBreakdown = 0)
							OR -- check if All Days of Week indicated
							(@WeekType = 'Weekday, Weekend')
							OR -- filter by Weekend or WeekDay
							(@WeekType = dbo.GetWeekEndIndicatorFromDateId(u.DateID))							
						  )
					-------------------------&gt;&gt;
				GROUP BY u.DateID, d.DeviceID, d.DeviceName, t.[UtilizationTypeID], t.[Name], u.[Monitored Element]

				INSERT INTO @mUtilization
				SELECT DeviceID, DeviceName, [Monitored Element], [UtilizationTypeID], [UtilizationTypeName], 
					MIN([Daily Average]), MAX([Daily Average]), 
					0, STDEV([Daily Average]), 0
				FROM @mDailyUtil 
				GROUP BY DeviceID, DeviceName, [UtilizationTypeID], [UtilizationTypeName], [Monitored Element]


				INSERT INTO @mAverageUtil
				SELECT u1.DeviceID, u1.[UtilizationTypeID], u1.[Monitored Element],      AVG(CASE
						   WHEN u1.[Usage Percentage] &lt; 0 THEN 0.0
						   WHEN u1.[Usage Percentage] &gt; 100 THEN 100.0
						   ELSE u1.[Usage Percentage]
						 END)
				FROM [fact_Utilization] u1 
				INNER JOIN @mUtilization u2 ON u1.DeviceID = u2.DeviceID AND u1.[UtilizationTypeID] = u2.[UtilizationTypeID] AND u1.[Monitored Element] = u2.[Monitored Element]
				WHERE u1.[UtilizationTypeID] = 2 
				AND (u1.[Maximum Units] &gt; 0 OR u1.[Usage Percentage] is not null)
				AND (u1.DateID &gt;= @Start_DateIndex) AND (u1.DateID &lt;= @End_DateIndex)
				GROUP BY u1.DeviceID, u1.[UtilizationTypeID], u1.[Monitored Element]

				UPDATE @mUtilization
				SET [Average] = a.[Average]
				FROM @mUtilization u
				INNER JOIN @mAverageUtil a ON a.DeviceID = u.DeviceID AND a.[UtilizationTypeID] = u.[UtilizationTypeID] AND a.[Monitored Element] = u.[Monitored Element]
				WHERE u.[UtilizationTypeID] = 2

				DECLARE @deltaDays int	
				IF dbo.GetPreviousDate(@Start_Date, @PeriodType) = @Start_Date
					BEGIN
						-- Bug 42765 - support of the date intervals based on "n days"
						SET @deltaDays = DATEDIFF(day,@Start_Date,@End_Date)
						SET @End_DateIndex = dbo.dateIndex(DATEADD(day,-1,@Start_Date))
						SET @Start_DateIndex = dbo.dateIndex(DATEADD(day,-(@deltaDays+1),@Start_Date))
					END
				ELSE
					BEGIN
						SELECT @Start_DateIndex = dbo.dateIndex(dbo.GetPreviousDate(@Start_Date, @PeriodType))
						SELECT @End_DateIndex = dbo.dateIndex(dbo.GetPreviousDate(@End_Date, @PeriodType))
					END
				-- Calculate previous utilization
				DELETE FROM @mAverageUtil

				INSERT INTO @mAverageUtil
				SELECT u1.DeviceID, u1.[UtilizationTypeID], '',
				  AVG(CASE
						   WHEN u1.[Usage Percentage] &lt; 0 THEN 0.0
						   WHEN u1.[Usage Percentage] &gt; 100 THEN 100.0
						   ELSE u1.[Usage Percentage]
						 END)
				FROM [fact_Utilization] u1 
				INNER JOIN @mUtilization u2 ON u1.DeviceID = u2.DeviceID AND u1.[UtilizationTypeID] = u2.[UtilizationTypeID]
				WHERE u1.[UtilizationTypeID] != 2
				AND (u1.[Maximum Units] &gt; 0 OR u1.[Usage Percentage] is not null)
				AND (u1.DateID &gt;= @Start_DateIndex) AND (u1.DateID &lt;= @End_DateIndex)
				-------------------------
					
						AND ( 
								--Check if Time SLA Is Included				
								(@IncludeBreakdown = 0)	
								OR -- Check if We are Reporting an All DAY
								(@allDay = 1)			  
								OR  -- Include Time Period
								( u1.TimeID between @StartHour AND @EndHour )					
							)
						AND (
							 --Check if time Sla is Included
							(@IncludeBreakdown = 0)
							OR -- check if All Days of Week indicated
							(@WeekType = 'Weekday, Weekend')
							OR -- filter by Weekend or WeekDay
							(@WeekType = dbo.GetWeekEndIndicatorFromDateId(u1.DateID))			
							OR
							(@WeekType = dbo.GetWeekEndIndicatorFromDateId(u1.DateID))				
						  )
					-------------------------&gt;&gt;
				GROUP BY u1.DeviceID, u1.[UtilizationTypeID]

				INSERT INTO @mAverageUtil
				SELECT u1.DeviceID, u1.[UtilizationTypeID], u1.[Monitored Element],
				  AVG(case
						   WHEN u1.[Usage Percentage] &lt; 0 THEN 0.0
						   WHEN u1.[Usage Percentage] &gt; 100 THEN 100.0
						   ELSE u1.[Usage Percentage]
						 END)
				FROM [fact_Utilization] u1 
				INNER JOIN @mUtilization u2 ON u1.DeviceID = u2.DeviceID AND u1.[UtilizationTypeID] = u2.[UtilizationTypeID] AND u1.[Monitored Element] = u2.[Monitored Element]
				WHERE u1.[UtilizationTypeID] = 2 
				AND (u1.[Maximum Units] &gt; 0 OR u1.[Usage Percentage] is not null)
				AND (u1.DateID &gt;= @Start_DateIndex) AND (u1.DateID &lt;= @End_DateIndex)
				-------------------------
						AND ( 
								--Check if Time SLA Is Included				
								(@IncludeBreakdown = 0)	
								OR -- Check if We are Reporting an All DAY
								(@allDay = 1)			  
								OR  -- Include Time Period
								( u1.TimeID BETWEEN @StartHour AND @EndHour )					
								
							)
						AND (
							 --Check if time Sla is Included
							(@IncludeBreakdown = 0)
							OR -- check if All Days of Week indicated
							(@WeekType = 'Weekday, Weekend')
							OR -- filter by Weekend or WeekDay
							(@WeekType = dbo.GetWeekEndIndicatorFromDateId(u1.DateID))							
						  )
					-------------------------&gt;&gt;
				GROUP BY u1.DeviceID, u1.[UtilizationTypeID], u1.[Monitored Element]

				DECLARE @PreviousCount int
				SELECT @PreviousCount = COUNT(*) FROM @mAverageUtil

				UPDATE @mUtilization
				SET [Delta Change] = ISNULL(u.[Average], 0) - ISNULL(a.[Average], 0)
				FROM @mUtilization u
				INNER JOIN @mAverageUtil a ON a.DeviceID = u.DeviceID AND a.[UtilizationTypeID] = u.[UtilizationTypeID] AND a.[Monitored Element] = u.[Monitored Element]
				-- Calculate the Score
				DECLARE @Count1 real
				DECLARE @Count2 real
				SELECT @Count1 = COUNT(*) FROM @mUtilization WHERE ROUND([Delta Change],2) &lt; @UtilizationDeltaLimit
				SELECT @Count2 = COUNT(*) FROM @mUtilization
				--!!
				DECLARE @ScoreValue float
				--!!
				IF @Count2 = 0 OR @PreviousCount = 0
					SET @ScoreValue = -1
				ELSE
					SET @ScoreValue = @Count1/@Count2 * 100
				-- Get top 5 devices
				DECLARE @NumOfDevices int
					SET @NumOfDevices = ISNULL(@NumberOfDevices,0)
				SET ROWCOUNT @NumOfDevices

				INSERT INTO @mTopUtil
				SELECT DeviceID, MAX(ABS([Delta Change]))
				FROM @mUtilization 
				GROUP BY DeviceID
				-- HAVING MAX(ABS([Delta Change])&gt;0 -- 
				ORDER BY MAX([Delta Change]) desc
				
				SET ROWCOUNT 0

				IF (@ScoreOnly = 0)
					BEGIN
						-- Return Utilization
						SELECT 
							u.[DeviceID], 
							u.[DeviceName], 
							CASE 
								WHEN u.[UtilizationTypeID] = 2 THEN u.[UtilizationTypeName] + ' - ' + LTRIM(RTRIM(u.[Monitored Element]))
								ELSE u.[UtilizationTypeName]
							END AS [Utilization_Detail_Name], 
							ISNULL(u.[Lowest Daily Average],0) AS [Lowest_Daily_Average], 
							ISNULL(u.[Highest Daily Average],0) AS [Highest_Daily_Average], 
							ISNULL(u.[Average],0) AS [Average], 
							ISNULL(u.[Standard Deviation],0) AS [Standard_Deviation],
							ISNULL(u.[Delta Change],0) AS [Delta_Change]	
						FROM @mUtilization u
						INNER JOIN @mTopUtil t ON u.[DeviceID] = t.[DeviceID]
						ORDER BY u.DeviceID, u.DeviceName, [Utilization_Detail_Name], u.[Monitored Element]
					END
		END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DeviceID">
          <DataField>DeviceID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DeviceName">
          <DataField>DeviceName</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Utilization_Detail_Name">
          <DataField>Utilization_Detail_Name</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Lowest_Daily_Average">
          <DataField>Lowest_Daily_Average</DataField>
