    <DataSet Name="RPTES_DataProtection_BackupSummary">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeDataProtection.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT 	
				NULL AS [Device_Name], 
				NULL AS	[FQDN], 
				NULL AS [Backup_Job_Name], 
				NULL AS [Successful_Backups], 
				NULL AS [Backups_With_Warnings],
				NULL AS [Failed_Backups], 
				NULL AS [Last_Successful_Backup],
				NULL AS [Size_Of_Last_Backup]
			WHERE 1=2
		END
	ELSE
		BEGIN
			DECLARE @Start_Date datetime
			DECLARE @End_Date datetime
			-- get the true start and end date based on subscription or not
			SELECT @Start_Date = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
			SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)		
			-- Get all the incidents for this time period, where an incident is any incident which is opened during the period
			DECLARE @mTmpBackupTasks1 TABLE (
				DeviceID INTEGER,
				[Job ID] NVARCHAR(200),
				[Device Name] NVARCHAR(200),
				[FQDN] NVARCHAR(200),
				[Defined Task] NVARCHAR(200),
				Successful INTEGER,
				Warning INTEGER,
				Failed INTEGER,
				[Last Successful Date] datetime
			)
			-- get the data
			INSERT INTO @mTmpBackupTasks1
				SELECT 	fbs.DeviceID,
						[Job ID],
						DeviceName AS [Device Name],
						URI AS FQDN,
						[Job Name] AS [Defined Task],
						SUM(CASE WHEN [Job Return Code] = 1 THEN 1 ELSE 0 END) AS Successful,
						SUM(CASE WHEN [Job Return Code] = 2 THEN 1 ELSE 0 END) AS Warning,
						SUM(CASE WHEN [Job Return Code] = 3 THEN 1 ELSE 0 END) AS Failed,
						MAX(CASE WHEN [Job Return Code] IN (1, 2) THEN [Job StartTime] ELSE NULL END) AS [Last Successful Date]
				FROM Fact_BackupServices fbs
					INNER JOIN Dim_Device d ON d.DeviceID = fbs.DeviceID
				WHERE fbs.CustomerID = @CustomerID 
				 AND d.[Is Managed Device Indicator] = 'Managed Device'
				 AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'
				 AND @Start_Date &lt;= [Job StartTime] AND [Job StartTime] &lt; DATEADD(Day, 1, @End_Date)				
				GROUP BY fbs.[DeviceID], fbs.[Job ID], DeviceName, URI, [Job Name]

			-- Here is is
			SELECT 	
					[Device Name] AS Device_Name, 
					[FQDN], 
					[Defined Task] AS [Backup_Job_Name], 
					ISNULL(Successful, 0) AS [Successful_Backups], 
					ISNULL(Warning, 0) AS [Backups_With_Warnings],
					ISNULL(Failed, 0) AS [Failed_Backups], 
					[Last Successful Date] AS [Last_Successful_Backup],
					ISNULL((SELECT TOP 1 [Total Bytes Processed] 
							FROM Fact_BackupServices fbs 
							WHERE fbs.DeviceID = bt.DeviceID 
							AND fbs.[Job ID] COLLATE Latin1_General_CI_AS  = bt.[Job ID] COLLATE Latin1_General_CI_AS 
							AND [Job StartTime] = [Last Successful Date]
							AND [Job Return Code] IN (1, 2)
							), 0) AS [Size_Of_Last_Backup]
			FROM @mTmpBackupTasks1 bt
		END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Device_Name">
          <DataField>Device_Name</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FQDN">
          <DataField>FQDN</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Backup_Job_Name">
          <DataField>Backup_Job_Name</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Successful_Backups">
          <DataField>Successful_Backups</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Backups_With_Warnings">
