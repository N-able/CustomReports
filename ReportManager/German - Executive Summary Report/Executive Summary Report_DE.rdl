    <DataSet Name="RPTES_NetworkReliability_CapacityDetails">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeNetworkReliability.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndHour">
            <Value>=IIf(Parameters!EndHour.Value&lt;24, Parameters!EndHour.Value*60 + 1, Parameters!EndHour.Value*60)</Value>
          </QueryParameter>
          <QueryParameter Name="@StartHour">
            <Value>=Parameters!StartHour.Value*60+1</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeBreakdown">
            <Value>=Parameters!IncludeBreakdown.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WeekType">
            <Value>=Parameters!WeekType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT	
				NULL AS [Device_Name], 
				NULL AS [Interface_Name], 
				NULL AS [Network_Utilization_Percent]
			WHERE 1=2
		END
	ELSE
	BEGIN
-- Declare @allDayFlag Indicator
		DECLARE @allDay bit
		DECLARE @Start_Hour int
		DECLARE @End_Hour int
		SET @Start_Hour=@StartHour
		SET @End_Hour = @EndHour
		-- Set All Day to true if the End Hour equals Start Hour 
		IF (@End_Hour = @Start_Hour)
			SET @allday = 1
		ELSE
			 SET @allDay = 0
----------------------
		DECLARE @Start_Date datetime
		DECLARE @End_Date datetime
		-- get the true start and end date based on subscription or not
		SELECT @Start_Date = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
		SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)
		
		-- get the ids for the dates
		DECLARE @Start_DateID INTEGER
		DECLARE @End_DateID INTEGER
		SET @Start_DateID = CONVERT(int, CONVERT(char(8),@Start_Date, 112))
		SET @End_DateID = CONVERT(int, CONVERT(char(8),@End_Date, 112))

		-- get the datasourceid
		DECLARE @DataSourceID INTEGER, @NcentralCustomerID INTEGER
		SELECT @DataSourceID = DataSourceID, @NcentralCustomerID = [N-central CustomerID] FROM dim_Customer WHERE CustomerID = @CustomerID
		DECLARE @mCustomer TABLE (DataSourceID int, CustomerID int)
		
		-- Create a list of N-central CustomerID based on a list of dim_CustomerIDs
		INSERT INTO @mCustomer 
		SELECT [DataSourceID], [N-central CustomerID] CustomerID FROM dim_Customer 
		WHERE CustomerID IN (SELECT Value FROM inline_split_me(@CustomerID))
		-- Check if showing deleted devices
			DECLARE @ShowDeleted nvarchar(30) 	 
			SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')
		-- Return
		SELECT	TOP 20 dim_Device.[DeviceName] AS [Device_Name], 
			fact_Bandwidth.[Monitored Interface] AS [Interface_Name] 
			,AVG(fact_Bandwidth.[Utilization %]) AS [Network_Utilization_Percent]
		FROM	fact_Bandwidth 
		INNER JOIN dim_Device ON fact_Bandwidth.DeviceID = dim_Device.DeviceID
		INNER JOIN @mCustomer c ON dim_Device.[N-central CustomerID] = c.CustomerID AND dim_Device.[DataSourceID] = c.[DataSourceID]
		-- Animal 3.0 Abena
		-- Included Join To Get the Day of Week Number
		Inner Join dim_Date d on fact_Bandwidth.DateID = d.DateID	

		WHERE	fact_Bandwidth.DateID &gt;= @Start_DateID AND fact_Bandwidth.DateID &lt;= @End_DateID
			AND ((@ShowDeleted = 'False' AND dim_Device.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False')
			AND dim_Device.[Is Managed Device Indicator] = 'Managed Device'
			AND dim_Device.[Is Ignored Device Indicator] = 'Not an Ignored Device'
			AND dim_Device.[Device Class] = 'switch/router'
		-- Animal 3.0 Abena
			and ( 
					--Check if Time SLA Is Included				
					(@IncludeBreakdown = 0)	
					OR -- Check if We are Reporting an All DAY
					(@allDay = 1)			  
					OR  -- Include Time Period
					(fact_bandwidth.TimeID between @Start_Hour and @End_Hour)
				 )
			and (
					 --Check if time Sla is Included
					(@IncludeBreakdown = 0)--was 0
					OR -- check if All Days of Week indicated
					(@WeekType = 'Weekday, Weekend' )
					OR -- filter by Weekend or WeekDay
					(@WeekType = d.[Weekend Indicator])				
				)
		GROUP BY dim_Device.[DeviceName], fact_Bandwidth.[Monitored Interface]
		ORDER BY AVG(fact_Bandwidth.[Utilization %]) DESC
	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Device_Name">
          <DataField>Device_Name</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Interface_Name">
          <DataField>Interface_Name</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Network_Utilization_Percent">
          <DataField>Network_Utilization_Percent</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
