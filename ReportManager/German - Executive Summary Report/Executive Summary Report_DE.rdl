        <CommandText>Select 
	Customer.CustomerName
	,'Reaktiv' as 'Source'
	,SUM(CASE WHEN man.TicketID is null AND inc.TicketID is null AND psa.TicketID is not null THEN 1
		  ELSE 0 END) as Hits
from fact_Ticket PSA
	LEFT JOIN
		fact_ManualTicket Man
			on man.CustomerID = PSA.CustomerID
			AND Man.TicketID = PSA.TicketID
	LEFT JOIN
		fact_IncidentTicket Inc
			on Inc.CustomerID = PSA.CustomerID
			AND Inc.TicketID = PSA.TicketID
	LEFT JOIN
		dim_Customer Customer
			ON Customer.CustomerID = PSA.CustomerID
WHERE CAST(PSA.TimeOpened as DATE) BETWEEN CAST(@StartDate as DATE)
		AND CAST(@EndDate as DATE)
	AND PSA.CustomerID in (@Customer_ID)
	AND ((PSA.TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
	AND ((PSA.TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')
GROUP BY Customer.CustomerName

UNION ALL

Select 
	Customer.CustomerName
	,'Proaktiv' as 'Source'
	,SUM(CASE WHEN (man.TicketID is not null OR inc.TicketID is not null) AND psa.TicketID is not null THEN 1
		  ELSE 0 END) as Hits
from fact_Ticket PSA
	LEFT JOIN
		fact_ManualTicket Man
			on man.CustomerID = PSA.CustomerID
			AND Man.TicketID = PSA.TicketID
	LEFT JOIN
		fact_IncidentTicket Inc
			on Inc.CustomerID = PSA.CustomerID
			AND Inc.TicketID = PSA.TicketID
	LEFT JOIN
		dim_Customer Customer
			ON Customer.CustomerID = PSA.CustomerID
WHERE CAST(PSA.TimeOpened as DATE) BETWEEN CAST(@StartDate as DATE)
		AND CAST(@EndDate as DATE)
	AND PSA.CustomerID in (@Customer_ID)
	AND ((PSA.TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
	AND ((PSA.TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')
GROUP BY Customer.CustomerName</CommandText>
