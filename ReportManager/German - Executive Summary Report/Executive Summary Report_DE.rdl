                                         WHEN [start time] &lt; @PeriodStartDate THEN Dateadd(HOUR,@Start_Hour,@PeriodStartDate) 
                                       END,CASE -- End day same but end time greater than end hour. 
                                             WHEN Datepart(YEAR,[end time]) = Datepart(YEAR,@PeriodStartDate) 
                                                  AND Datepart(MONTH,[end time]) = Datepart(MONTH,@PeriodStartDate) 
                                                  AND Datepart(DAY,[end time]) = Datepart(DAY,@PeriodStartDate) 
                                                  AND Datepart(HOUR,[end time]) &gt;= @End_Hour THEN Dateadd(HOUR,@End_Hour,@PeriodStartDate) 
                                             -- End day same and end time between. 
                                             WHEN Datepart(YEAR,[end time]) = Datepart(YEAR,@PeriodStartDate) 
                                                  AND Datepart(MONTH,[end time]) = Datepart(MONTH,@PeriodStartDate) 
                                                  AND Datepart(DAY,[end time]) = Datepart(DAY,@PeriodStartDate) 
                                                  AND Datepart(HOUR,[end time]) &gt;= @Start_Hour 
                                                  AND Datepart(HOUR,[end time]) &lt; @End_Hour THEN [end time] 
                                             -- End day greater than date. 
                                             WHEN [end time] &gt;= Dateadd(DAY,1,@PeriodStartDate) THEN Dateadd(HOUR,@End_Hour,@PeriodStartDate) 
                                           END) AS FLOAT))) -- SPAN 
      FROM     #mFactAvailReduced AS a 
      WHERE    [start time] &lt;= @PeriodEndDate 
               AND [end time] &gt;= @PeriodStartDate 
      GROUP BY serviceid,a.available 
    END 
  -- cleanup
  DROP TABLE #mFactAvailReduced

  -- Delete all days of the week that are not selected. 
  IF (@IncludeBreakdown = 1) 
    BEGIN 
      IF (@WeekType = 'Weekday') 
        BEGIN 
          DELETE @mPeriodValues 
          FROM   @mPeriodValues AS red 
                 INNER JOIN [dim_date] AS dat 
                   ON red.[periodstartdate] = dat.[date] 
          WHERE  dat.[weekend indicator] != 'Weekday' 
        END 
      ELSE 
        IF (@WeekType = 'Weekend') 
          BEGIN 
            DELETE @mPeriodValues 
            FROM   @mPeriodValues AS red 
                   INNER JOIN [dim_date] AS dat 
                     ON red.[periodstartdate] = dat.[date] 
            WHERE  dat.[weekend indicator] != 'Weekend' 
          END 
    END 
DECLARE @Vals TABLE (
		serviceid int,
		servicename	nvarchar(200),
		available float,
		[span length] float
		)

Insert into @Vals
  SELECT s.serviceid,
		 servicename   AS [Service Name], 
         SUM(available),  -- [total Availability]
         SUM([span length])  --total length
  FROM     @mPeriodValues pv 
           INNER JOIN [dim_service] s 
             ON s.serviceid = pv.serviceid 
  GROUP BY servicename, s.serviceid 

  DECLARE  @mAvailPercentages  TABLE( 
                               [AvailPercentage]  float null 
                               ) 
INSERT INTO @mAvailPercentages
SELECT (v.available/v.[span length])*100 as AvailPercentage
FROM @Vals v iNNER JOIN [dim_service] s 
             ON s.serviceid = v.serviceid 
WHERE ISNULL(v.[span length],0) &lt;&gt; 0 AND v.available IS NOT NULL
GROUP BY v.servicename,v.serviceid,v.available,v.[span length], s.[Is Service Group],s.[Service Group SLA]
	ORDER BY v.serviceid,v.available

DECLARE  @AggAvailability FLOAT 

SELECT @AggAvailability = AVG(AvailPercentage) 
FROM   @mAvailPercentages
 
   
  
SELECT @AggAvailability       AS [Available_Percent],
         100 - @AggAvailability AS [Unavailable_Percent]
		WHERE @AggAvailability IS NOT NULL
END</CommandText>
