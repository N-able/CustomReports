    <DataSet Name="RPTES_ManagedDevices_IncidentSummary">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeManagedDevices.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeProgramLevel">
            <Value>=Parameters!IncludeProgramLevel.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- format the results for reporting services
			DECLARE @mTmpReturnTable TABLE 
			(
				Detail nvarchar(200),
				Workstation nvarchar(100),
				[Other Devices Classes] nvarchar(100)
			)
-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT * FROM @mTmpReturnTable WHERE 1=2
		END
	ELSE
		BEGIN
			DECLARE @tmpText nvarchar(200)
			DECLARE @Incident_ID int 
			DECLARE @Processed int
			DECLARE @MTTR datetime
			DECLARE @MTTA datetime
			DECLARE @Start_Date datetime
			DECLARE @End_Date datetime

			-- get the true start and end date based on subscription or not
			SELECT @Start_Date = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
			SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

			DECLARE @mTmpTableReduced TABLE(
				[IncidentID] INTEGER,
				[Incident Devices] NVARCHAR(200),
				[Incident Services] NVARCHAR(200),
				MTTA DATETIME,
				MTTR DATETIME
			)
			INSERT INTO @mTmpTableReduced 
			SELECT [N-central IncidentID], [Incident Devices], [Incident Services],
				CASE WHEN [Mean Time To Acknowledgement] IS NULL THEN [Mean Time To Resolution] ELSE [Mean Time To Acknowledgement] END,
				[Mean Time To Resolution] 
			FROM fact_Incident 
			WHERE CustomerID = @CustomerID
			AND @Start_Date &lt;= [Time Incident Opened] AND [Time Incident Opened] &lt;= @End_Date


			-- Get all the incidents for this time period, where an incident is any incident which is opened during the period
			DECLARE @mTmpTableMultipleDevices TABLE (
				[IncidentID] INTEGER, 
				Devices NVARCHAR(200), 
				MTTA DATETIME,
				MTTR DATETIME,
				Processed INTEGER
			)
			-- declare a temp table to store incidentid/deviceid so that we can split up the CSV stored in Incident Devices
			DECLARE @mTmpTableSingleDevices TABLE (
				[IncidentID] INTEGER,
				DeviceID INTEGER,
				MTTA DATETIME,
				MTTR DATETIME
			)

			-- First take care of only those with multiple devices
			INSERT INTO @mTmpTableMultipleDevices 
			SELECT [IncidentID], [Incident Devices], MTTA, MTTR, -1 
			FROM @mTmpTableReduced  
			WHERE [Incident Devices] LIKE '%,%'
			

			-- don't forget the ones that do not have multiple devices
			INSERT INTO @mTmpTableSingleDevices
			SELECT [IncidentID], [Incident Devices], MTTA, MTTR 
			FROM @mTmpTableReduced 
			WHERE [Incident Devices] NOT LIKE '%,%'
						

			-- loop through all the different incidents and break out the deviceids
			SELECT TOP 1 @Incident_ID = [IncidentID], @tmpText = Devices, @Processed = Processed, @MTTA = MTTA, @MTTR = MTTR FROM @mTmpTableMultipleDevices
			WHILE (@Processed = -1)
			BEGIN
				INSERT INTO @mTmpTableSingleDevices SELECT @Incident_ID, Value, @MTTA, @MTTR FROM dbo.inline_split_me(@tmpText)
				UPDATE @mTmpTableMultipleDevices SET Processed = 1 WHERE incidentid = @Incident_ID
				SELECT TOP 1 @Incident_ID = [IncidentID], @tmpText = Devices, @Processed = Processed, @MTTA = MTTA, @MTTR = MTTR 
				FROM @mTmpTableMultipleDevices ORDER BY Processed ASC
			END

			-- assign a device class to each incident/device combo
			CREATE TABLE #mTmpTableIncidentsWithDeviceClass (
				[IncidentID] INTEGER,
				DeviceID INTEGER,
				Status NVARCHAR(200),
				DeviceClass NVARCHAR(200),
				MTTA FLOAT,
				MTTR FLOAT
			)

			INSERT INTO #mTmpTableIncidentsWithDeviceClass
			SELECT [IncidentID], a.DeviceID, Status,
				CASE WHEN [Device Class] IN ('Generic Workstation', 'Windows Workstation') THEN 'Workstation' ELSE 'Non-Workstation' END AS DeviceClass,
				CONVERT(float,DATEDIFF(s, '19000101', MTTA)), CONVERT(float,DATEDIFF(s, '19000101', MTTR))
			FROM @mTmpTableSingleDevices a INNER JOIN dim_Device b ON a.DeviceID = b.DeviceID 
			/* Added 3.0 17th Sept, 2008*/
			Where	b.[is managed device indicator] = 'Managed Device' 
					AND b.[Is Ignored Device Indicator] = 'Not an Ignored Device'


			-- Honour the ShowDeleted flag (never show deleted devices in Managed section)
			DELETE FROM #mTmpTableIncidentsWithDeviceClass WHERE [Status] != 'Active'

			-- we only want one row per incident, with Non-Workstations having higher priority
			DELETE FROM #mTmpTableIncidentsWithDeviceClass WHERE DeviceClass != 'Non-Workstation' AND IncidentID IN (SELECT IncidentID FROM #mTmpTableIncidentsWithDeviceClass WHERE DeviceClass = 'Non-Workstation')



			-- determine which services are not incident based and remove them from the calculation
			DECLARE @mTmpTableMultipleServices TABLE (
				[IncidentID] INTEGER, 
				Services NVARCHAR(200), 
				Processed INTEGER
			)
			-- declare a temp table to store incidentid/serviceid so that we can split up the CSV stored in Incident Services
			CREATE TABLE #mTmpTableSingleService (
				[IncidentID] INTEGER,
				ServiceID INTEGER
			)

			-- first take care of only those with multiple services
			INSERT INTO @mTmpTableMultipleServices
			SELECT [IncidentID], [Incident Services], -1 
			FROM @mTmpTableReduced 
			WHERE [Incident Services] LIKE '%,%'
			
			-- don't forget the ones that do not have multiple devices
			INSERT INTO #mTmpTableSingleService
			SELECT [IncidentID], [Incident Services] 
			FROM @mTmpTableReduced 
			WHERE [Incident Services] NOT LIKE '%,%'

			-- loop through all the different incidents and break out the serviceids
			SELECT TOP 1 @Incident_ID = IncidentID, @tmpText = Services, @Processed = Processed FROM @mTmpTableMultipleServices
			WHILE (@Processed = -1)
				BEGIN
					INSERT INTO #mTmpTableSingleService SELECT @Incident_ID, Value FROM dbo.inline_split_me(@tmpText)
					UPDATE @mTmpTableMultipleServices SET Processed = 1 WHERE incidentid = @Incident_ID
					SELECT TOP 1 @Incident_ID = IncidentID, @tmpText = Services, @Processed = Processed
						FROM @mTmpTableMultipleServices ORDER BY Processed ASC
				END

			-- Figure out which incidents are incident based
			CREATE INDEX tmpIDX_TmpTableIncidentsWithDeviceClass ON #mTmpTableIncidentsWithDeviceClass (IncidentID)
			CREATE INDEX tmpIDX_TmpTableSingleService ON #mTmpTableSingleService (ServiceID)

			-- figure out how many incidents before we get rid of the non-incident based ones
			DECLARE @mCountWorkstation INTEGER, @mCountNonWorkstation INTEGER
			SELECT @mCountWorkstation = COUNT(*) FROM #mTmpTableIncidentsWithDeviceClass WHERE [DeviceClass] = 'Workstation'
			SELECT @mCountNonWorkstation = COUNT(*) FROM #mTmpTableIncidentsWithDeviceClass WHERE [DeviceClass] = 'Non-Workstation'

			-- figure out how many closed incidents before we get rid of the non-incident based ones
			DECLARE @mCountClosedNonWorkstation INTEGER, @mCountClosedWorkstation INTEGER
			SELECT @mCountClosedWorkstation = COUNT(*) FROM #mTmpTableIncidentsWithDeviceClass WHERE [DeviceClass] = 'Workstation' AND [MTTR] IS NOT NULL
			SELECT @mCountClosedNonWorkstation = COUNT(*) FROM #mTmpTableIncidentsWithDeviceClass WHERE [DeviceClass] = 'Non-Workstation' AND [MTTR] IS NOT NULL

			DELETE FROM #mTmpTableIncidentsWithDeviceClass WHERE IncidentID IN 
				(SELECT IncidentID FROM #mTmpTableSingleService t6 INNER JOIN [dim_service] ds on t6.ServiceID = ds.ServiceID WHERE [Incident Based] = 'Is incident based')


			-- get the customer MTTA and MTTR
			DECLARE @CustMTTA FLOAT
			DECLARE @CustMTTR FLOAT
			SELECT @CustMTTA = ([Program Mean Time to Acknowledgement] * 60 * 60), @CustMTTR = ([Program Mean Time to Resolution] * 60 * 60)
				FROM dim_Customer WHERE CustomerID = @CustomerID

			-- For each device class group, determine MTTA and MTTR, and the number of incidents exceeding the customer MTTA and MTTR
			DECLARE @mTmpTable7 TABLE(
				DeviceClass NVARCHAR(200),
				[Total Number of Incidents Generated] INTEGER,
				[Number of Closed Incidents] INTEGER,
				[Average Time to Acknowlegement] FLOAT,
				[Average Time to Resolution] FLOAT,
				[Number of Incidents Exceeding MTTA] INTEGER,
				[Number of Incidents Exceeding MTTR] INTEGER
			)

			-- get our final table ready
			INSERT INTO @mTmpTable7
				SELECT DeviceClass, 0, 0, AVG(MTTA), AVG(MTTR), 0, 0 FROM #mTmpTableIncidentsWithDeviceClass GROUP BY DeviceClass

			DECLARE @mMTTA1 INTEGER, @mMTTA2 INTEGER, @mMTTR1 INTEGER, @mMTTR2 INTEGER

			-- get the MTTAs for the different device class groups
			SELECT @mMTTA1 = COUNT(*) FROM #mTmpTableIncidentsWithDeviceClass WHERE DeviceClass = 'Non-Workstation' AND MTTA &gt; @CustMTTA;
			SELECT @mMTTA2 = COUNT(*) FROM #mTmpTableIncidentsWithDeviceClass WHERE DeviceClass = 'Workstation' AND MTTA &gt; @CustMTTA;

			-- get the MTTRs for the different device class groups
			SELECT @mMTTR1 = COUNT(*) FROM #mTmpTableIncidentsWithDeviceClass WHERE DeviceClass = 'Non-Workstation' AND MTTR &gt; @CustMTTR;
			SELECT @mMTTR2 = COUNT(*) FROM #mTmpTableIncidentsWithDeviceClass WHERE DeviceClass = 'Workstation' AND MTTR &gt; @CustMTTR;

			-- set the device class group Non-Workstations
			UPDATE @mTmpTable7 
				SET [Total Number of Incidents Generated] = @mCountNonWorkstation, 
					[Number of Closed Incidents] = @mCountClosedNonWorkstation, 
					[Number of Incidents Exceeding MTTA] = @mMTTA1, 
					[Number of Incidents Exceeding MTTR] = @mMTTR1
				WHERE DeviceClass = 'Non-Workstation'

			-- set the device class group Workstations
			UPDATE @mTmpTable7 
				SET [Total Number of Incidents Generated] = @mCountWorkstation,  
					[Number of Closed Incidents] = @mCountClosedWorkstation, 
					[Number of Incidents Exceeding MTTA] = @mMTTA2, 
					[Number of Incidents Exceeding MTTR] = @mMTTR2
				WHERE DeviceClass = 'Workstation'

			DECLARE @mW NVARCHAR(100)
			DECLARE @mN NVARCHAR(100)
			DECLARE @miW INTEGER
			DECLARE @miN INTEGER

			SELECT @mW = [Total Number of Incidents Generated] FROM @mTmpTable7 WHERE DeviceClass = 'Workstation'
			SELECT @mN = [Total Number of Incidents Generated] FROM @mTmpTable7 WHERE DeviceClass = 'Non-Workstation'
			INSERT INTO @mTmpReturnTable VALUES ('Gesamtanzahl der generierten Vorfälle', 
					CASE WHEN @mW IS NULL THEN '0' ELSE @mW END, 
					CASE WHEN @mN IS NULL THEN '0' ELSE @mN END)

			SELECT @mW = [Number of Closed Incidents] FROM @mTmpTable7 WHERE DeviceClass = 'Workstation'
			SELECT @mN = [Number of Closed Incidents] FROM @mTmpTable7 WHERE DeviceClass = 'Non-Workstation'
			INSERT INTO @mTmpReturnTable VALUES ('Anzahl der geschlossenen Vorfälle', 
					CASE WHEN @mW IS NULL THEN '0' ELSE @mW END, 
					CASE WHEN @mN IS NULL THEN '0' ELSE @mN END)

			SELECT @miW = [Average Time to Acknowlegement] FROM @mTmpTable7 WHERE DeviceClass = 'Workstation'
			SELECT @miN = [Average Time to Acknowlegement] FROM @mTmpTable7 WHERE DeviceClass = 'Non-Workstation'
			SELECT @mW = CONVERT(varchar(6), @miW/3600)
							+ ':' + RIGHT('0' + CONVERT(varchar(2), (@miW % 3600) / 60), 2)
							+ ':' + RIGHT('0' + CONVERT(varchar(2), @miW % 60), 2) FROM @mTmpTable7 WHERE DeviceClass = 'Workstation'
			SELECT @mN = CONVERT(varchar(6), @miN/3600)
							+ ':' + RIGHT('0' + CONVERT(varchar(2), (@miN % 3600) / 60), 2)
							+ ':' + RIGHT('0' + CONVERT(varchar(2), @miN % 60), 2) FROM @mTmpTable7 WHERE DeviceClass = 'Non-Workstation'
			INSERT INTO @mTmpReturnTable VALUES ('Durchschnittliche Zeit bis zur Anerkennung geschlossener Vorfälle', 
					CASE WHEN @mW IS NULL THEN '0' ELSE @mW END, 
					CASE WHEN @mN IS NULL THEN '0' ELSE @mN END)

			SELECT @miW = [Average Time to Resolution] FROM @mTmpTable7 WHERE DeviceClass = 'Workstation'
			SELECT @miN = [Average Time to Resolution] FROM @mTmpTable7 WHERE DeviceClass = 'Non-Workstation'
			SELECT @mW = CONVERT(varchar(6), @miW/3600)
							+ ':' + RIGHT('0' + CONVERT(varchar(2), (@miW % 3600) / 60), 2)
							+ ':' + RIGHT('0' + CONVERT(varchar(2), @miW % 60), 2) FROM @mTmpTable7 WHERE DeviceClass = 'Workstation'
			SELECT @mN = CONVERT(varchar(6), @miN/3600)
							+ ':' + RIGHT('0' + CONVERT(varchar(2), (@miN % 3600) / 60), 2)
							+ ':' + RIGHT('0' + CONVERT(varchar(2), @miN % 60), 2) FROM @mTmpTable7 WHERE DeviceClass = 'Non-Workstation'
			INSERT INTO @mTmpReturnTable VALUES ('Durchschnittliche Zeit bis zur Lösung geschlossener Vorfälle', 
					CASE WHEN @mW IS NULL THEN '0' ELSE @mW END, 
					CASE WHEN @mN IS NULL THEN '0' ELSE @mN END)


			IF (@IncludeProgramLevel = 1 AND @CustMTTA IS NOT NULL AND @CustMTTR IS NOT NULL AND @CustMTTA &gt;= 0 AND @CustMTTR &gt;= 0)
			BEGIN
				SELECT @mW = [Number of Incidents Exceeding MTTA] FROM @mTmpTable7 WHERE DeviceClass = 'Workstation'
				SELECT @mN = [Number of Incidents Exceeding MTTA] FROM @mTmpTable7 WHERE DeviceClass = 'Non-Workstation'
				INSERT INTO @mTmpReturnTable VALUES ('Number of Closed Incidents Exceeding MTTA', 
						CASE WHEN @mW IS NULL THEN '0' ELSE @mW END, 
						CASE WHEN @mN IS NULL THEN '0' ELSE @mN END)
			
				SELECT @mW = [Number of Incidents Exceeding MTTR] FROM @mTmpTable7 WHERE DeviceClass = 'Workstation'
				SELECT @mN = [Number of Incidents Exceeding MTTR] FROM @mTmpTable7 WHERE DeviceClass = 'Non-Workstation'
				INSERT INTO @mTmpReturnTable VALUES ('Number of Closed Incidents Exceeding MTTR', 
						CASE WHEN @mW IS NULL THEN '0' ELSE @mW END, 
						CASE WHEN @mN IS NULL THEN '0' ELSE @mN END)
			END
			
			-- Return our results
			SELECT * FROM @mTmpReturnTable

			-- cleanup
			DROP TABLE #mTmpTableIncidentsWithDeviceClass
			DROP TABLE #mTmpTableSingleService
		END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Detail">
          <DataField>Detail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Workstation">
          <DataField>Workstation</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Other_Devices_Classes">
          <DataField>Other Devices Classes</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
