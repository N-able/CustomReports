    <DataSet Name="dsTicketScore">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Delta">
            <Value>=iif(Parameters!TicketGracePeriod.Value &lt;0, 0, Parameters!TicketGracePeriod.Value)</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PeriodType">
            <Value>=Parameters!PeriodType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Customer_ID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TicketQueue">
            <Value>=Parameters!TicketQueue.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TicketPriority">
            <Value>=Parameters!TicketPriority.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FilterByQueue">
            <Value>=Parameters!FilterByQueue.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FilterByPriority">
            <Value>=Parameters!FilterByPriority.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @PreviousStartDate datetime
DECLARE @PreviousEndDate datetime

IF dbo.GetPreviousDate(@StartDate, @PeriodType) = @StartDate
	
BEGIN
		SET @PreviousStartDate = DATEADD(day, -(DATEDIFF(day, @StartDate, @EndDate)), DATEADD(day, -1, @StartDate))
		SET @PreviousEndDate = DATEADD(day, -1, @StartDate)
	
END
ELSE
	
BEGIN
		
SET @PreviousStartDate = dbo.GetPreviousDate(@StartDate, @PeriodType)
		
SET @PreviousEndDate = dbo.GetPreviousDate(@EndDate, @PeriodType)
	
END

Select 
	CAST(DateAdd(day, -(@Delta), @StartDate) as DATE) as 'StartDate'
	,CAST(DateAdd(day,-(@Delta), @EndDate) as DATE) as 'EndDate'
	,CASE WHEN (isnull(SUM(case when CAST(psa.TimeOpened as DATE) 
										between CAST(DateAdd(day, -(@Delta), @StartDate) as DATE) 
											AND CAST(DateAdd(day, -(@Delta), @EndDate) as DATE) 
									then 1 else 0 end), 0)) = 0 THEN -1
		ELSE
	isnull(ROUND((cast(sum(CASE WHEN CAST(psa.TimeClosed as DATE) 
									between CAST(DateAdd(day, -(@Delta), @StartDate) as DATE) 
										AND CAST(@EndDate as DATE) 
									AND PSA.TicketResolved = 1 
									AND CAST(psa.TimeOpened as DATE) 
										between CAST(DateAdd(day, -(@Delta), @StartDate) as DATE) 
											AND CAST(DateAdd(day, -(@Delta), @EndDate) as DATE)
								THEN 1 ELSE 0 END) as float)
								/
								cast(SUM(case when CAST(psa.TimeOpened as DATE) 
										between CAST(DateAdd(day, -(@Delta), @StartDate) as DATE) 
											AND CAST(DateAdd(day, -(@Delta), @EndDate) as DATE) 
									then 1 else 0 end) as float)) * 100, 2), 0) END as 'Score'
	,CASE WHEN (isnull(SUM(case when CAST(psa.TimeOpened as DATE) 
										between CAST(DateAdd(day, -(@Delta), @PreviousStartDate) as DATE) 
											AND CAST(DateAdd(day, -(@Delta), @PreviousEndDate) as DATE) 
										then 1 else 0 end), 0)) = 0 THEN -1
		ELSE
	isnull(ROUND((cast(sum(CASE WHEN CAST(psa.TimeClosed as DATE) 
										between CAST(DateAdd(day, -(@Delta), @PreviousStartDate) as DATE) 
											AND CAST(@PreviousEndDate as DATE) 
										AND PSA.TicketResolved = 1 
										AND CAST(psa.TimeOpened as DATE) 
										between CAST(DateAdd(day, -(@Delta), @PreviousStartDate) as DATE) 
											AND CAST(DateAdd(day, -(@Delta), @PreviousEndDate) as DATE)
								THEN 1 ELSE 0 END) as float)
								/
								cast(SUM(case when CAST(psa.TimeOpened as DATE) 
										between CAST(DateAdd(day, -(@Delta), @PreviousStartDate) as DATE) 
											AND CAST(DateAdd(day, -(@Delta), @PreviousEndDate) as DATE) 
										then 1 else 0 end) as float)) * 100, 2), 0) END as 'PreviousScore'
	,@Delta as 'SLAChange'
from fact_Ticket PSA		
WHERE 
	CAST(psa.TimeOpened as DATE) between CAST(DateAdd(day, -(@Delta), @PreviousStartDate) as DATE) AND CAST(DateAdd(day, -(@Delta - 1), @EndDate) as DATE)
	AND PSA.CustomerID in (@Customer_ID)
	AND ((PSA.TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
	AND ((PSA.TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="EndDate">
          <DataField>EndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="SLAChange">
          <DataField>SLAChange</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Score">
          <DataField>Score</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="PreviousScore">
          <DataField>PreviousScore</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsTicketMetrics">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Customer_ID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TicketQueue">
            <Value>=Parameters!TicketQueue.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TicketPriority">
            <Value>=Parameters!TicketPriority.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FilterByQueue">
            <Value>=Parameters!FilterByQueue.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FilterByPriority">
            <Value>=Parameters!FilterByPriority.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>If (
(SELECT COUNT(DISTINCT TicketPriority)
FROM fact_Ticket psa
WHERE CAST(psa.TimeOpened as DATE) &lt; CAST(@EndDate as DATE)
	AND (CAST(psa.TimeClosed as DATE) &gt; CAST(@StartDate as DATE)
		OR psa.TimeClosed is null)
	AND psa.CustomerID in (@Customer_ID)
	AND ((PSA.TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
	AND ((PSA.TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')) &gt;4
)	
BEGIN
DECLARE @Priority1 as nvarchar(50)
DECLARE @Priority2 as nvarchar(50)
DECLARE @Priority3 as nvarchar(50)

SELECT Top 1 @Priority1 = TicketPriority 
FROM fact_Ticket 
WHERE ((TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
	AND ((TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')
GROUP BY TicketPriority 
ORDER BY COUNT(*) desc

SELECT Top 1 @Priority2 = TicketPriority 
FROM fact_Ticket 
WHERE TicketPriority &lt;&gt; @Priority1
	AND ((TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
	AND ((TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')
GROUP BY TicketPriority 
ORDER BY COUNT(*) desc

SELECT Top 1 @Priority3 = TicketPriority 
FROM fact_Ticket 
WHERE TicketPriority not in (@Priority1, @Priority2)
	AND ((TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
	AND ((TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')
GROUP BY TicketPriority 
ORDER BY COUNT(*) desc

Select 
	CASE WHEN psa.Issue is null OR psa.Issue = '' THEN 'Nicht spezifiziert' ELSE psa.Issue END as 'Issue'
	,CASE WHEN psa.SubIssue is null OR psa.SubIssue = '' THEN 'Nicht spezifiziert' ELSE psa.SubIssue END as 'SubIssue' 
	,Case WHEN TicketPriority not in (@Priority1, @Priority2, @Priority3) Then 'Other'
		ELSE TicketPriority END as TicketPriority
	,COUNT(*) as 'Count'
from fact_Ticket PSA		
WHERE CAST(psa.TimeClosed as DATE) between CAST(@StartDate as DATE) AND CAST(@EndDate as DATE)
	AND PSA.CustomerID in (@Customer_ID)
	AND ((PSA.TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
	AND ((PSA.TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')
GROUP BY psa.Issue, psa.Subissue, Case WHEN TicketPriority not in (@Priority1, @Priority2, @Priority3) Then 'Other'
		ELSE TicketPriority END

END
ELSE
BEGIN
Select 
	CASE WHEN psa.Issue is null OR psa.Issue = '' THEN 'Nicht spezifiziert' ELSE psa.Issue END as 'Issue'
	,CASE WHEN psa.SubIssue is null OR psa.SubIssue = '' THEN 'Nicht spezifiziert' ELSE psa.SubIssue END as 'SubIssue' 
	,TicketPriority
	,COUNT(*) as 'Count'
from fact_Ticket PSA		
WHERE CAST(psa.TimeClosed as DATE) between CAST(@StartDate as DATE) AND CAST(@EndDate as DATE)
	AND PSA.CustomerID in (@Customer_ID)
	AND ((PSA.TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
	AND ((PSA.TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')
GROUP BY psa.Issue, psa.Subissue, TicketPriority
END</CommandText>
