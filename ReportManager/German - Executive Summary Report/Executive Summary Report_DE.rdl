        </Field>
        <Field Name="Failed_Backups">
          <DataField>Failed_Backups</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Last_Successful_Backup">
          <DataField>Last_Successful_Backup</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Size_Of_Last_Backup">
          <DataField>Size_Of_Last_Backup</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
