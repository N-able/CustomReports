    <ReportParameter Name="OverallScoreNumber">
      <DataType>Float</DataType>
      <DefaultValue>
        <Values>
          <Value>=IIf(
	(Parameters!ServerPerformanceScore.Value &lt; 0 OR Parameters!TicketScore.Value &lt; 0)
	AND (	ABS(CInt(Parameters!IncludeManagedDevices.Value))
			* Parameters!ManagedDevicesWeight.Value 
		+ 
			ABS(CInt(Parameters!IncludeSecurityMonitoring.Value))
			* Parameters!SecurityMonitoringWeight.Value 
		+
			ABS(CInt(Parameters!IncludeApplicationLicenseCompliance.Value))
			* Parameters!ApplicationWeight.Value 
		+
			ABS(CInt(Parameters!IncludeDataProtection.Value))
			* Parameters!DataProtectionWeight.Value 
		+ 
			ABS(CInt(Parameters!IncludeNetworkReliability.Value))
			* Parameters!NetworkReliabilityWeight.Value 
		+
			iif(Parameters!ServerPerformanceScore.Value &lt; 0, 0,
				ABS(CInt(Parameters!IncludeServerPerformance.Value))
				* Parameters!ServerPerformanceWeight.Value 
			)
		+
			iif(Parameters!TicketScore.Value = -1, 0, 
				ABS(CInt(Parameters!IncludeTicketSummary.Value))
				* Parameters!TicketSummaryWeight.Value 
			)
		+ 
			ABS(CInt(Parameters!IncludeWarrantyStatus.Value))
			* Parameters!WarrantyStatusWeight.Value
	= 0),
	-1,
	IIf(
			ABS(CInt(Parameters!IncludeManagedDevices.Value))
			* Parameters!ManagedDevicesWeight.Value 
		+
			ABS(CInt(Parameters!IncludeSecurityMonitoring.Value))
			* Parameters!SecurityMonitoringWeight.Value 
		+
			ABS(CInt(Parameters!IncludeApplicationLicenseCompliance.Value))
			* Parameters!ApplicationWeight.Value 
		+
			ABS(CInt(Parameters!IncludeDataProtection.Value))
			* Parameters!DataProtectionWeight.Value 
		+ 
			ABS(CInt(Parameters!IncludeNetworkReliability.Value))
			* Parameters!NetworkReliabilityWeight.Value 
		+ 	
			ABS(CInt(Parameters!IncludeServerPerformance.Value))
			* Parameters!ServerPerformanceWeight.Value 
		+ 
			iif(Parameters!TicketScore.Value = -1, 0, 
				ABS(CInt(Parameters!IncludeTicketSummary.Value))
				* Parameters!TicketSummaryWeight.Value 
			)
		+
			ABS(CInt(Parameters!IncludeWarrantyStatus.Value))
			* Parameters!WarrantyStatusWeight.Value 
		= 0, 
		0,
		Round(
				(
					ABS(CInt(Parameters!IncludeManagedDevices.Value))
					* Parameters!ManagedDevicesScore.Value
					* Parameters!ManagedDevicesWeight.Value 
				+ 
					ABS(CInt(Parameters!IncludeSecurityMonitoring.Value))
					* Parameters!SecurityMonitoringScore.Value
					* Parameters!SecurityMonitoringWeight.Value 
				+
					ABS(CInt(Parameters!IncludeApplicationLicenseCompliance.Value))
					* Parameters!AandLComplianceScore.Value
					* Parameters!ApplicationWeight.Value 
				+
					ABS(CInt(Parameters!IncludeDataProtection.Value))
					* Parameters!DataProtectionScore.Value
					* Parameters!DataProtectionWeight.Value 
				+
					ABS(CInt(Parameters!IncludeNetworkReliability.Value))
					* Parameters!NetworkReliabilityScore.Value
					* Parameters!NetworkReliabilityWeight.Value 
				+
					ABS(CInt(Parameters!IncludeWarrantyStatus.Value))
					* Parameters!WarrantyStatusScore.Value
					* Parameters!WarrantyStatusWeight.Value 
				+
					iif(Parameters!TicketScore.Value = -1, 0, 
						ABS(CInt(Parameters!IncludeTicketSummary.Value))
						* Parameters!TicketScore.Value
						* Parameters!TicketSummaryWeight.Value 
					)
				+
					ABS(CInt(Parameters!IncludeServerPerformance.Value))
					* Parameters!ServerPerformanceScore.Value
					* IIf(
						Parameters!ServerPerformanceScore.Value&lt;0, 
						0, 
						Parameters!ServerPerformanceWeight.Value
						)
				)/
				(
					ABS(CInt(Parameters!IncludeManagedDevices.Value))
					* Parameters!ManagedDevicesWeight.Value 
				+
					ABS(CInt(Parameters!IncludeSecurityMonitoring.Value))
					* Parameters!SecurityMonitoringWeight.Value 
				+ 
					ABS(CInt(Parameters!IncludeApplicationLicenseCompliance.Value))
					* Parameters!ApplicationWeight.Value 
				+
					ABS(CInt(Parameters!IncludeDataProtection.Value))
					* Parameters!DataProtectionWeight.Value 
				+ 
					ABS(CInt(Parameters!IncludeNetworkReliability.Value))
					* Parameters!NetworkReliabilityWeight.Value 
				+ 
					iif(Parameters!TicketScore.Value = -1, 0, 
						ABS(CInt(Parameters!IncludeTicketSummary.Value))
						* Parameters!TicketSummaryWeight.Value 
					)
				+
					ABS(CInt(Parameters!IncludeWarrantyStatus.Value))
					* Parameters!WarrantyStatusWeight.Value
				+ 
					ABS(CInt(Parameters!IncludeServerPerformance.Value))
					* IIf(
						Parameters!ServerPerformanceScore.Value&lt;0,
						0,
						Parameters!ServerPerformanceWeight.Value
					)
				
				)
			)
		)
	)</Value>
        </Values>
      </DefaultValue>
      <Hidden>true</Hidden>
    </ReportParameter>
    <ReportParameter Name="OverallScoreValue">
      <DataType>String</DataType>
      <DefaultValue>
        <Values>
          <Value>=IIf(IsNumeric(Parameters!OverallScoreNumber.Value)=True,
IIf(Parameters!IncludeLetterGrade.Value=True,
IIf(Parameters!OverallScoreNumber.Value&gt;=90,"A+",
IIf(Parameters!OverallScoreNumber.Value&gt;=85,"A",
IIf(Parameters!OverallScoreNumber.Value&gt;=80,"A-",
IIf(Parameters!OverallScoreNumber.Value&gt;=75,"B+",
IIf(Parameters!OverallScoreNumber.Value&gt;=70,"B",
IIf(Parameters!OverallScoreNumber.Value&gt;=65,"B-",
IIf(Parameters!OverallScoreNumber.Value&gt;=60,"C",
IIf(Parameters!OverallScoreNumber.Value&gt;=50,"D",
IIf(Parameters!OverallScoreNumber.Value&gt;=0,"F","N/A"))))))))),
IIf(Parameters!OverallScoreNumber.Value&gt;=0,CStr(Fix(Parameters!OverallScoreNumber.Value + 0.5)),"N/A")),
"Unknown")</Value>
        </Values>
      </DefaultValue>
      <Hidden>true</Hidden>
    </ReportParameter>
