    <DataSet Name="dsOpenedClosed">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Customer_ID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TicketQueue">
            <Value>=Parameters!TicketQueue.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TicketPriority">
            <Value>=Parameters!TicketPriority.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FilterByQueue">
            <Value>=Parameters!FilterByQueue.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FilterByPriority">
            <Value>=Parameters!FilterByPriority.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>If ( 
SELECT COUNT(*) 
		FROM fact_Ticket 
		WHERE (TimeOpened Between @StartDate AND @EndDate 
				OR TimeClosed Between @StartDate AND @EndDate) 
			AND CustomerID = @Customer_ID
			AND ((TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
			AND ((TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')

) &gt; 0
BEGIN

Select 
	CAST(dim_date.[Date] as DATE) AS [Date]
	,'Geschlossen' as [Status]
	,SUM(CASE WHEN PSA.TimeClosed is not null THEN 1
		ELSE 0 END) 
		as HITS
from dim_date 
	LEFT JOIN
		fact_Ticket PSA
			on CAST(PSA.TimeClosed as DATE) = CAST(dim_date.[Date] as DATE)
			AND PSA.CustomerID in (@Customer_ID)
			AND ((PSA.TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
			AND ((PSA.TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')
WHERE CAST(dim_date.[Date] as DATE) BETWEEN CAST(@StartDate as DATE)
		AND CAST(@EndDate as DATE)
GROUP BY CAST(dim_date.[Date] as DATE), CAST(PSA.TimeClosed as DATE)

UNION ALL

Select 
	CAST(dim_date.[Date] as DATE) AS [Date]
	,'Geöffnet' as [Status]
	,SUM(CASE WHEN PSA.TimeOpened is not null THEN 1
		ELSE 0 END) 
		as HITS
from dim_date 
	LEFT JOIN
		fact_Ticket PSA
			on CAST(PSA.TimeOpened as DATE) = CAST(dim_date.[Date] as DATE)
			AND PSA.CustomerID in (@Customer_ID)
			AND ((PSA.TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
			AND ((PSA.TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')
WHERE CAST(dim_date.[Date] as DATE) BETWEEN CAST(@StartDate as DATE)
		AND CAST(@EndDate as DATE)
GROUP BY CAST(dim_date.[Date] as DATE), CAST(PSA.TimeOpened as DATE)

UNION ALL


Select 
	CAST([Date] as DATE) as [Date] 
	,'Verbleibende offene Tickets' as [Status]
	,COUNT(ticketID) as Hits
from dim_date 
	LEFT JOIN
		fact_Ticket Ticket
			on CAST(Ticket.TimeOpened as DATE) &lt;= CAST([Date] as DATE)
			AND (Ticket.TicketResolved = 0 OR CAST(Ticket.TimeClosed as DATE) &gt; CAST([Date] as DATE))
			AND ((Ticket.TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
			AND ((Ticket.TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')
			AND Ticket.CustomerID in (@Customer_ID)
WHERE CAST(dim_date.[Date] as DATE) BETWEEN CAST(@StartDate as DATE)
		AND CAST(@EndDate as DATE)
GROUP BY CAST([Date] as DATE)
END

ELSE
BEGIN
Select 
	Ticket.TimeOpened as [Date] 
	,Ticket.ticketid as [Status]
	,Ticket.TicketID as Hits
FROM fact_Ticket Ticket
WHERE (TimeOpened Between @StartDate AND @EndDate 
		OR TimeClosed Between @StartDate AND @EndDate) 
	AND CustomerID = @Customer_ID
	AND ((TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
	AND ((TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')
END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
