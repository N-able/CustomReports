    <DataSet Name="RPTES_SecurityMonitoring_PatchMissing">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!PatchMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT 
				NULL AS [Number of Monitored Devices Missing Patches], 
				NULL AS [Total Number of Unapplied Patches On Monitored Devices] 
			WHERE 1=2
		END
	ELSE
	BEGIN
		-- store our results
		CREATE TABLE #tmpTable  (
			[DeviceClass] 				nvarchar(1000),
			[DeviceName] 				nvarchar(1000),
			[URI] 						nvarchar(1000), 
			[Number Of Missing Patches] int
		)
---------------		
			-- Get the start and end dates for the report
		DECLARE @End_Date datetime
		SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

		-- Create the date variables for reuse in the database filter.
		DECLARE @End_DateIndex int
		SELECT @End_DateIndex = dbo.dateIndex(@End_Date)

		IF (OBJECT_ID('tempdb..#tmp_Customer') IS NOT NULL)
			DROP TABLE #tmp_Customer

		IF (OBJECT_ID('tempdb..#tmp_PatchUnappliedDevice') IS NOT NULL)
			DROP TABLE #tmp_PatchUnappliedDevice

		IF (OBJECT_ID('tempdb..#tmp_tblDevices') IS NOT NULL)
			DROP TABLE #tmp_tblDevices

		IF (OBJECT_ID('tempdb..#tmp_mRecDevicePatch') IS NOT NULL)
			DROP TABLE #tmp_mRecDevicePatch

		CREATE TABLE #tmp_Customer (DataSourceID int, CustomerID int)
		CREATE TABLE #tmp_PatchUnappliedDevice (DeviceID int)
		
		-- Create a list of N-central CustomerID based on a list of dim_CustomerIDs
		INSERT INTO #tmp_Customer 
		SELECT [DataSourceID], [N-central CustomerID] CustomerID FROM dim_Customer 
		WHERE CustomerID IN (SELECT Value FROM inline_split_me(@CustomerID))

		-- Check if showing deleted devices
		DECLARE @ShowDeleted nvarchar(30) 	 
		SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')

		-- temp table for devices	
		CREATE TABLE #tmp_tblDevices 
			(
			DeviceID int, 
			ServiceID int, 
			MaxDateID int, 
			MaxTimeID int
			) 
		
		-- get the serviceid for MBSA 1.2.1
		DECLARE @MBSA1ServiceID INTEGER
		SELECT @MBSA1ServiceID = ServiceID 
		FROM DIM_Service 
		WHERE [N-central ServiceID] = 142 AND DataSourceID IN (SELECT DISTINCT DataSourceID FROM #tmp_Customer)

		-- get the serviceid for MBSA 2.0
		DECLARE @MBSA2ServiceID INTEGER
		SELECT @MBSA2ServiceID = ServiceID 
		FROM DIM_Service 
		WHERE [N-central ServiceID] = 149 AND DataSourceID IN (SELECT DISTINCT DataSourceID FROM #tmp_Customer)

		-- get the serviceid for WSUS
		DECLARE @WSUSServiceID INTEGER
		SELECT @WSUSServiceID = ServiceID 
		FROM DIM_Service 
		WHERE [N-central ServiceID] = 218 AND DataSourceID IN (SELECT DISTINCT DataSourceID FROM #tmp_Customer)
		
		-- get the serviceid for Patch Status -- bug # 1165
		DECLARE @PatchStatusServiceID INTEGER
		SET @PatchStatusServiceID = (SELECT TOP 1 ServiceID 
		FROM DIM_Service 
		WHERE [N-central ServiceID] = 219 AND DataSourceID IN (SELECT DISTINCT DataSourceID FROM #tmp_Customer))


		-- order of importance, in case the device has all 3 services installed, and we only care of the last scan of the day
		-- 1) Patch Status (bug # 1165)
		-- 2) Patch Management (WSUS)
		-- 3) MBSA 2.0
		-- 4) MBSA 1.2.1 or Patch Level
		----------------------------------
		CREATE TABLE #tmp_mRecDevicePatch  (
			DeviceID					int,
			ServiceID					int,
			[Number Of Missing Patches] int
		)
		
		-- Bug 42758 - code changed to improve performance
		----------- 1. Patch Status processing -------------
		INSERT INTO #tmp_tblDevices 
				(
				DeviceID, 
				ServiceID, 
				MaxDateID, 
				MaxTimeID
				)	
		SELECT  
				[DeviceID]
				,ServiceID
				--,MAX(CAST(DateID AS numeric)*10000+TimeID) as MaxDateTime
				,CAST(MAX(CAST(DateID AS numeric)*10000+TimeID)/10000  AS int) as MaxDateID
				,MAX(CAST(DateID AS numeric)*10000+TimeID)-CAST(CAST(MAX(CAST(DateID AS numeric)*10000+TimeID)/10000  AS int) AS numeric)*10000 as MaxTimeID
		FROM [warehouse].[dbo].[fact_SEM_Patch Level Count]
		WHERE 
				CustomerID=@CustomerID
			AND ServiceID=@PatchStatusServiceID
			--AND ServiceID IN (@PatchStatusServiceID,@WSUSServiceID,@MBSA1ServiceID,@MBSA2ServiceID)
			AND DateID &lt;= @End_DateIndex
		GROUP BY DeviceID, ServiceID
	  
		INSERT INTO #tmp_mRecDevicePatch
				(
				DeviceID, 
				ServiceID,
				[Number Of Missing Patches]
				)
		SELECT 
				a.DeviceID,
				a.ServiceID,
				a.[Number of Missing Patches]
		FROM 
			dbo.[fact_SEM_Patch Level Count] a
				INNER JOIN
			#tmp_tblDevices b
					ON a.DeviceID	= b.DeviceID
					AND a.ServiceID	= b.ServiceID
					AND a.DateID	= b.MaxDateID
					AND a.TimeID	= b.MaxTimeID
			
		----------- 2. WSUS processing -------------
		INSERT INTO #tmp_tblDevices 
				(
				DeviceID, 
				ServiceID, 
				MaxDateID, 
				MaxTimeID
				)	
		SELECT  
				a.[DeviceID]
				,a.ServiceID
				--,MAX(CAST(DateID AS numeric)*10000+TimeID) as MaxDateTime
				,CAST(MAX(CAST(a.DateID AS numeric)*10000+a.TimeID)/10000  AS int) as MaxDateID
				,MAX(CAST(a.DateID AS numeric)*10000+a.TimeID)-CAST(CAST(MAX(CAST(a.DateID AS numeric)*10000+a.TimeID)/10000  AS int) AS numeric)*10000 as MaxTimeID
		FROM 
			[warehouse].[dbo].[fact_SEM_Patch Level Count] a
				LEFT OUTER JOIN 
			#tmp_mRecDevicePatch b
					ON a.DeviceID=b.DeviceID
		WHERE 
			a.CustomerID = @CustomerID
			AND a.ServiceID = @WSUSServiceID
			AND DateID &lt;= @End_DateIndex
			AND b.DeviceID IS NULL
		GROUP BY a.DeviceID, a.ServiceID
	  
		INSERT INTO #tmp_mRecDevicePatch
				(
				DeviceID, 
				ServiceID,
				[Number Of Missing Patches]
				)
		SELECT 
				a.DeviceID,
				a.ServiceID,
				a.[Number of Missing Patches]
		FROM 
			dbo.[fact_SEM_Patch Level Count] a
				INNER JOIN
			#tmp_tblDevices b
					ON a.DeviceID	= b.DeviceID
					AND a.ServiceID	= b.ServiceID
					AND a.DateID	= b.MaxDateID
					AND a.TimeID	= b.MaxTimeID
			WHERE
				a.ServiceID=@WSUSServiceID
		----------- 3. MBSA 2.0 processing -------------
		INSERT INTO #tmp_tblDevices 
				(
				DeviceID, 
				ServiceID, 
				MaxDateID, 
				MaxTimeID
				)	
		SELECT  
				a.[DeviceID]
				,a.ServiceID
				--,MAX(CAST(DateID AS numeric)*10000+TimeID) as MaxDateTime
				,CAST(MAX(CAST(a.DateID AS numeric)*10000+a.TimeID)/10000  AS int) as MaxDateID
				,MAX(CAST(a.DateID AS numeric)*10000+a.TimeID)-CAST(CAST(MAX(CAST(a.DateID AS numeric)*10000+a.TimeID)/10000  AS int) AS numeric)*10000 as MaxTimeID
		FROM 
			[warehouse].[dbo].[fact_SEM_Patch Level Count] a
				LEFT OUTER JOIN 
			#tmp_mRecDevicePatch b
					ON a.DeviceID=b.DeviceID
		WHERE 
			a.CustomerID = @CustomerID
			AND a.ServiceID = @MBSA2ServiceID
			AND DateID &lt;= @End_DateIndex
			AND b.DeviceID IS NULL
		GROUP BY a.DeviceID, a.ServiceID
	  
		INSERT INTO #tmp_mRecDevicePatch
				(
				DeviceID, 
				ServiceID,
				[Number Of Missing Patches]
				)
		SELECT 
				a.DeviceID,
				a.ServiceID,
				a.[Number of Missing Patches]
		FROM 
			dbo.[fact_SEM_Patch Level Count] a
				INNER JOIN
			#tmp_tblDevices b
					ON a.DeviceID	= b.DeviceID
					AND a.ServiceID	= b.ServiceID
					AND a.DateID	= b.MaxDateID
					AND a.TimeID	= b.MaxTimeID
			WHERE
				a.ServiceID=@MBSA2ServiceID
		----------- 4. MBSA 1.2.1 processing -------------
		INSERT INTO #tmp_tblDevices 
				(
				DeviceID, 
				ServiceID, 
				MaxDateID, 
				MaxTimeID
				)	
		SELECT  
				a.[DeviceID]
				,a.ServiceID
				--,MAX(CAST(DateID AS numeric)*10000+TimeID) as MaxDateTime
				,CAST(MAX(CAST(a.DateID AS numeric)*10000+a.TimeID)/10000  AS int) as MaxDateID
				,MAX(CAST(a.DateID AS numeric)*10000+a.TimeID)-CAST(CAST(MAX(CAST(a.DateID AS numeric)*10000+a.TimeID)/10000  AS int) AS numeric)*10000 as MaxTimeID
		FROM 
			[warehouse].[dbo].[fact_SEM_Patch Level Count] a
				LEFT OUTER JOIN 
			#tmp_mRecDevicePatch b
					ON a.DeviceID=b.DeviceID
		WHERE 
			a.CustomerID = @CustomerID
			AND a.ServiceID = @MBSA1ServiceID
			AND b.DeviceID IS NULL
			AND DateID &lt;= @End_DateIndex
		GROUP BY a.DeviceID, a.ServiceID
	  
		INSERT INTO #tmp_mRecDevicePatch
				(
				DeviceID, 
				ServiceID,
				[Number Of Missing Patches]
				)
		SELECT 
				a.DeviceID,
				a.ServiceID,
				a.[Number of Missing Patches]
		FROM 
			dbo.[fact_SEM_Patch Level Count] a
				INNER JOIN
			#tmp_tblDevices b
					ON a.DeviceID	= b.DeviceID
					AND a.ServiceID	= b.ServiceID
					AND a.DateID	= b.MaxDateID
					AND a.TimeID	= b.MaxTimeID
		WHERE
				a.ServiceID=@MBSA1ServiceID
	-----------------------------------
		-- now that we have gone through all the different patch services, get rid of the devices which have 0 missing patches
		DELETE FROM #tmp_mRecDevicePatch WHERE [Number Of Missing Patches] = 0

		INSERT INTO #tmpTable ([DeviceClass],[DeviceName],[URI],[Number Of Missing Patches])
		-- Return Patch Unapplied Devices
		SELECT d.[Device Class], d.DeviceName, [URI], p.[Number Of Missing Patches] 
		FROM #tmp_mRecDevicePatch p
		INNER JOIN dim_Device d ON d.DeviceID = p.DeviceID
		INNER JOIN #tmp_Customer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
		WHERE [Device Class] IN ('Windows Server', 'Windows Workstation','Windows Laptop') 
			AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
			AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
			AND (d.[Is Managed Device Indicator] = 'Managed Device')
			AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
		ORDER BY d.[Device Class], d.DeviceName	

		-- display results
		SELECT COUNT(*) AS [Number of Monitored Devices Missing Patches], 
			ISNULL(SUM([Number Of Missing Patches]),0) AS [Total Number of Unapplied Patches On Monitored Devices] 
		FROM #tmpTable

		-- cleanup
		DROP TABLE #tmpTable
	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Number_of_Monitored_Devices_Missing_Patches">
          <DataField>Number of Monitored Devices Missing Patches</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total_Number_of_Unapplied_Patches_On_Monitored_Devices">
          <DataField>Total Number of Unapplied Patches On Monitored Devices</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPTES_SecurityMonitoring_PatchSummary">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!PatchMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT 
				0 AS [Servers_Monitored_Percent], 0 AS [Servers_Monitored_Count], 0 AS [Servers_Unmonitored_Percent], 0 AS [Servers_Unmonitored_Count],
				0 AS [Workstations_Monitored_Percent], 0 AS [Workstations_Monitored_Count], 0 AS [Workstations_Unmonitored_Percent], 0 AS [Workstations_Unmonitored_Count]
			WHERE 1=2
		END
	ELSE
		BEGIN
			--
			CREATE TABLE #mserverTable  (
				[Servers_Monitored_Percent] float,
				[Servers_Monitored_Count] int,
				[Servers_Unmonitored_Percent] float, 
				[Servers_Unmonitored_Count] int
			)

			CREATE TABLE #mWorkStationTable  (
				[WorkStations_Monitored_Percent] float,
				[WorkStations_Monitored_Count] int,
				[WorkStations_Unmonitored_Percent] float, 
				[WorkStations_Unmonitored_Count] int
			)
			DECLARE @End_Date datetime
			-- Get the start and end dates for the report
			SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)
			-- Create the date variables for reuse in the database filter.
			DECLARE @End_DateIndex int
			SELECT @End_DateIndex = dbo.dateIndex(@End_Date)
			DECLARE @mCustomer TABLE (DataSourceID int, CustomerID int)
			-- Patch Status support added 
			DECLARE @PatchServiceNames TABLE (ServiceName nvarchar(100))
			INSERT INTO @PatchServiceNames (ServiceName) VALUES ('Patch Level')
			INSERT INTO @PatchServiceNames (ServiceName) VALUES ('MBSA 1.2.1')
			INSERT INTO @PatchServiceNames (ServiceName) VALUES ('MBSA 2.0')
			INSERT INTO @PatchServiceNames (ServiceName) VALUES ('Patch Management')
			INSERT INTO @PatchServiceNames (ServiceName) VALUES ('Patch Status')
			-- Create a list of N-central CustomerID based on a list of dim_CustomerIDs
			INSERT INTO @mCustomer 
			SELECT [DataSourceID], [N-central CustomerID] CustomerID FROM dim_Customer 
			WHERE CustomerID IN (SELECT Value FROM inline_split_me(@CustomerID))

			-- Check if showing deleted devices
				DECLARE @ShowDeleted nvarchar(30) 	 
				SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')
			--
			DECLARE @TotalDevices int
			DECLARE @TotalMonitoredDevices int
			SET @TotalDevices = 0
			SET @TotalMonitoredDevices = 0
			-- Workstations and laptops
				BEGIN
					-- Get Total Windows Devices Count
					SELECT @TotalDevices = COUNT(DISTINCT d.DeviceID)
					FROM dim_Device d
					INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
					WHERE (CAST( FLOOR( CAST( CreatedOn AS float ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS float ) ) AS DATETIME )) OR DeletedOn IS NULL)
						AND [Device Class] IN ('Windows Workstation', 'Windows Laptop') 
						AND [Is Managed Device Indicator] = 'Managed Device'
						AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
						AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 		
						AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
					-- Get Total Windows Devices Count
					SELECT @TotalDevices = COUNT(DISTINCT d.DeviceID)
					FROM dim_Device d
					INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
					WHERE (CAST( FLOOR( CAST( CreatedOn AS float ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS float ) ) AS DATETIME )) OR DeletedOn IS NULL)
						AND [Device Class] IN ('Windows Workstation', 'Windows Laptop') 
						AND [Is Managed Device Indicator] = 'Managed Device'
						AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
						AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 		
						AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
					
					-- Get Total Monitored Devices Count
					SELECT @TotalMonitoredDevices = COUNT(DISTINCT d.DeviceID)
					FROM dim_Device d
					INNER JOIN [fact_Device Services] ON d.DeviceID = [fact_Device Services].DeviceID 
					INNER JOIN [dim_Service] ON [fact_Device Services].ServiceID = dim_Service.ServiceID
					INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
					WHERE  [ServiceName] IN (SELECT ServiceName FROM @PatchServiceNames) 
						AND [Device Class] IN ('Windows Workstation', 'Windows Laptop') 
						AND [Is Managed Device Indicator] = 'Managed Device'
						AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
						AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
						AND [Date Monitoring Started On] &lt;= @End_Date
						AND [Date Monitoring Stopped On] &gt; @End_Date
						AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
						
					IF @TotalDevices = 0
						INSERT INTO #mWorkStationTable
						SELECT 0 AS [Devices Monitored Percent], 0 AS [Devices Monitored Count], 0 AS [Devices Unmonitored Percent], 0 AS [Devices Unmonitored Count]
					ELSE
						INSERT INTO #mWorkStationTable
						SELECT CAST(@TotalMonitoredDevices AS float)/CAST(@TotalDevices AS float)*100 AS [Devices Monitored Percent], 
							@TotalMonitoredDevices AS [Devices Monitored Count], 
							(CAST(@TotalDevices AS float)-CAST(@TotalMonitoredDevices AS float))/CAST(@TotalDevices AS float)*100 AS [Devices Unmonitored Percent], 
							(@TotalDevices-@TotalMonitoredDevices) AS [Devices Unmonitored Count]
				END
			
			SET @TotalDevices = 0
			SET @TotalMonitoredDevices = 0
				-- Windows Servers
				BEGIN
					-- Get Total Windows Devices Count
					SELECT @TotalDevices = COUNT(DISTINCT d.DeviceID)
					FROM dim_Device d
					INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
					WHERE (CAST( FLOOR( CAST( CreatedOn AS float ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS float ) ) AS DATETIME )) OR DeletedOn IS NULL)
						AND [Device Class] IN ('Windows Server') 
						AND [Is Managed Device Indicator] = 'Managed Device'
						AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
						AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 		
						AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
					-- Get Total Windows Devices Count
					SELECT @TotalDevices = COUNT(DISTINCT d.DeviceID)
					FROM dim_Device d
					INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
					WHERE (CAST( FLOOR( CAST( CreatedOn AS float ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS float ) ) AS DATETIME )) OR DeletedOn IS NULL)
						AND [Device Class] IN ('Windows Server') 
						AND [Is Managed Device Indicator] = 'Managed Device'
						AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
						AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 		
						AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
			
						-- Get Total Monitored Devices Count
						SELECT @TotalMonitoredDevices = COUNT(DISTINCT d.DeviceID)
						FROM dim_Device d
						INNER JOIN [fact_Device Services] ON d.DeviceID = [fact_Device Services].DeviceID 
						INNER JOIN [dim_Service] ON [fact_Device Services].ServiceID = dim_Service.ServiceID
						INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
						WHERE  [ServiceName] IN (SELECT ServiceName FROM @PatchServiceNames)
							AND [Device Class] IN ('Windows Server') 
							AND [Is Managed Device Indicator] = 'Managed Device'
							AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
							AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
							AND [Date Monitoring Started On] &lt;= @End_Date
							AND [Date Monitoring Stopped On] &gt; @End_Date
							AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
			
					IF @TotalDevices = 0
						INSERT INTO #mserverTable
						SELECT 0 AS [Devices Monitored Percent], 0 AS [Devices Monitored Count], 0 AS [Devices Unmonitored Percent], 0 AS [Devices Unmonitored Count]
					ELSE
						INSERT INTO #mserverTable
						SELECT CAST(@TotalMonitoredDevices AS float)/CAST(@TotalDevices AS float)*100 AS [Devices Monitored Percent], 
							@TotalMonitoredDevices AS [Devices Monitored Count], 
							(CAST(@TotalDevices AS float)-CAST(@TotalMonitoredDevices AS float))/CAST(@TotalDevices AS float)*100 AS [Devices Unmonitored Percent], 
							(@TotalDevices-@TotalMonitoredDevices) AS [Devices Unmonitored Count]
				END			
			
			SELECT s.*, w.* FROM #mserverTable s, #mWorkStationTable w

			DROP TABLE #mserverTable
			DROP TABLE #mWorkStationTable
		END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
