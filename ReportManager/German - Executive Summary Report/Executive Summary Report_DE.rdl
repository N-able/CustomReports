    <DataSet Name="RPTES_SecurityMonitoring_Score">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!CustomerID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ScoreOnly">
            <Value>0</Value>
          </QueryParameter>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeSecurityMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Score">
            <Value>=Parameters!Score.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FirewallMonitoring">
            <Value>=Parameters!FirewallMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AVMonitoring">
            <Value>=Parameters!AVMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PatchMonitoring">
            <Value>=Parameters!PatchMonitoring.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @IsManaged nvarchar(20), @IsIgnoredDevice nvarchar(30)
SET @IsManaged = 'Managed Device'
SET @IsIgnoredDevice = 'Not an Ignored Device'
  
  -- check to see if this section is enabled, if not, get out!
  IF (@SectionEnabled = 0)
    BEGIN
      SELECT 0 AS [monitoring of firewall enabled],
             0 AS [monitoring of antivirus on all computers],
             0 AS [monitoring of patch management enabled],
             @Score AS [score]
    END
  ELSE
	BEGIN
		  -- Get the start and end dates for the report
		  SELECT @EndDate = [dbo].Getenddate(@EndDate,Getdate(),@Subscription)
		  
		  -- Create the date variables for reuse in the database filter.
		  DECLARE  @EndDateIndex INT
		  
		  SELECT @EndDateIndex = [dbo].Dateindex(@EndDate)
		  
		  DECLARE  @mCustomer  TABLE(
									 datasourceid INT,
									 customerid   INT
									 )
		  
		  -- Create a list of N-central CustomerID based on a list of dim_CustomerIDs
		  INSERT INTO @mCustomer
		  SELECT [datasourceid],
				 [n-central customerid] customerid
		  FROM   dim_customer
		  WHERE  customerid IN (SELECT VALUE
								FROM   Inline_split_me (@CustomerID ))
		  

		  DECLARE  @TotalWindowsDevices INT
		  DECLARE  @TotalFirewallDevices INT
		  DECLARE  @TotalAntivirusDevices INT
		  DECLARE  @TotalPatchLevelDevices INT
		  
		  SET @TotalWindowsDevices = 0
		  SET @TotalFirewallDevices = 0
		  SET @TotalAntivirusDevices = 0
		  SET @TotalPatchLevelDevices = 0
		  
		  DECLARE  @ShowDeleted NVARCHAR(30)
		  
		  SELECT @ShowDeleted = (SELECT [value]
								 FROM   config.dbo.config
								 WHERE  [key] = 'ShowDeletedDevices')
		  
			declare @WindowsLogEventServiceID table ( WinIDs int)
			declare @LogAnalysisServiceID table ( LogIDs int)
			declare @LogAnalysisAppendServiceID table ( LogIDs int)
			
			/* Bug # 1165 -- Patch Status support added */
			DECLARE @PatchServiceNames TABLE (ServiceName nvarchar(100))
			INSERT INTO @PatchServiceNames (ServiceName) VALUES ('Patch Level')
			INSERT INTO @PatchServiceNames (ServiceName) VALUES ('MBSA 1.2.1')
			INSERT INTO @PatchServiceNames (ServiceName) VALUES ('MBSA 2.0')
			INSERT INTO @PatchServiceNames (ServiceName) VALUES ('Patch Management')
			INSERT INTO @PatchServiceNames (ServiceName) VALUES ('Patch Status')

			
			-- get all windows event log service ids in each datasource
			Insert into @WindowsLogEventServiceID 
			select serviceid
			from dbo.dim_Service
			where datasourceid in (select [DataSourceID] from @mCustomer )
			 and (ServiceName = 'Windows Event Log')

			-- get all log analysis service ids in each datasource 
			Insert into @LogAnalysisServiceID 
			select serviceid
			from dbo.dim_Service
			where datasourceid in (select [DataSourceID] from @mCustomer )
			 and (ServiceName = 'Log Analysis (Batch)')
		  
			-- get all log analysis (Appended) service ids in each datasource 
			Insert into @LogAnalysisAppendServiceID 
			select serviceid
			from dbo.dim_Service
			where datasourceid in (select [DataSourceID] from @mCustomer )
			 and (ServiceName = 'Log Analysis (Appended)')

		  -- Get Total Windows Devices Count
		  SELECT @TotalWindowsDevices = COUNT(DISTINCT d.deviceid)
		  FROM   dim_device d
				 INNER JOIN @mCustomer c
				   ON d.[n-central customerid] = c.customerid
					  AND d.[datasourceid] = c.[datasourceid]
		  WHERE  (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) &lt;= @EndDate AND (@EndDate &lt;= (CAST( FLOOR( CAST( DeletedOn AS FLOAT ) ) AS DATETIME )) OR DeletedOn IS NULL)
				 AND [device class] IN ('Windows Server','Windows Workstation','Windows Laptop')
				 AND [appliance type] NOT IN ('Workgroup - Windows','Network - Windows')
				 AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
					 OR @ShowDeleted &lt;&gt; 'False')
				 AND (d.[is managed device indicator] = @IsManaged)
				 AND (d.[Is Ignored Device Indicator] = @IsIgnoredDevice)

			--SELECT   @TotalWindowsDevices
		  -- Get Total Firewall Devices Count
		  SELECT @TotalFirewallDevices = COUNT(DISTINCT d.deviceid)
		  FROM   dim_device d
				 INNER JOIN [fact_device services]
				   ON d.deviceid = [fact_device services].deviceid
				 INNER JOIN [dim_service]
				   ON [fact_device services].serviceid = dim_service.serviceid
				 INNER JOIN @mCustomer c
				   ON d.[n-central customerid] = c.customerid
					  AND d.[datasourceid] = c.[datasourceid]
		  WHERE  [servicename] LIKE 'FW-%'
				 AND [servicename] NOT LIKE 'FW-Chk Point'
				 AND [appliance type] NOT IN ('Workgroup - Windows','Network - Windows')
				 AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
					 OR @ShowDeleted &lt;&gt; 'False')
				 AND [date monitoring started on] &lt;= @EndDate
				 AND (d.[is managed device indicator] = @IsManaged)
				 AND (d.[Is Ignored Device Indicator] = @IsIgnoredDevice)

			DECLARE @mAvDevices TABLE (Deviceid int) 	 
			--Get all custom antivirus devices monitored by windows event log services
			INSERT INTO @mAvDevices 
				SELECT DISTINCT dsp.DeviceID
				FROM 
					dbo.[fact_Device Service Parameters]  dsp
						INNER JOIN 
					dbo.dim_Device d 
							ON d.DeviceID=dsp.DeviceID
				WHERE 
						dsp.serviceid IN (SELECT WinIDs FROM @WindowsLogEventServiceID )
					AND (CAST( FLOOR( CAST( d.CreatedOn AS FLOAT )) AS DATETIME )) &lt;= @EndDate AND (@EndDate &lt;= (CAST( FLOOR( CAST( d.DeletedOn AS FLOAT )) AS DATETIME )) OR d.DeletedOn IS NULL)			
					AND dsp.CustomerID IN  (SELECT [Value] FROM inline_split_me(@CustomerID)) 		
					AND (dsp.[Parameter Name] = 'SourceIncludeFilter')
					AND (dsp.[Parameter Value] IN (SELECT [ParameterValue] FROM config.dbo.[AntivirusProducts] WHERE [ParameterName] = 'Event Source Include Filter' )) 
					AND dsp.DeviceID NOT IN (SELECT deviceid FROM @mAvDevices)

			--Get all custom antivirus devices monitored by log analysis(batch) services

			INSERT INTO @mAvDevices 
				SELECT DISTINCT dsp.DeviceID
				FROM 
					dbo.[fact_Device Service Parameters] dsp
						INNER JOIN 
					dbo.dim_Device d 
							ON d.DeviceID=dsp.DeviceID		
				WHERE 
						dsp.serviceid IN (SELECT LogIDs FROM @LogAnalysisServiceID )
					AND (CAST( FLOOR( CAST( d.CreatedOn AS FLOAT )) AS DATETIME )) &lt;= @EndDate AND (@EndDate &lt;= (CAST( FLOOR( CAST( d.DeletedOn AS FLOAT )) AS DATETIME )) OR d.DeletedOn IS NULL)			
					AND dsp.CustomerID IN  (SELECT [Value] FROM inline_split_me(@CustomerID)) 
					AND (dsp.[Parameter Name] = 'FileSpec') 
					AND (dsp.[Parameter Value] IN (SELECT [ParameterValue] FROM config.dbo.[AntivirusProducts] WHERE [ParameterName] = 'Log File Name and Path' ))
					AND dsp.DeviceID NOT IN (SELECT deviceid FROM @mAvDevices)

			--Get all custom antivirus devices monitored by log analysis(appended) services

			INSERT INTO @mAvDevices 
				SELECT DISTINCT dsp.DeviceID
				FROM 
					dbo.[fact_Device Service Parameters] dsp
						INNER JOIN 
					dbo.dim_Device d 
							ON d.DeviceID=dsp.DeviceID		
				WHERE 
						dsp.serviceid IN (SELECT LogIDs FROM @LogAnalysisAppendServiceID )
					AND (CAST( FLOOR( CAST( d.CreatedOn AS FLOAT )) AS DATETIME )) &lt;= @EndDate AND (@EndDate &lt;= (CAST( FLOOR( CAST( d.DeletedOn AS FLOAT )) AS DATETIME )) OR d.DeletedOn IS NULL)							
					AND dsp.CustomerID IN  (SELECT [Value] FROM inline_split_me(@CustomerID)) 
					AND (dsp.[Parameter Name] = 'FileSpec') 
					AND (dsp.[Parameter Value] IN (SELECT [ParameterValue] FROM config.dbo.[AntivirusProducts] WHERE [ParameterName] = 'Log File Name and Path' ))
					AND dsp.DeviceID NOT IN (SELECT deviceid FROM @mAvDevices)

			-- Get Total Antivirus Devices Count
			
			INSERT INTO @mAvDevices 
			SELECT DISTINCT d.DeviceID
			FROM dim_Device d
			INNER JOIN [fact_Device Services] ON d.DeviceID = [fact_Device Services].DeviceID 
			INNER JOIN [dim_Service] ON [fact_Device Services].ServiceID = dim_Service.ServiceID
			INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
			WHERE (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) &lt;= @EndDate AND (@EndDate &lt;= (CAST( FLOOR( CAST( DeletedOn AS FLOAT ) ) AS DATETIME )) OR DeletedOn IS NULL)
				AND [ServiceName] LIKE 'AV %' 
				AND [Device Class] IN ('Windows Server', 'Windows Workstation','Windows Laptop') 
				AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
				AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
					 OR @ShowDeleted &lt;&gt; 'False')
				AND [Date Monitoring Started On] &lt;= @EndDate
				AND [Date Monitoring Stopped On] &gt; @EndDate 
				AND [Is Managed Device Indicator] = 'Managed Device'
				AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'
				and d.DeviceID not in (select deviceid from @mAvDevices)		


				
			INSERT INTO @mAvDevices 
			SELECT DISTINCT d.DeviceID
			FROM dim_Device d
			INNER JOIN [fact_Device Services] ON d.DeviceID = [fact_Device Services].DeviceID 
			INNER JOIN [dim_Service] ON [fact_Device Services].ServiceID = dim_Service.ServiceID
			INNER JOIN	[fact_Device Service Parameters] ON [fact_device Service Parameters].Serviceid = [fact_Device Services].ServiceID 
			INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
			WHERE (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) &lt;= @EndDate AND (@EndDate &lt;= (CAST( FLOOR( CAST( DeletedOn AS FLOAT ) ) AS DATETIME )) OR DeletedOn IS NULL)
			AND [ServiceName] LIKE 'Endpoint Security%'
			AND [Device Class] IN ('Windows Server', 'Windows Workstation', 'Windows Laptop')
			AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows') 
			AND [fact_Device Service Parameters].[Parameter Name] = 'Detection_Type_List'
			AND [fact_Device Service Parameters].[Parameter Value] LIKE '%AV%'
			AND ([fact_Device Service Parameters].[Parameter In Effect Started On] &lt;= @EndDate)
			AND ([fact_Device Service Parameters].[Parameter In Effect Ended On] &gt; @EndDate
				OR [fact_Device Service Parameters].[Parameter In Effect Ended On] is null)		
			AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
				OR @ShowDeleted &lt;&gt; 'False')
			AND [Date Monitoring Started On] &lt;= @EndDate 
			AND [Date Monitoring Stopped On] &gt; @EndDate 
			AND [Is Managed Device Indicator] = 'Managed Device' 
			AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'
			AND d.deviceID NOT IN (select deviceid from @mAvDevices)	
				
			SELECT @TotalAntivirusDevices = COUNT(*) 
			FROM @mAVDevices a
			INNER JOIN  dbo.dim_device d
				 ON a.Deviceid=d.DeviceID
				 AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
					 OR @ShowDeleted &lt;&gt; 'False')
				 AND (d.[Is managed device indicator] = 'Managed Device')
				 AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
				 
		  -- Get Total Patch Level Devices Count
		  SELECT @TotalPatchLevelDevices = COUNT(DISTINCT d.deviceid)
		  FROM   dim_device d
				 INNER JOIN [fact_device services]
				   ON d.deviceid = [fact_device services].deviceid
				 INNER JOIN [dim_service]
				   ON [fact_device services].serviceid = dim_service.serviceid
				 INNER JOIN @mCustomer c
				   ON d.[n-central customerid] = c.customerid
					  AND d.[datasourceid] = c.[datasourceid]
		  WHERE  [ServiceName] IN (SELECT ServiceName FROM @PatchServiceNames)
				 AND [device class] IN ('Windows Server','Windows Workstation','Windows Laptop')
				 AND [appliance type] NOT IN ('Workgroup - Windows','Network - Windows')
				 AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
					 OR @ShowDeleted &lt;&gt; 'False')
				 AND [date monitoring started on] &lt;= @EndDate
				 AND [Date Monitoring Stopped On] &gt; @EndDate
				 AND (d.[is managed device indicator] = @IsManaged)
				 AND (d.[Is Ignored Device Indicator] = @IsIgnoredDevice)
		  
		  DECLARE  @FirewallMonitoringEnabled BIT
		  DECLARE  @AntivirusMonitoringEnabledPercentage REAL
		  DECLARE  @PatchMonitoringEnabledPercentage REAL
		  
		  IF @TotalFirewallDevices = 0
			SET @FirewallMonitoringEnabled = 0
		  ELSE
			SET @FirewallMonitoringEnabled = 1
		  
		  IF @TotalWindowsDevices = 0
			BEGIN
			  SET @AntivirusMonitoringEnabledPercentage = 0
			  SET @PatchMonitoringEnabledPercentage = 0
			  SET @Score = 0
			END
		  ELSE
			BEGIN
			  SET @AntivirusMonitoringEnabledPercentage = CAST(@TotalAntivirusDevices AS REAL) / CAST(@TotalWindowsDevices AS REAL) * 100
			  SET @PatchMonitoringEnabledPercentage = CAST(@TotalPatchLevelDevices AS REAL) / CAST(@TotalWindowsDevices AS REAL) * 100
			--SELECT CAST(@TotalWindowsDevices AS REAL) REMEMBER TO REMOVE!!
			--SELECT CAST(@TotalPatchLevelDevices AS REAL)REMEMBER TO REMOVE!!

		------------------------------------------
					declare @Value int
					declare @bString nvarchar(10)
					SET @bString =CAST(@AVMonitoring AS nvarchar (1))+','+CAST(@FirewallMonitoring AS nvarchar (1))+','+CAST(@PatchMonitoring AS nvarchar (1))
					SET	@bString=REVERSE(@bString)

					------------------------------------------
					--Convert comma delinited binary string to interger
					------------------------------------------
							Select @Value = Sum(X) 
							from(
							Select *,Power(2,RowNumber)*[Value] X from(
							Select  cast(ROW_NUMBER() over( order by [order]) as int) -1 RowNumber, cast([VALUE] as int) [Value]
							from(
							Select [Value] , 1 [order]
							from warehouse.dbo.inline_split_me(@bString))
							AA) BB) CC
					------------------------------------------

				Set @Score = (Select Case
							when  @Value = 1 then   @PatchMonitoringEnabledPercentage
							when  @Value = 2 then   CAST(@FirewallMonitoringEnabled AS REAL) * 100 
							when  @Value = 3 then  (CAST(@FirewallMonitoringEnabled AS REAL) * 100 + @PatchMonitoringEnabledPercentage) / 2
							when  @Value = 4 then   @AntivirusMonitoringEnabledPercentage
							when  @Value = 5 then  (@AntivirusMonitoringEnabledPercentage+ @PatchMonitoringEnabledPercentage)/2
							when  @Value = 6 then  (@AntivirusMonitoringEnabledPercentage +CAST(@FirewallMonitoringEnabled AS REAL) * 100)/2
							when  @Value = 7 then  (CAST(@FirewallMonitoringEnabled AS REAL) * 100 + @AntivirusMonitoringEnabledPercentage + @PatchMonitoringEnabledPercentage) / 3
						End Score)
		------------------------------------------

		END
		  
		  IF (@ScoreOnly = 0)
			BEGIN
			  SELECT @FirewallMonitoringEnabled AS [monitoring of firewall enabled],
					 @AntivirusMonitoringEnabledPercentage AS [percentage of antivirus monitoring enabled],
					 @PatchMonitoringEnabledPercentage AS [percentage of patch monitoring enabled],
					 @Score AS [score]
			END
	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Monitoring_of_Firewall_Enabled">
          <DataField>Monitoring of Firewall Enabled</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Percentage_Of_Antivirus_Monitoring_Enabled">
          <DataField>Percentage of Antivirus Monitoring Enabled</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Percentage_Of_Patch_Monitoring_Enabled">
          <DataField>Percentage of Patch Monitoring Enabled</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Score">
          <DataField>Score</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPTES_ServerPerformance_Score">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!CustomerID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PeriodType">
            <Value>=Parameters!PeriodType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NumberOfDevices">
            <Value>=Parameters!ServerPerformanceNumberOfDevices.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CriticalServerUtilization">
            <Value>=Parameters!ServerPerformanceUtilizationLimit.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ServerPerformanceProximity">
            <Value>=Parameters!ServerPerformanceProximity.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UtilizationDeltaLimit">
            <Value>=Parameters!ServerPerformanceDeltaLimit.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ScoreOnly">
            <Value>0</Value>
          </QueryParameter>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeServerPerformance.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Score">
            <Value>=Parameters!Score.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT 0 AS [Servers Exceeding Critical Utilization Values], 
				0 AS [Servers Reaching Critical Utilization Values], 
				0 AS [Servers with Normal Utilization], 
				0 AS [Servers Exceeding Critical Utilization Values Percent], 
				0 AS [Servers Reaching Critical Utilization Values Percent], 
				0 AS [Servers with Normal Utilization Percent], 
				@Score AS [Score], 
				0 AS [Utilization Score], 
				0 AS [Delta Change Score], 
				0 AS [Total Monitored Elements]
		END
	ELSE
		BEGIN
			-- Get the start and end dates for the report
			SELECT @StartDate = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
			SELECT @EndDate = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

			DECLARE @UtilizationScore real
			DECLARE @DeltaChangeScore real
			DECLARE @TotalMonitoredElements int
			DECLARE @TotalServersExceedingCriticalValue int
			DECLARE @TotalServersReachingCriticalValue int
			DECLARE @TotalServersWithNormalValue int

			EXEC RPTES_ServerPerformance_ServerUtilizationDetails @StartDate, @EndDate, @CustomerID, @NumberOfDevices, @CriticalServerUtilization, @ServerPerformanceProximity, @Subscription, 1, 1, @UtilizationScore OUTPUT, @TotalMonitoredElements OUTPUT, @TotalServersExceedingCriticalValue OUTPUT, @TotalServersReachingCriticalValue OUTPUT, @TotalServersWithNormalValue OUTPUT
			EXEC RPTES_ServerPerformance_ServerDeltaDetails @StartDate, @EndDate, @CustomerID, @PeriodType, @NumberOfDevices, @UtilizationDeltaLimit, @Subscription, 1, 1, @DeltaChangeScore OUTPUT

			IF @UtilizationScore &gt; 0 AND @DeltaChangeScore &gt; 0
				SET @Score = (@UtilizationScore + @DeltaChangeScore)/2
			ELSE IF @UtilizationScore &gt; 0 AND @DeltaChangeScore &lt; 0
				SET @Score = @UtilizationScore
			ELSE
				SET @Score = -1

			IF @Score &lt; 0 SET @Score = -1

			IF (@ScoreOnly = 0)
				BEGIN

					--		
					DECLARE @TotalCount int
					SET @TotalCount = @TotalServersExceedingCriticalValue + @TotalServersReachingCriticalValue + @TotalServersWithNormalValue

					IF @TotalCount &gt; 0	
						SELECT @TotalServersExceedingCriticalValue AS [Servers Exceeding Critical Utilization Values], 
							@TotalServersReachingCriticalValue AS [Servers Reaching Critical Utilization Values], 
							@TotalServersWithNormalValue AS [Servers with Normal Utilization], 
							CAST(@TotalServersExceedingCriticalValue AS REAL)/CAST(@TotalCount AS REAL)*100 AS [Servers Exceeding Critical Utilization Values Percent], 
							CAST(@TotalServersReachingCriticalValue AS REAL)/CAST(@TotalCount AS REAL)*100 AS [Servers Reaching Critical Utilization Values Percent], 
							CAST(@TotalServersWithNormalValue AS REAL)/CAST(@TotalCount AS REAL)*100 AS [Servers with Normal Utilization Percent], 
							@Score AS [Score], 
							@UtilizationScore AS [Utilization Score], 
							@DeltaChangeScore AS [Delta Change Score], 
							@TotalMonitoredElements AS [Total Monitored Elements]
					ELSE
						SELECT 0 AS [Servers Exceeding Critical Utilization Values], 
							0 AS [Servers Reaching Critical Utilization Values], 
							0 AS [Servers with Normal Utilization], 
							0 AS [Servers Exceeding Critical Utilization Values Percent], 
							0 AS [Servers Reaching Critical Utilization Values Percent], 
							0 AS [Servers with Normal Utilization Percent], 
							-1 AS [Score],
							-1 AS [Utilization Score], 
							-1 AS [Delta Change Score], 
							0 AS [Total Monitored Elements]
				END
		END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Servers_Exceeding_Critical_Utilization_Values">
          <DataField>Servers Exceeding Critical Utilization Values</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Servers_Reaching_Critical_Utilization_Values">
          <DataField>Servers Reaching Critical Utilization Values</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Servers_with_Normal_Utilization">
          <DataField>Servers with Normal Utilization</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Servers_Exceeding_Critical_Utilization_Values_Percent">
          <DataField>Servers Exceeding Critical Utilization Values Percent</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Servers_Reaching_Critical_Utilization_Values_Percent">
          <DataField>Servers Reaching Critical Utilization Values Percent</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Servers_with_Normal_Utilization_Percent">
          <DataField>Servers with Normal Utilization Percent</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
