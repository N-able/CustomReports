        <CommandText>Select 
	psa.TicketID
	,psa.TicketTitle as 'Subject'
	,psa.TimeOpened
	,CASE WHEN psa.TimeClosed &gt; Dateadd(day, 1, @EndDate) then null else psa.TimeClosed end as TimeClosed
	,CASE WHEN man.TicketID is null AND inc.TicketID is null then 'No' else 'Yes' end as 'IsProactive'
	,isnull(th.TicketStatus, psa.TicketStatus) as TicketStatus
	,psa.TicketPriority
	,psa.TicketSource
	,psa.TicketQueue
	,psa.Issue
	,psa.Subissue
	,CASE WHEN DATEDIFF(hour, PSA.timeopened, CASE WHEN ISNULL(psa.timeclosed, @EndDate) &gt;= @EndDate then DATEADD(day, 1, @EndDate) else psa.timeclosed end) &lt;= 48 THEN 
		Cast(ROUND(cast(DATEDIFF(minute, PSA.timeopened, CASE WHEN ISNULL(psa.timeclosed, @EndDate) &gt;= @EndDate then DATEADD(day, 1, @EndDate) else psa.timeclosed end)as float)/60, 2) as varchar(8)) + ' Hour(s)'
	ELSE Cast(ROUND(cast(DATEDIFF(hour, PSA.timeopened, CASE WHEN ISNULL(psa.timeclosed, @EndDate) &gt;= @EndDate then DATEADD(day, 1, @EndDate) else psa.timeclosed end)as float)/24, 2) as varchar(8)) + ' Day(s)' END as Age
	,psa.Resolution
	,psa.TicketResolved
from fact_Ticket PSA		
	LEFT JOIN
		fact_ManualTicket Man
			on man.CustomerID = PSA.CustomerID
			AND Man.TicketID = PSA.TicketID
	LEFT JOIN
		fact_IncidentTicket Inc
			on Inc.CustomerID = PSA.CustomerID
			AND Inc.TicketID = PSA.TicketID
	LEFT JOIN
		fact_TicketHistory th
			on th.FactTicketID = PSA.FactTicketID
			AND dateadd(day, 1, @EndDate) between dbo.GetDateTimeFromDateTimeIds(th.DateID, th.TimeID) and dbo.GetDateTimeFromDateTimeIds(th.EndDateID, th.EndTimeID)
WHERE CAST(psa.TimeOpened as DATE) &lt;= CAST(@EndDate as DATE)
	AND ((CAST(psa.TimeClosed as DATE) between CAST(@StartDate as DATE) AND CAST(@EndDate as DATE))
		OR psa.TimeClosed is null)
	AND PSA.CustomerID in (@Customer_ID)
	AND ((PSA.TicketQueue in (@TicketQueue) and @FilterByQueue = 'true') OR @FilterByQueue = 'false')
	AND ((PSA.TicketPriority in (@TicketPriority) and @FilterByPriority = 'true') OR @FilterByPriority = 'false')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
