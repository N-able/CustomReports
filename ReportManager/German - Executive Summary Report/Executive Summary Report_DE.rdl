    <DataSet Name="RPTES_AandLCompliance_Score">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!CustomerID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ScoreOnly">
            <Value>0</Value>
          </QueryParameter>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeApplicationLicenseCompliance.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Score">
            <Value>=Parameters!Score.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out! 
	IF (@SectionEnabled = 0) 
		BEGIN 
		  SELECT 0      AS [number of devices monitored], 
				 0      AS [number of unmonitored devices], 
				 0      AS [number of unlicensed applications], 
				 0      AS [number of non compliance applications installed], 
				 @Score AS score 
		END 
	ELSE
		BEGIN
		  -- get the true start and end date based on subscription or not 
		  SELECT @EndDate = [dbo].Getenddate(@EndDate,Getdate(),@Subscription) 
							 
		  -- get the enddateid 
		  DECLARE  @EndDateID INTEGER 
							   
		  SET @EndDateID = CONVERT(INT,CONVERT(CHAR(8),@EndDate,112)) 
							
		  -- get the datasourceid 
		  DECLARE  @DataSourceID INTEGER 
								  
		  SELECT @DataSourceID = datasourceid 
		  FROM   dim_customer 
		  WHERE  customerid = @CustomerID 
							   
		  DECLARE  @ShowDeleted NVARCHAR(30) 
								 
		  SELECT @ShowDeleted = (SELECT [value] 
								 FROM   config.dbo.config 
								 WHERE  [key] = 'ShowDeletedDevices') 
								 
		  -- Find out the number of unlicensed applications 
		  DECLARE  @UnlicensedApps INTEGER, 
				   @TotalApps      INTEGER 
									
		  SET @UnlicensedApps = (SELECT COUNT(* ) 
								 FROM   [fact_application license compliance] 
								 WHERE  customerid = @CustomerID 
										AND dateid = @EndDateID 
										AND [percentage consumed] &gt; 100) 
								 
		  SET @TotalApps = (SELECT COUNT(* ) 
							FROM   [fact_application license compliance] 
							WHERE  customerid = @CustomerID 
								   AND dateid = @EndDateID) 
							
		  -- get the total number of Windows devices the customer had on the end date 
		  DECLARE  @mTmpDeviceReduced  TABLE( 
											 deviceid       INTEGER, 
											 [devicename]   NVARCHAR(100), 
											 [device class] NVARCHAR(100) 
											 ) 
					
		  INSERT INTO @mTmpDeviceReduced 
		  SELECT deviceid, 
				 devicename, 
				 [device class] 
		  FROM   dim_device d 
		  WHERE  (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME ))&lt;= @EndDate 
				 AND (@EndDate &lt;= (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) 
					   OR deletedon IS NULL) 
				 AND datasourceid = @DataSourceID 
				 AND [n-central customerid] = (SELECT [n-central customerid] 
											   FROM   dim_customer 
											   WHERE  customerid = @CustomerID) 
				 AND [device class] IN ('Windows Server','Windows Workstation','Windows Laptop') 
				 AND ((@ShowDeleted = 'False' 
					   AND d.[status] = 'Active') 
					   OR @ShowDeleted &lt;&gt; 'False') 
				 AND [is managed device indicator] = 'Managed Device' 
				 AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
		  -- determine how many of the devices are not being monitored for application compliance (141).   
		  -- first find out the serviceids, then figure out how many devices are being monitored, then subtract that number from the total    
		  --  number of devices for that customer. 
		  DECLARE  @ACServiceID INTEGER 
								 
		  SELECT @ACServiceID = serviceid 
		  FROM   dim_service 
		  WHERE  [n-central serviceid] = 141 
				 AND @DataSourceID = datasourceid 
									  
		  DECLARE  @TotalDevicesMissingServices INTEGER, 
				   @TotalDevices                INTEGER, 
				   @TotalMonitoredDevices       INTEGER 
												 
		  SET @TotalDevices = (SELECT COUNT(* ) 
							   FROM   @mTmpDeviceReduced) 
							   
		  SET @TotalDevicesMissingServices = (SELECT COUNT(* ) 
											  FROM   @mTmpDeviceReduced 
											  WHERE  deviceid NOT IN (SELECT deviceid 
																	  FROM   [fact_device services] 
																	  WHERE  customerid = @CustomerID 
																			 AND deviceid IN (SELECT deviceid 
																							  FROM   @mTmpDeviceReduced) 
																			 AND (serviceid = @ACServiceID) 
																			 AND [date monitoring started on] &lt; @EndDate 
																			 AND [date monitoring stopped on] &gt; @EndDate)) 
											  
		  SET @TotalMonitoredDevices = (SELECT COUNT(* ) 
										FROM   @mTmpDeviceReduced) - @TotalDevicesMissingServices 
																	  
		  -- determine number of devices with non compliance applications installed 
		  DECLARE  @TotalNonComplianceDevices INTEGER 
											   
		  SELECT @TotalNonComplianceDevices = COUNT(DISTINCT deviceid) 
		  FROM   [fact_device license compliance details] 
		  WHERE  customerid = @CustomerID 
				 AND [date not in compliance] &lt; @EndDate + ' 23:59' 
				 AND ([date in compliance] IS NULL 
					   OR [date in compliance] &gt;= @EndDate + ' 23:59') 
				 AND deviceid IN (SELECT deviceid 
								  FROM   [fact_device services] 
								  WHERE  customerid = @CustomerID 
										 AND serviceid = @ACServiceID 
										 AND [date monitoring started on] &lt; @EndDate + ' 23:59' 
										 AND [date monitoring stopped on] &gt;= @EndDate + ' 23:59') 
								  
		  -- determine percentages 
		  DECLARE  @ScoreNum INTEGER 
							  
		  SET @ScoreNum = 0 
						   
		  -- there will be 4 entries in the table:   
		  -- a) # of Monitored Devices   
		  -- b) # of Unmonitored Devices   
		  -- c) # of Apps Over License Allowance   
		  -- d) # of Devices with Non Compliant Apps   
		  --    
		  -- The score will be calculated as follows:   
		  -- 50% -&gt; a / a + b   
		  -- 25% -&gt; # of apps under license allowance / # apps monitored for license compliance   
		  -- 25% -&gt; d / a 
		  DECLARE  @Score1 REAL, 
				   @Score2 REAL, 
				   @Score3 REAL 
							
		  IF @TotalDevices &lt;&gt; 0 
			BEGIN 
			  SET @Score1 = (CAST(@TotalMonitoredDevices AS REAL) / (CAST(@TotalDevices AS REAL))) 
							 
			  SET @Score3 = 1 - (CAST(@TotalNonComplianceDevices AS REAL) / CAST(@TotalDevices AS REAL)) 
			END 
		  ELSE 
			BEGIN 
			  SET @Score1 = 0 
							 
			  SET @Score3 = 0 
			END 
			 
		  IF @TotalApps &lt;&gt; 0 
			SET @Score2 = 1 - (CAST(@UnlicensedApps AS REAL) / CAST(@TotalApps AS REAL)) 
		  ELSE 
			SET @Score2 = 0 
						   
		  -- check to see if license compliance is being monitored 
		  DECLARE  @LicenseComplianceCount INTEGER, 
				   @AppComplianceCount     INTEGER, 
				   @Denominator            INTEGER 
											
		  SELECT @LicenseComplianceCount = COUNT(* ) 
		  FROM   [fact_device services] 
		  WHERE  datasourceid = @DataSourceID 
				 AND customerid = @CustomerID 
				 AND serviceid IN (SELECT serviceid 
								   FROM   dim_service 
								   WHERE  servicename = 'Application Compliance') 

		  SELECT @AppComplianceCount = COUNT(* ) 
		  FROM   [fact_device services] 
		  WHERE  datasourceid = @DataSourceID 
				 AND customerid = @CustomerID 
				 AND serviceid IN (SELECT serviceid 
								   FROM   dim_service 
								   WHERE  servicename = 'License Compliance') 

		  SET @Score = @Score1 * 50 
		   
		  SET @Denominator = 50 
		   
		  IF (@AppComplianceCount != 0) 
			BEGIN 
			  SET @Score = @Score + (@Score2 * 25) 
			   
			  SET @Denominator = @Denominator + 25 
			END 
		   
		  IF (@LicenseComplianceCount != 0) 
			BEGIN 
			  SET @Score = @Score + (@Score3 * 25) 
			   
			  SET @Denominator = @Denominator + 25 
			END 
			 
		  SET @Score = @Score * 100 / @Denominator 
									   
		  -- determine score 
		  IF (@ScoreOnly = 0) 
			BEGIN 
			  SELECT @TotalMonitoredDevices       AS [number of devices monitored], 
					 @TotalDevicesMissingServices AS [number of unmonitored devices], 
					 CASE 
					   WHEN @AppComplianceCount = 0 THEN 'N/A' 
					   ELSE CAST(@UnlicensedApps AS VARCHAR(6)) 
					 END AS [number of applications over license usage], 
					 CASE 
					   WHEN @LicenseComplianceCount = 0 THEN 'N/A' 
					   ELSE CAST(@TotalNonComplianceDevices AS VARCHAR(6)) 
					 END AS [number of devices with non compliant applications installed], 
					 @Score                       AS score 
			END 
		END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Number_of_Devices_Monitored">
          <DataField>Number of Devices Monitored</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Number_of_Unmonitored_Devices">
          <DataField>Number of Unmonitored Devices</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Number_of_Applications_Over_License_Usage">
          <DataField>Number of Applications Over License Usage</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Number_of_Devices_with_Non_Compliant_Applications_Installed">
          <DataField>Number of Devices with Non Compliant Applications Installed</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Score">
          <DataField>Score</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPTES_DataProtection_Score">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!CustomerID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ScoreOnly">
            <Value>0</Value>
          </QueryParameter>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeDataProtection.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Score">
            <Value>=Parameters!Score.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT 
				0 AS [Failed Backup Jobs Percent],
				0 AS [Failed Backup Jobs Count],
				0 AS [Successful Backup Jobs Percent],
				0 AS [Successful Backup Jobs Count],
				0 AS [Warning Backup Jobs Percent],
				0 AS [Warning Backup Jobs Count],
				@Score AS [Score]
		END
	ELSE
		BEGIN
			-- get the true start and end date based on subscription or not
			SELECT @StartDate = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
			SELECT @EndDate = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

			-- get the number of successful backup jobs (status = 1)
			DECLARE @SuccessJobs INTEGER
			SET @SuccessJobs = 
					(SELECT COUNT(*) FROM [Fact_BackupServices] 
					WHERE CustomerID = @CustomerID AND [Job Return Code] = 1 
					 AND @StartDate &lt;= [Job StartTime] AND [Job StartTime] &lt; DATEADD(Day, 1, @EndDate))

			-- get the number of backup jobs with warnings (status = 2)
			DECLARE @WarningJobs INTEGER
			SET @WarningJobs = 
					(SELECT COUNT(*) FROM [Fact_BackupServices] 
					WHERE CustomerID = @CustomerID AND [Job Return Code] = 2 
					 AND @StartDate &lt;= [Job StartTime] AND [Job StartTime] &lt; DATEADD(Day, 1, @EndDate))

			-- get the number of failed backup jobs (status = 3)
			DECLARE @FailedJobs INTEGER
			SET @FailedJobs = 
					(SELECT COUNT(*) FROM [Fact_BackupServices] 
					WHERE CustomerID = @CustomerID AND [Job Return Code] = 3 
					 AND @StartDate &lt;= [Job StartTime] AND [Job StartTime] &lt; DATEADD(Day, 1, @EndDate))

			-- get the total number of backup jobs count
			DECLARE @TotalBackupJobs INTEGER
			SET @TotalBackupJobs = @SuccessJobs + @WarningJobs + @FailedJobs

			-- determine percentages
			DECLARE @pctSuccessJobs REAL
			DECLARE @pctWarningJobs REAL
			DECLARE @pctFailedJobs REAL
			IF (@TotalBackupJobs = 0)
			BEGIN
				-- to avoid divide by 0
				SET @pctSuccessJobs = 0
				SET @pctWarningJobs = 0
				SET @pctFailedJobs = 0
			END
			ELSE
			BEGIN
				SET @pctSuccessJobs = CAST(@SuccessJobs AS REAL) / CAST(@TotalBackupJobs AS REAL) * 100
				SET @pctWarningJobs = CAST(@WarningJobs AS REAL) / CAST(@TotalBackupJobs AS REAL) * 100
				SET @pctFailedJobs = CAST(@FailedJobs AS REAL) / CAST(@TotalBackupJobs AS REAL) * 100
			END

			SET @Score = @pctSuccessJobs + @pctWarningJobs

			-- determine score
			IF (@ScoreOnly = 0)
				BEGIN
					SELECT 
						@pctFailedJobs AS [Failed Backup Jobs Percent],
						@FailedJobs AS [Failed Backup Jobs Count],
						@pctSuccessJobs AS [Successful Backup Jobs Percent],
						@SuccessJobs AS [Successful Backup Jobs Count],
						@pctWarningJobs AS [Warning Backup Jobs Percent],
						@WarningJobs AS [Warning Backup Jobs Count],
						@Score AS [Score]
				END
		END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Failed_Backup_Jobs_Percent">
          <DataField>Failed Backup Jobs Percent</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Failed_Backup_Jobs_Count">
          <DataField>Failed Backup Jobs Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Successful_Backup_Jobs_Percent">
          <DataField>Successful Backup Jobs Percent</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Successful_Backup_Jobs_Count">
          <DataField>Successful Backup Jobs Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Warning_Backup_Jobs_Percent">
          <DataField>Warning Backup Jobs Percent</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Warning_Backup_Jobs_Count">
          <DataField>Warning Backup Jobs Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Score">
          <DataField>Score</DataField>
          <rd:TypeName>System.Single</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPTES_OverallScore">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PeriodType">
            <Value>=Parameters!PeriodType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ManagedDevicesEnabled">
            <Value>=Parameters!IncludeManagedDevices.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!CustomerID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ManagedDevicesWeight">
            <Value>=Parameters!ManagedDevicesWeight.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SecurityMonitoringEnabled">
            <Value>=Parameters!IncludeSecurityMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FirewallMonitoring">
            <Value>=Parameters!FirewallMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AVMonitoring">
            <Value>=Parameters!AVMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PatchMonitoring">
            <Value>=Parameters!PatchMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SecurityMonitoringWeight">
            <Value>=Parameters!SecurityMonitoringWeight.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AandLEnabled">
            <Value>=Parameters!IncludeApplicationLicenseCompliance.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AandLWeight">
            <Value>=Parameters!ApplicationWeight.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DataProtectionEnabled">
            <Value>=Parameters!IncludeDataProtection.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DataProtectionWeight">
            <Value>=Parameters!DataProtectionWeight.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NetworkReliabilityEnabled">
            <Value>=Parameters!IncludeNetworkReliability.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NetworkReliabilityServices">
            <Value>=JOIN(Parameters!NetworkReliabilityServices.Value, “,”)</Value>
          </QueryParameter>
          <QueryParameter Name="@StartHour">
            <Value>=Parameters!StartHour.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndHour">
            <Value>=Parameters!EndHour.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeBreakdown">
            <Value>=Parameters!IncludeBreakdown.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WeekType">
            <Value>=Parameters!WeekType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NetworkReliabilityWeight">
            <Value>=Parameters!NetworkReliabilityWeight.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ServerPerformanceEnabled">
            <Value>=Parameters!IncludeServerPerformance.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ServerPerformanceNDevices">
            <Value>=Parameters!ServerPerformanceNumberOfDevices.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ServerPerformanceUtilizationLimit">
            <Value>=Parameters!ServerPerformanceUtilizationLimit.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ServerPerformanceDelta">
            <Value>=Parameters!ServerPerformanceDeltaLimit.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ServerPerformanceWeight">
            <Value>=Parameters!ServerPerformanceWeight.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeWarrantyStatus">
            <Value>=Parameters!IncludeWarrantyStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WarrantyStatusPreviousScore">
            <Value>=Parameters!WarrantyStatusPreviousScore.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WarrantyStatusWeight">
            <Value>=Parameters!WarrantyStatusWeight.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeTicketSummary">
            <Value>=Parameters!IncludeTicketSummary.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TicketPreviousScore">
            <Value>=Parameters!TicketPreviousScore.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TicketSummaryWeight">
            <Value>=Parameters!TicketSummaryWeight.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @Start_Date datetime
	DECLARE @End_Date datetime
	-- get the true start and end date based on subscription or not
	SELECT @Start_Date = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
	SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)
	
	-- get previous period dates
	DECLARE @PreviousStartDate DATETIME
	DECLARE @PreviousEndDate DATETIME
	
	--!SELECT @PreviousStartDate = dbo.GetPreviousDate(@Start_Date, @PeriodType)
	--!SELECT @PreviousEndDate = dbo.GetPreviousDate(@End_Date, @PeriodType)
	DECLARE @deltaDays int	
	IF dbo.GetPreviousDate(@Start_Date, @PeriodType) = @Start_Date
		BEGIN
			-- Bug 42765 - support of the date intervals based on "n days"
			SET @deltaDays = DATEDIFF(day,@Start_Date,@End_Date)
			SET @PreviousEndDate = DATEADD(day,-1,@Start_Date)
			SET @PreviousStartDate = DATEADD(day,-(@deltaDays+1),@Start_Date)
		END
	ELSE
		BEGIN
			SET @PreviousStartDate = dbo.GetPreviousDate(@StartDate, @PeriodType)
			SET @PreviousEndDate = dbo.GetPreviousDate(@EndDate, @PeriodType)
		END
		
	-- declare our output
	DECLARE @PreviousManagedDevicesScore REAL, @PreviousSecurityMonitoringScore REAL, @PreviousAandLScore REAL
	DECLARE @PreviousDataProtectionScore REAL, @PreviousNetworkReliabilityScore REAL, @PreviousServerPerformanceScore REAL

	SELECT @PreviousManagedDevicesScore = 0, @PreviousSecurityMonitoringScore = 0, @PreviousAandLScore = 0
	SELECT @PreviousDataProtectionScore = 0, @PreviousNetworkReliabilityScore = 0, @PreviousServerPerformanceScore = 0

	-- declare some variables used to determine the weighted average
	DECLARE @PreviousOverallSum REAL
	DECLARE @PreviousWeightSum REAL
	
	SELECT @PreviousOverallSum = 0, @PreviousWeightSum = 0

	DECLARE @IsInvalidScore bit
	SET @IsInvalidScore = 0

	DECLARE @IsValidScore bit

	SET @IsValidScore = 0

	-- get only the sections we care about
	-- Managed Devices
	IF (@ManagedDevicesEnabled = 1)
		BEGIN
			EXEC RPTES_ManagedDevices_Score @PreviousEndDate, @CustomerID, 'none', 1, 1, @PreviousManagedDevicesScore OUTPUT
			SET @PreviousOverallSum = @PreviousOverallSum + (ISNULL(@PreviousManagedDevicesScore,0) * @ManagedDevicesWeight)
			SET @PreviousWeightSum = @PreviousWeightSum + @ManagedDevicesWeight

			SET @IsValidScore = 1
		END

	-- Security Monitoring
	IF (@SecurityMonitoringEnabled = 1)
		BEGIN
			EXEC RPTES_SecurityMonitoring_Score @PreviousEndDate, @CustomerID, 'none', 1, 1, @PreviousSecurityMonitoringScore OUTPUT,@FirewallMonitoring,@AVMonitoring,@PatchMonitoring
			SET @PreviousOverallSum = @PreviousOverallSum + (ISNULL(@PreviousSecurityMonitoringScore,0) * @SecurityMonitoringWeight)
			SET @PreviousWeightSum = @PreviousWeightSum + @SecurityMonitoringWeight

			SET @IsValidScore = 1
		END

	-- Application and License Compliance
	IF (@AandLEnabled = 1)
		BEGIN
			EXEC RPTES_AandLCompliance_Score @PreviousEndDate, @CustomerID, 'none', 1, 1, @PreviousAandLScore OUTPUT
			SET @PreviousOverallSum = @PreviousOverallSum + (ISNULL(@PreviousAandLScore,0) * @AandLWeight)
			SET @PreviousWeightSum = @PreviousWeightSum + @AandLWeight

			SET @IsValidScore = 1
		END

	-- Data Protection
	IF (@DataProtectionEnabled = 1)
		BEGIN
			EXEC RPTES_DataProtection_Score @PreviousStartDate, @PreviousEndDate, @CustomerID, 'none', 1, 1, @PreviousDataProtectionScore OUTPUT
			SET @PreviousOverallSum = @PreviousOverallSum + (ISNULL(@PreviousDataProtectionScore,0) * @DataProtectionWeight)
			SET @PreviousWeightSum = @PreviousWeightSum + @DataProtectionWeight

			SET @IsValidScore = 1
		END

	-- Network Reliability
	IF (@NetworkReliabilityEnabled = 1)
		BEGIN
			EXEC RPTES_NetworkReliability_Score @PreviousStartDate, @PreviousEndDate, 'none', @CustomerID, @NetworkReliabilityServices, 0, 1, 1, @PreviousNetworkReliabilityScore OUTPUT, @StartHour, @EndHour, @IncludeBreakdown, @WeekType
			SET @PreviousOverallSum = @PreviousOverallSum + (ISNULL(@PreviousNetworkReliabilityScore,0) * @NetworkReliabilityWeight)
			SET @PreviousWeightSum = @PreviousWeightSum + @NetworkReliabilityWeight

			SET @IsValidScore = 1
		END

	-- Server Performance
	IF (@ServerPerformanceEnabled = 1)
		BEGIN	
			EXEC RPTES_ServerPerformance_Score @PreviousStartDate, @PreviousEndDate, @CustomerID, @PeriodType, @ServerPerformanceNDevices, @ServerPerformanceUtilizationLimit, 10, @ServerPerformanceDelta, 'none', 1, 1, @PreviousServerPerformanceScore OUTPUT
			IF @PreviousWeightSum = 0 AND @PreviousServerPerformanceScore &lt; 0
				SET @IsInvalidScore = 1
			IF @PreviousServerPerformanceScore &gt;= 0
				BEGIN
			SET @PreviousOverallSum = @PreviousOverallSum + (@PreviousServerPerformanceScore * @ServerPerformanceWeight)
			SET @PreviousWeightSum = @PreviousWeightSum + @ServerPerformanceWeight
				END

			SET @IsValidScore = 1
		END

	-- Warranty Status
	IF (@IncludeWarrantyStatus = 1) AND (@WarrantyStatusPreviousScore &gt;= 0)
		BEGIN
			SET @PreviousOverallSum = @PreviousOverallSum + (ISNULL(@WarrantyStatusPreviousScore,0) * cast(@WarrantyStatusWeight as float))
			SET @PreviousWeightSum = @PreviousWeightSum + @WarrantyStatusWeight

			SET @IsValidScore = 1
		END

	-- Ticket Status
	IF (@IncludeTicketSummary = 1) AND (@TicketPreviousScore &gt;=0)
		BEGIN
			SET @PreviousOverallSum = @PreviousOverallSum + (@TicketPreviousScore * cast(@TicketSummaryWeight as float))
			SET @PreviousWeightSum = @PreviousWeightSum + @TicketSummaryWeight

			SET @IsValidScore = 1
		END

	-- now do the overall score
	DECLARE @PreviousScore REAL
	
	-- avoid divide by 0
	IF @IsInvalidScore = 1 OR @IsValidScore = 0
		SET @PreviousScore = -1
	ELSE IF @PreviousWeightSum = 0
		SET @PreviousScore = 0
	ELSE
		SET @PreviousScore = @PreviousOverallSum / @PreviousWeightSum

	SELECT 
			ISNULL(@PreviousManagedDevicesScore,0) AS [Previous_Managed_Devices_Score],
			ISNULL(@WarrantyStatusPreviousScore,0) AS [Previous_Warranty_Status_Score],
			ISNULL(@PreviousSecurityMonitoringScore,0) AS [Previous_Security_Monitoring_Score],
			ISNULL(@PreviousAandLScore,0) AS [Previous_AandL_Score],
			ISNULL(@PreviousDataProtectionScore,0) AS [Previous_Data_Protection_Score],
			ISNULL(@PreviousNetworkReliabilityScore,0) AS [Previous_Network_Reliability_Score],
			ISNULL(@PreviousServerPerformanceScore,0) AS [Previous_Server_Performance_Score],
			ISNULL(@TicketPreviousScore,0) AS [Previous_Ticket_Summary_Score],
			ISNULL(@PreviousScore,0) AS [Previous_Score]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
