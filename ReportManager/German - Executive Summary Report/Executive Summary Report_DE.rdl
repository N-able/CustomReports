    <DataSet Name="RPTES_NetworkReliability_Score">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!CustomerID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Services">
            <Value>=Parameters!NetworkReliabilityServices.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CriticalUtilPct">
            <Value>=Parameters!NetworkUtilizationLimit.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ScoreOnly">
            <Value>0</Value>
          </QueryParameter>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeNetworkReliability.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Score">
            <Value>=Parameters!Score.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartHour">
            <Value>=Parameters!StartHour.Value*60+1</Value>
          </QueryParameter>
          <QueryParameter Name="@EndHour">
            <Value>=Parameters!EndHour.Value*60+1</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeBreakdown">
            <Value>=Parameters!IncludeBreakdown.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WeekType">
            <Value>=Parameters!WeekType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- Set start and end hours based on include breakdown selection. 
  IF (@IncludeBreakdown = 1) 
    BEGIN 
      -- Correct for the EndHour if "All Day" flag is selected (both Start and End will be zero). 
      IF (@StartHour = 0 
          AND @EndHour = 0) 
        SET @EndHour = 1440 
       
      -- Convert the @StartHour and @EndHour to the 24 hour clock. 
      SET @StartHour = Round(@StartHour / 60,0) 
       
      SET @EndHour = Round(@EndHour / 60,0) 
    END 
  ELSE 
    BEGIN 
      SET @StartHour = 0 
       
      SET @EndHour = 24 
    END 
	
  -- get the true start and end date based on subscription or not 
  SELECT @StartDate = [dbo].Getstartdate(@StartDate,Getdate(),@Subscription) 
   
  SELECT @EndDate = [dbo].Getenddate(@EndDate,Getdate(),@Subscription) 
   
 DECLARE  @StartDateID INTEGER 
   
  DECLARE  @EndDateID INTEGER 
   
  SET @StartDateID = CONVERT(INT,CONVERT(CHAR(8),@StartDate,112)) 
   
  SET @EndDateID = CONVERT(INT,CONVERT(CHAR(8),@EndDate,112))    
   
  -- get the datasourceid 
  DECLARE  @DataSourceID       INTEGER, 
           @NcentralCustomerID INTEGER 
   
  SELECT @DataSourceID = datasourceid, 
         @NcentralCustomerID = [n-central customerid] 
  FROM   dim_customer 
  WHERE  customerid = @CustomerID 
   
  -- Create the temp. table to store the filtered Fact_availability table. 
  CREATE TABLE #mFactAvailReduced (
                                     [customerid]   SMALLINT, 
                                     [serviceid]    INTEGER, 
                                     [datasourceid] TINYINT, 
                                     [deviceid]     INT, 
                                     [available]    TINYINT, 
                                     [start time]   DATETIME, 
                                     [end time]     DATETIME
--									 ,[isServiceGrp] bit 
                                     ) 
   
  -- Create temp. table to store all devices
  -- that are managed  
  DECLARE  @mDeviceList  TABLE( 
                               [deviceid] INT, 
                               [deleted]  NVARCHAR(20) 
                               ) 
   
  INSERT INTO @mDeviceList 
  SELECT deviceid, 
         [dim_device].[status] 
  FROM   [dim_device] 
  WHERE  [n-central customerid] = @NcentralCustomerID 
         AND datasourceid = @DataSourceID 
		AND [Is Managed Device Indicator] = 'Managed Device'
		AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
 
  -- Get the show deleted devices value from the config database -&gt; config table. 
  DECLARE  @ShowDeleted NVARCHAR(1000) 
   
  SET @ShowDeleted = (SELECT [value] 
                      FROM   config.dbo.config 
                      WHERE  [key] = 'ShowDeletedDevices') 
   
  -- If the user has specified TO SHOW deleted devices, filter as normal.  Otherwise remove deleted devices. 
  IF (@ShowDeleted = 'False') 
    DELETE FROM @mDeviceList 
    WHERE       [deleted] != 'Active' 
   
  -- PREFILTER the fact_Availability table with customer, devices, services, dates, and deleted flag. 
  INSERT INTO #mFactAvailReduced 
  SELECT avail.[customerid], 
         avail.[serviceid], 
         avail.[datasourceid], 
         avail.[deviceid], 
         avail.[available], 
         avail.[start time], 
         avail.[end time] 
  FROM   fact_availability AS avail 
         INNER JOIN @mDeviceList AS dev 
           ON dev.[deviceid] = avail.[deviceid] 
  WHERE customerid = @CustomerID 
    AND [end time] &gt;= @StartDate

  -- create indexes to speed things up                     
  CREATE INDEX tmpIDX_mFactAvailReducedStart ON #mFactAvailReduced ([start time])
  CREATE INDEX tmpIDX_mFactAvailReducedEnd ON #mFactAvailReduced ([end time])
  
  -- Create temp. variables for the startdate and enddate and span for the period loop. 
  DECLARE  @PeriodStartDate DATETIME, 
           @PeriodEndDate   DATETIME, 
           @ServiceID       INTEGER 
   
  -- Create the temp. table for the avaialability for each period. 
  DECLARE  @mPeriodValues  TABLE( 
                                 [periodstartdate] DATETIME, 
                                 [serviceid]       INTEGER, 
                                 [available]       FLOAT, 
                                 [span length]     FLOAT
--								 ,[isServiceGrp]    bit 
                                 ) 
   
  -- Set flag before entering loop. 
  SET @PeriodEndDate = @StartDate 
   
  -- Go through the reduced table to determine availability for each Day (will regroup later). 
  WHILE (@PeriodEndDate &lt;= @EndDate) 
    BEGIN 
      -- Set the Period Start and End Dates. 
      SET @PeriodStartDate = @PeriodEndDate 
       
      SET @PeriodEndDate = Dateadd(DAY,1,@PeriodStartDate) 
       
      -- Get the length of time where available = 1. 
      INSERT INTO @mPeriodValues 
      SELECT   @PeriodStartDate, 
               serviceid, 
               CASE 
                 WHEN a.[available] = 0 THEN 0 
                 ELSE Abs(SUM(CAST(Datediff(s,CASE -- Start day same but start time less than start hour. 
                                                WHEN Datepart(YEAR,[start time]) = Datepart(YEAR,@PeriodStartDate) 
                                                     AND Datepart(MONTH,[start time]) = Datepart(MONTH,@PeriodStartDate) 
                                                     AND Datepart(DAY,[start time]) = Datepart(DAY,@PeriodStartDate) 
                                                     AND Datepart(HOUR,[start time]) &lt; @Starthour THEN Dateadd(HOUR,@StartHour,@PeriodStartDate) 
                                                -- Start day same but start time between. 
                                                WHEN Datepart(YEAR,[start time]) = Datepart(YEAR,@PeriodStartDate) 
                                                     AND Datepart(MONTH,[start time]) = Datepart(MONTH,@PeriodStartDate) 
                                                     AND Datepart(DAY,[start time]) = Datepart(DAY,@PeriodStartDate) 
                                                     AND Datepart(HOUR,[start time]) &gt;= @Starthour 
                                                     AND Datepart(HOUR,[start time]) &lt; @Endhour THEN [start time] 
                                                -- Start day less than date. 
                                                WHEN [start time] &lt; @PeriodStartDate THEN Dateadd(HOUR,@StartHour,@PeriodStartDate) 
                                              END,CASE -- End day same but end time greater than end hour. 
                                                    WHEN Datepart(YEAR,[end time]) = Datepart(YEAR,@PeriodStartDate) 
                                                         AND Datepart(MONTH,[end time]) = Datepart(MONTH,@PeriodStartDate) 
                                                         AND Datepart(DAY,[end time]) = Datepart(DAY,@PeriodStartDate) 
                                                         AND Datepart(HOUR,[end time]) &gt;= @Endhour THEN Dateadd(HOUR,@EndHour,@PeriodStartDate) 
                                                    -- End day same and end time between. 
                                                    WHEN Datepart(YEAR,[end time]) = Datepart(YEAR,@PeriodStartDate) 
                                                         AND Datepart(MONTH,[end time]) = Datepart(MONTH,@PeriodStartDate) 
                                                         AND Datepart(DAY,[end time]) = Datepart(DAY,@PeriodStartDate) 
                                                         AND Datepart(HOUR,[end time]) &gt;= @Starthour 
                                                         AND Datepart(HOUR,[end time]) &lt; @Endhour THEN [end time] 
                                                    -- End day greater than date. 
                                                    WHEN [end time] &gt;= Dateadd(DAY,1,@PeriodStartDate) THEN Dateadd(HOUR,@EndHour,@PeriodStartDate) 
                                                  END) AS FLOAT))) 
               END, 
               Abs(SUM(CAST(Datediff(s,CASE -- Start day same but start time less than start hour . 
                                         WHEN Datepart(YEAR,[start time]) = Datepart(YEAR,@PeriodStartDate) 
                                              AND Datepart(MONTH,[start time]) = Datepart(MONTH,@PeriodStartDate) 
                                              AND Datepart(DAY,[start time]) = Datepart(DAY,@PeriodStartDate) 
                                              AND Datepart(HOUR,[start time]) &lt; @Starthour THEN Dateadd(HOUR,@StartHour,@PeriodStartDate) 
                                         -- Start day same but start time between. 
                                         WHEN Datepart(YEAR,[start time]) = Datepart(YEAR,@PeriodStartDate) 
                                              AND Datepart(MONTH,[start time]) = Datepart(MONTH,@PeriodStartDate) 
                                              AND Datepart(DAY,[start time]) = Datepart(DAY,@PeriodStartDate) 
                                              AND Datepart(HOUR,[start time]) &gt;= @Starthour 
                                              AND Datepart(HOUR,[start time]) &lt; @Endhour THEN [start time] 
                                         -- Start day less than date. 
                                         WHEN [start time] &lt; @PeriodStartDate THEN Dateadd(HOUR,@StartHour,@PeriodStartDate) 
                                       END,CASE -- End day same but end time greater than end hour. 
                                             WHEN Datepart(YEAR,[end time]) = Datepart(YEAR,@PeriodStartDate) 
                                                  AND Datepart(MONTH,[end time]) = Datepart(MONTH,@PeriodStartDate) 
                                                  AND Datepart(DAY,[end time]) = Datepart(DAY,@PeriodStartDate) 
                                                  AND Datepart(HOUR,[end time]) &gt;= @Endhour THEN Dateadd(HOUR,@EndHour,@PeriodStartDate) 
                                             -- End day same and end time between. 
                                             WHEN Datepart(YEAR,[end time]) = Datepart(YEAR,@PeriodStartDate) 
                                                  AND Datepart(MONTH,[end time]) = Datepart(MONTH,@PeriodStartDate) 
                                                  AND Datepart(DAY,[end time]) = Datepart(DAY,@PeriodStartDate) 
                                                  AND Datepart(HOUR,[end time]) &gt;= @Starthour 
                                                  AND Datepart(HOUR,[end time]) &lt; @Endhour THEN [end time] 
                                             -- End day greater than date. 
                                             WHEN [end time] &gt;= Dateadd(DAY,1,@PeriodStartDate) THEN Dateadd(HOUR,@EndHour,@PeriodStartDate) 
                                           END) AS FLOAT))) -- SPAN 
      FROM     #mFactAvailReduced AS a 
      WHERE    [start time] &lt;= @PeriodEndDate 
               AND [end time] &gt;= @PeriodStartDate 
      GROUP BY serviceid,a.available 
    END 
  -- cleanup
  DROP TABLE #mFactAvailReduced

  -- Delete all days of the week that are not selected. 
  IF (@IncludeBreakdown = 1) 
    BEGIN 
      IF (@WeekType = 'Weekday') 
        BEGIN 
          DELETE @mPeriodValues 
          FROM   @mPeriodValues AS red 
                 INNER JOIN [dim_date] AS dat 
                   ON red.[periodstartdate] = dat.[date] 
          WHERE  dat.[weekend indicator] != 'Weekday' 
        END 
      ELSE 
        IF (@WeekType = 'Weekend') 
          BEGIN 
            DELETE @mPeriodValues 
            FROM   @mPeriodValues AS red 
                   INNER JOIN [dim_date] AS dat 
                     ON red.[periodstartdate] = dat.[date] 
            WHERE  dat.[weekend indicator] != 'Weekend' 
          END 
    END 
DECLARE @Vals TABLE (
		serviceid int,
		servicename	nvarchar(200),
		available float,
		[span length] float
		)

Insert into @Vals
  SELECT s.serviceid,
		 servicename   AS [Service Name], 
         SUM(available),  -- [total Availability]
         SUM([span length])  --total length
  FROM     @mPeriodValues pv 
           INNER JOIN [dim_service] s 
             ON s.serviceid = pv.serviceid 
  GROUP BY servicename, s.serviceid 

  DECLARE  @mAvailPercentages  TABLE( 
                               [AvailPercentage]  float null 
                               ) 
INSERT INTO @mAvailPercentages
SELECT (v.available/v.[span length])*100 as AvailPercentage
FROM @Vals v iNNER JOIN [dim_service] s 
             ON s.serviceid = v.serviceid 
GROUP BY v.servicename,v.serviceid,v.available,v.[span length], s.[Is Service Group],s.[Service Group SLA]
	ORDER BY v.serviceid,v.available

DECLARE  @AggAvailability FLOAT 

SELECT @AggAvailability = AVG(AvailPercentage) 
FROM   @mAvailPercentages 
   
  SET @Score = @AggAvailability 
   
  -- do this only if we do not care about the score 
  IF (@ScoreOnly = 0) 
    BEGIN 
      -- get the network utilization exceeding count (bandwidth services) 
      -- and get the network utilization max count (bandwidth services) 
      DECLARE  @tmpCritical  TABLE( 
                                   [id]      INT    IDENTITY ( 1,1 )    PRIMARY KEY, 
                                   deviceid  INT, 
                                   interface NVARCHAR(250), 
                                   flag      SMALLINT 
                                   ) 
       
      -- these are all the records we care about 
      INSERT INTO @tmpCritical 
                 (deviceid, 
                  interface, 
                  flag) 
      SELECT   deviceid, 
               [monitored interface], 
               CASE 
                 WHEN [utilization %] &gt;= @CriticalUtilPct THEN 1 
                 WHEN [utilization %] &gt;= 100 THEN 2 
                 ELSE 0 
               END 
      FROM     fact_bandwidth 
      WHERE    customerid = @CustomerID 
               AND dateid &gt;= @StartDateID 
               AND dateid &lt;= @EndDateID 
      ORDER BY deviceid, 
               [monitored interface], 
               dateid, 
               timeid 
       
      INSERT INTO @tmpCritical 
                 (deviceid, 
                  interface, 
                  flag) 
      VALUES     (0, 
                  '', 
                  0) 
       
      DECLARE  @OverCriticalCount INT, 
               @At100Count        INT 
       
      DECLARE  @Id_Low  INT, 
               @Id_High INT 
       
      DECLARE  @temp INT 
       
      SET @OverCriticalCount = 0 
       
      SET @At100Count = 0 
       
      -- GET @OverCriticalCount ------------------------------------------------------ 
      SELECT TOP 1 @Id_Low = id 
      FROM     @tmpCritical 
      WHERE    flag = 1 
      ORDER BY id ASC 
       
      WHILE (@@ROWCOUNT &gt; 0) 
        BEGIN 
          SELECT   TOP 1 @Id_High = id 
          FROM     @tmpCritical 
          WHERE    id &gt; @Id_Low 
                   AND flag = 0 
          ORDER BY id ASC 
           
          IF @Id_High IS NULL 
            BREAK 
           
          SELECT   @temp = COUNT(* ) 
          FROM     @tmpCritical 
          WHERE    id &gt;= @Id_Low 
                   AND id &lt; @Id_High 
                   AND flag = 1 
          GROUP BY deviceid,interface 
           
          SET @OverCriticalCount = @OverCriticalCount + @@ROWCOUNT 
           
          SELECT   TOP 1 @Id_Low = id 
          FROM     @tmpCritical 
          WHERE    id &gt; @Id_High 
                   AND flag = 1 
          ORDER BY id ASC 
        END 
       
      -- GET @At100Count ------------------------------------------------------ 
      SELECT TOP 1 @Id_Low = id 
      FROM     @tmpCritical 
      WHERE    flag = 2 
      ORDER BY id ASC 
       
      WHILE (@@ROWCOUNT &gt; 0) 
        BEGIN 
          SELECT   TOP 1 @Id_High = id 
          FROM     @tmpCritical 
          WHERE    id &gt; @Id_Low 
                   AND flag = 0 
          ORDER BY id ASC 
           
          IF @@ROWCOUNT &lt; 1 
            BREAK 
           
          SELECT   @temp = COUNT(* ) 
          FROM     @tmpCritical 
          WHERE    id &gt;= @Id_Low 
                   AND id &lt; @Id_High 
                   AND flag = 2 
          GROUP BY deviceid,interface 
           
          SET @At100Count = @OverCriticalCount + @@ROWCOUNT 
           
          SELECT   TOP 1 @Id_Low = id 
          FROM     @tmpCritical 
          WHERE    id &gt; @Id_High 
                   AND flag = 2 
          ORDER BY id ASC 
        END 
       
      -- determine score 
      SELECT isnull(@AggAvailability,0)   AS [aggregated_network_availability], 
             isnull(@OverCriticalCount,0) AS [network_utilization_exceeded_critical_count], 
             isnull(@At100Count,0)        AS [network_utilization_max_count], 
             isnull(@Score,0)             AS score 
    END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Aggregated_Network_Availability">
          <DataField>aggregated_network_availability</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Network_Utilization_Exceeded_Critical_Count">
          <DataField>network_utilization_exceeded_critical_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Network_Utilization_Max_Count">
          <DataField>network_utilization_max_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Score">
          <DataField>score</DataField>
          <rd:TypeName>System.Single</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPTES_ManagedDevices_Score">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ScoreOnly">
            <Value>0</Value>
          </QueryParameter>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeManagedDevices.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Score">
            <Value>=Parameters!Score.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT 
				0 AS [Devices Managed Percentage],
				0 AS [Devices Managed Number],
				0 AS [Devices Unmanaged Percentage],
				0 AS [Devices Unmanaged Number],
				@Score AS [Score]
		END
	ELSE
		BEGIN
			-- get the true start and end date based on subscription or not
			SELECT @EndDate = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)
			
			-- get the enddateid
			DECLARE @EndDateID int
			SET @EndDateID = (SELECT DateID FROM dim_Date WHERE [Date] = @EndDate)

			-- get the total number of devices the customer had on the end date
			-- Determine the total number of notifications for all "selected" customers.
			DECLARE @TotalDevices int
			SET @TotalDevices = 
				(
					SELECT COUNT(*) 
					FROM dim_Device 
					WHERE 
							(CAST(FLOOR(CAST(CreatedOn AS FLOAT)) AS DATETIME)) &lt;= @EndDate 
						AND (
								   @EndDate &lt;= (CAST(FLOOR(CAST(DeletedOn AS FLOAT)) AS DATETIME)) 
								OR DeletedOn IS NULL
							)
						AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
						AND [IsProbe Device Indicator] = 'Not a Probe Device' 
						AND CustomerID = @CustomerID
						AND [DataSourceID] = (SELECT [DataSourceID] FROM dim_Customer WHERE CustomerID = @CustomerID)

				)

			-- get the total number of managed devices the customer had on the end date
			DECLARE @TotalManagedDevices int
			SET @TotalManagedDevices = 
				(
					SELECT COUNT(*) 
					FROM [Fact_Device Management History] 
					WHERE 
							([Date Device Management Started On] &lt;= @EndDateID) 
						AND (
								   @EndDateID &lt;= [Date Device Management Ended On] 
								OR [Date Device Management Ended On] IS NULL
							)
						AND CustomerID = @CustomerID
						AND DeviceID IN 
							(	
								SELECT DeviceID 
								FROM dim_Device 
								WHERE 
										(CAST(FLOOR(CAST(CreatedOn AS FLOAT)) AS DATETIME)) &lt;= @EndDate 
									AND (
											   @EndDate &lt;= (CAST(FLOOR(CAST(DeletedOn AS FLOAT)) AS DATETIME)) 
											OR DeletedOn IS NULL
										)
									AND [is managed device indicator] = 'Managed Device' 
									AND [Is Ignored Device Indicator] = 'Not an Ignored Device'
									AND [IsProbe Device Indicator] = 'Not a Probe Device' 
									AND CustomerID = @CustomerID
							)
				)

			-- get the total number of unmanaged devices the customer had on the end date
			-- NOTE: Managed + Unmanaged = Total
			DECLARE @TotalUnmanagedDevices int
			SET @TotalUnmanagedDevices = @TotalDevices - @TotalManagedDevices
			
			-- determine percentages
			DECLARE @pctTotalManagedDevices REAL
			DECLARE @pctTotalUnmanagedDevices REAL
			IF (@TotalDevices = 0)
			BEGIN
				-- to avoid divide by 0
				SET @pctTotalManagedDevices = 0
				SET @pctTotalUnmanagedDevices = 0
			END
			ELSE
			BEGIN
				SET @pctTotalManagedDevices = CAST(@TotalManagedDevices AS REAL) / CAST(@TotalDevices AS REAL) * 100
				SET @pctTotalUnmanagedDevices = CAST(@TotalUnmanagedDevices AS REAL) / CAST(@TotalDevices AS REAL) * 100
			END

			-- set the score
			SET @Score = @pctTotalManagedDevices

			-- determine score
			IF (@ScoreOnly = 0)
				BEGIN
					SELECT 
						--@TotalDevices AS [Total Number of Devices],
						@pctTotalManagedDevices AS [Devices Managed Percentage],
						@TotalManagedDevices AS [Devices Managed Number],
						@pctTotalUnmanagedDevices AS [Devices Unmanaged Percentage],
						@TotalUnmanagedDevices AS [Devices Unmanaged Number],
						@Score AS [Score]
				END
		END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Devices_Managed_Percentage">
          <DataField>Devices Managed Percentage</DataField>
        </Field>
        <Field Name="Devices_Managed_Number">
          <DataField>Devices Managed Number</DataField>
        </Field>
        <Field Name="Devices_Unmanaged_Percentage">
          <DataField>Devices Unmanaged Percentage</DataField>
        </Field>
        <Field Name="Devices_Unmanaged_Number">
          <DataField>Devices Unmanaged Number</DataField>
        </Field>
        <Field Name="Score">
          <DataField>Score</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="GetLargeBrandingImage">
      <Query>
        <DataSourceName>config</DataSourceName>
        <CommandType>StoredProcedure</CommandType>
        <CommandText>GetLargeBrandingImage</CommandText>
      </Query>
      <Fields>
        <Field Name="ImageLocation">
          <DataField>ImageLocation</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="GetStartDate">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription) AS UpdatedStartDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
