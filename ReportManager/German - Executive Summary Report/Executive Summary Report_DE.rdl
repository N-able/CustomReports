    <DataSet Name="ds_ManagedDevices_IncidentDetails">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeManagedDevicesDetails.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NumberOfDevicesToInclude">
            <Value>=Parameters!NumberOfDevicesToInclude.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
				-- create a table to store our results
	DECLARE  @mTmpTableFinal_MD_IncidentDetail TABLE
	(
		DeviceID int,
		[Device Name] nvarchar(200),
		[URI] nvarchar(500),
		[Number of Incidents] int,
		[Number of Closed Incidents] int,
		Processed bit
	)
	
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT
				DeviceID, 
				[Device Name] AS Device_Name, 
				URI, 
				[Number of Incidents] AS Number_of_Incidents,
				[Number of Closed Incidents] AS Number_of_Closed_Incidents 
			FROM @mTmpTableFinal_MD_IncidentDetail
			WHERE 1=2
		END
	ELSE
		BEGIN
			DECLARE @tmpText NVARCHAR(200), @IncidentID INTEGER, @Processed INTEGER, @MTTR DATETIME, @MTTA DATETIME, @Closed BIT
			DECLARE @Start_Date datetime
			DECLARE @End_Date datetime

			-- get the true start and end date based on subscription or not
			SELECT @Start_Date = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
			SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

			DECLARE @mTmpTableReduced TABLE(
				IncidentID INTEGER,
				[Incident Devices] NVARCHAR(200),
				[Incident Services] NVARCHAR(200),
				Closed BIT
			)
			INSERT INTO @mTmpTableReduced 
			SELECT [N-central IncidentID], [Incident Devices], [Incident Services], CASE WHEN [Mean Time To Resolution] IS NULL THEN 0 ELSE 1 END
			FROM fact_Incident 
			WHERE CustomerID = @CustomerID
			AND @Start_Date &lt;= [Time Incident Opened] AND [Time Incident Opened] &lt;= @End_Date


			-- Get all the incidents for this time period, where an incident is any incident which is opened during the period
			DECLARE @mTmpTableMultipleDevices TABLE (
				IncidentID INTEGER, 
				Devices NVARCHAR(200), 
				Closed BIT,
				Processed INTEGER
			)
			-- declare a temp table to store incidentid/deviceid so that we can split up the CSV stored in Incident Devices
			DECLARE @mTmpTableSingleDevices TABLE (
				IncidentID INTEGER,
				DeviceID INTEGER,
				Closed BIT
			)

			-- First take care of only those with multiple devices
			INSERT INTO @mTmpTableMultipleDevices 
			SELECT [IncidentID], [Incident Devices], Closed, -1 
			FROM @mTmpTableReduced  
			WHERE [Incident Devices] LIKE '%,%'

			-- don't forget the ones that do not have multiple devices
			INSERT INTO @mTmpTableSingleDevices
			SELECT [IncidentID], [Incident Devices], Closed
			FROM @mTmpTableReduced 
			WHERE [Incident Devices] NOT LIKE '%,%'
						
			-- loop through all the different incidents and break out the deviceids
			SELECT TOP 1 @IncidentID = IncidentID, @tmpText = Devices, @Closed = Closed, @Processed = Processed FROM @mTmpTableMultipleDevices
			WHILE (@Processed = -1)
			BEGIN
				INSERT INTO @mTmpTableSingleDevices SELECT @IncidentID, Value, @Closed FROM dbo.inline_split_me(@tmpText)
				UPDATE @mTmpTableMultipleDevices SET Processed = 1 WHERE incidentid = @IncidentID
				SELECT TOP 1 @IncidentID = IncidentID, @tmpText = Devices, @Closed = Closed, @Processed = Processed 
				FROM @mTmpTableMultipleDevices ORDER BY Processed ASC
			END

			-- assign a URI to each incident/device combo
			CREATE TABLE #mTmpTableIncidentsWithURI(
				IncidentID int,
				DeviceID int,
				DeviceName nvarchar(200),
				URI nvarchar(500),
				Closed bit,
				Status nvarchar(200)
			)

			INSERT INTO #mTmpTableIncidentsWithURI
			SELECT IncidentID, a.DeviceID, b.DeviceName, b.URI, Closed, Status
			FROM @mTmpTableSingleDevices a INNER JOIN dim_Device b ON a.DeviceID = b.DeviceID 
			/* Added 3.0 17th Sept, 2008*/
			Where	b.[is managed device indicator] = 'Managed Device' 
					AND b.[Is Ignored Device Indicator] = 'Not an Ignored Device'

			-- Honour the ShowDeleted flag (never show deleted devices in Managed section)
			DELETE FROM #mTmpTableIncidentsWithURI WHERE [Status] != 'Active'

			-- Figure out which incidents are incident based
			CREATE INDEX tmpIDX_TmpTableIncidentsWithURI ON #mTmpTableIncidentsWithURI (DeviceID)

		DECLARE @NumOfDevicesToInclude int			
		SET @NumOfDevicesToInclude=ISNULL(@NumberOfDevicesToInclude,0)

			IF @NumOfDevicesToInclude&gt;=0
				SET ROWCOUNT @NumOfDevicesToInclude

			INSERT INTO @mTmpTableFinal_MD_IncidentDetail
				SELECT DeviceID, DeviceName, URI, COUNT(*) AS [Number Of Incidents], 0, 0
				FROM #mTmpTableIncidentsWithURI
				GROUP BY DeviceID, DeviceName, URI
				ORDER BY [Number Of Incidents] DESC


			DECLARE @DeviceID int
			SELECT TOP 1 @DeviceID = DeviceID, @Processed = Processed FROM @mTmpTableFinal_MD_IncidentDetail
			WHILE (@Processed = 0)
				BEGIN
					UPDATE @mTmpTableFinal_MD_IncidentDetail SET Processed = 1, [Number of Closed Incidents] = 
										(SELECT COUNT(*) FROM #mTmpTableIncidentsWithURI WHERE DeviceID = @DeviceID AND Closed = 1)
						WHERE DeviceID = @DeviceID
					SELECT TOP 1 @DeviceID = DeviceID, @Processed = Processed FROM @mTmpTableFinal_MD_IncidentDetail ORDER BY Processed ASC
				END

			-- cleanup
			DROP TABLE #mTmpTableIncidentsWithURI

	SELECT
	DeviceID, 
	[Device Name] AS Device_Name, 
	URI, 
	[Number of Incidents] AS Number_of_Incidents,
	[Number of Closed Incidents] AS Number_of_Closed_Incidents 
	FROM @mTmpTableFinal_MD_IncidentDetail
		END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DeviceID">
          <DataField>DeviceID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Device_Name">
          <DataField>Device_Name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="URI">
          <DataField>URI</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Number_of_Incidents">
          <DataField>Number_of_Incidents</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Number_of_Closed_Incidents">
          <DataField>Number_of_Closed_Incidents</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPTES_SecurityMonitoring_AVDetails">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!AVMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @End_Date datetime
-- Get the start and end dates for the report
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT
				NULL AS [Device_Class], NULL AS DeviceName, NULL AS URI
			WHERE 1=2
		END
	ELSE
BEGIN
	SET @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

	-- Create the date variables for reuse in the database filter.
	DECLARE @End_DateIndex int
	SELECT @End_DateIndex = dbo.dateIndex(@End_Date)

	-- Bug 42758 - improving performance
	IF (OBJECT_ID('tempdb..#tmp_Customer') IS NOT NULL)
		DROP TABLE #tmp_Customer
		
	IF (OBJECT_ID('tempdb..#tmp_NonAVDevice') IS NOT NULL)
		DROP TABLE #tmp_NonAVDevice
		
	IF (OBJECT_ID('tempdb..#tmp_WindowsLogEventServiceID') IS NOT NULL)
		DROP TABLE #tmp_WindowsLogEventServiceID
		
	IF (OBJECT_ID('tempdb..#tmp_LogAnalysisServiceID') IS NOT NULL)
		DROP TABLE #tmp_LogAnalysisServiceID
		
	IF (OBJECT_ID('tempdb..#tmp_LogAnalysisAppendServiceID') IS NOT NULL)
		DROP TABLE #tmp_LogAnalysisAppendServiceID
				
	CREATE TABLE #tmp_Customer (DataSourceID int, CustomerID int)
	CREATE TABLE #tmp_NonAVDevice (DeviceID int)
	
	-- Create a list of N-central CustomerID based on a list of dim_CustomerIDs
	INSERT INTO #tmp_Customer 
	SELECT [DataSourceID], [N-central CustomerID] CustomerID FROM dim_Customer 
	WHERE CustomerID IN (SELECT Value FROM inline_split_me(@CustomerID))

	-- Check if showing deleted devices
        DECLARE @ShowDeleted nvarchar(30) 	 
        SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')

	CREATE TABLE #tmp_WindowsLogEventServiceID  ( WinIDs int)
	CREATE TABLE #tmp_LogAnalysisServiceID  ( LogIDs int)
	CREATE TABLE #tmp_LogAnalysisAppendServiceID  ( LogIDs int)
	
	-- get all windows event log service ids in each datasource
	Insert into #tmp_WindowsLogEventServiceID 
	select serviceid
	from dbo.dim_Service
	where datasourceid in (select [DataSourceID] from #tmp_Customer )
	 and (ServiceName = 'Windows Event Log')

	-- get all log analysis service ids in each datasource 
	Insert into #tmp_LogAnalysisServiceID 
	select serviceid
	from dbo.dim_Service
	where datasourceid in (select [DataSourceID] from #tmp_Customer )
	 and (ServiceName = 'Log Analysis (Batch)')

	-- get all log analysis (Appended) service ids in each datasource 
	Insert into #tmp_LogAnalysisAppendServiceID 
	select serviceid
	from dbo.dim_Service
	where datasourceid in (select [DataSourceID] from #tmp_Customer )
	 and (ServiceName = 'Log Analysis (Appended)')

	-- Insert ALL Windows Devices
	INSERT INTO #tmp_NonAVDevice
	SELECT DISTINCT d.DeviceID FROM dim_Device d
	INNER JOIN #tmp_Customer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
	WHERE (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS FLOAT ) ) AS DATETIME )) OR DeletedOn IS NULL)
		AND [Device Class] IN ('Windows Server', 'Windows Workstation','Windows Laptop') 
		AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
		AND d.[Is Managed Device Indicator] = 'Managed Device'
		AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
		AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'

	DELETE FROM #tmp_NonAVDevice WHERE DeviceID IN (
		SELECT DeviceID
		FROM dbo.[fact_Device Service Parameters]  dsp
		WHERE dsp.serviceid in (Select WinIDs from #tmp_WindowsLogEventServiceID )
		and dsp.CustomerID in  (SELECT Value FROM inline_split_me(@CustomerID)) 				
		and (dsp.[Parameter Name] = 'SourceIncludeFilter')
		and (dsp.[Parameter Value] in (Select [ParameterValue] from config.dbo.[AntivirusProducts] where [ParameterName] = 'Event Source Include Filter' ) ) 
		)
	DELETE FROM #tmp_NonAVDevice WHERE DeviceID IN (
		SELECT DeviceID
		FROM dbo.[fact_Device Service Parameters] dsp
		WHERE dsp.serviceid in (Select LogIDs from #tmp_LogAnalysisServiceID )
		and dsp.CustomerID in  (SELECT Value FROM inline_split_me(@CustomerID)) 				
		and (dsp.[Parameter Name] = 'FileSpec') 
		and (dsp.[Parameter Value] in (Select [ParameterValue] from config.dbo.[AntivirusProducts] where [ParameterName] = 'Log File Name and Path' ) ) 
		) 

	DELETE FROM #tmp_NonAVDevice WHERE DeviceID IN (
		SELECT DeviceID
		FROM dbo.[fact_Device Service Parameters] dsp
		WHERE dsp.serviceid in (Select LogIDs from #tmp_LogAnalysisAppendServiceID )
		and dsp.CustomerID in  (SELECT Value FROM inline_split_me(@CustomerID)) 				
		and (dsp.[Parameter Name] = 'FileSpec') 
		and (dsp.[Parameter Value] in (Select [ParameterValue] from config.dbo.[AntivirusProducts] where [ParameterName] = 'Log File Name and Path' ) ) 
		) 

	-- Remove Antivirus Devices From #tmp_NonAVDevice
	DELETE #tmp_NonAVDevice FROM #tmp_NonAVDevice n
	INNER JOIN dim_Device d ON d.DeviceID = n.DeviceID
	INNER JOIN [fact_Device Services] ON d.DeviceID = [fact_Device Services].DeviceID 
	INNER JOIN [dim_Service] ON [fact_Device Services].ServiceID = dim_Service.ServiceID
	INNER JOIN #tmp_Customer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
	WHERE (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS FLOAT ) ) AS DATETIME )) OR DeletedOn IS NULL)
		AND [ServiceName] LIKE 'AV %' 
		AND [Device Class] IN ('Windows Server', 'Windows Workstation','Windows Laptop') 
		AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
		AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
		AND [Date Monitoring Started On] &lt;= @End_Date
		AND [Date Monitoring Stopped On] &gt; @End_Date
		AND d.[Is Managed Device Indicator] = 'Managed Device'
		AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'

	DELETE #tmp_NonAVDevice FROM #tmp_NonAVDevice n
	INNER JOIN  dim_Device d  ON d.DeviceID = n.DeviceID
	INNER JOIN	[fact_Device Services] ON d .DeviceID = [fact_Device Services].DeviceID 
	INNER JOIN	[dim_Service] ON [fact_Device Services].ServiceID = dim_Service.ServiceID 
	INNER JOIN	[fact_Device Service Parameters] ON [fact_device Service Parameters].Serviceid = [fact_Device Services].ServiceID 
	INNER JOIN #tmp_Customer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
	WHERE (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS FLOAT ) ) AS DATETIME )) OR DeletedOn IS NULL)
	AND [ServiceName] LIKE 'Endpoint Security%'
	AND [Device Class] IN ('Windows Server', 'Windows Workstation', 'Windows Laptop')
	AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows') 
	AND [fact_Device Service Parameters].[Parameter Name] = 'Detection_Type_List'
	AND [fact_Device Service Parameters].[Parameter Value] LIKE '%AV%'
	AND ([fact_Device Service Parameters].[Parameter In Effect Started On] &lt;= @End_Date)
	AND ([fact_Device Service Parameters].[Parameter In Effect Ended On] &gt; @End_Date
		OR [fact_Device Service Parameters].[Parameter In Effect Ended On] is null)
	AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
		 OR @ShowDeleted &lt;&gt; 'False')
	AND [Date Monitoring Started On] &lt;= @End_Date 
	AND [Date Monitoring Stopped On] &gt; @End_Date 
	AND d.[Is Managed Device Indicator] = 'Managed Device' 
	AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'
	
	SELECT d.[Device Class] AS Device_Class, d.DeviceName, d.URI
	FROM #tmp_NonAVDevice n
	INNER JOIN dim_Device d ON d.DeviceID = n.DeviceID
	WHERE  
			d.[Is Managed Device Indicator] = 'Managed Device'
		AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'
		AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
			 OR @ShowDeleted &lt;&gt; 'False')	
	ORDER BY d.[Device Class], d.DeviceName
END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Device_Class">
          <DataField>Device_Class</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DeviceName">
          <DataField>DeviceName</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="URI">
          <DataField>URI</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPTES_SecurityMonitoring_AVSummary">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!AVMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			DECLARE  @tmpReturnStructure TABLE 
				(
				[Devices Monitored Percent] float,
				[Devices Monitored Count] int, 
				[Devices Unmonitored Percent] float,
				[Devices Unmonitored Count] int
				)
			SELECT * FROM @tmpReturnStructure
		END
	ELSE
	BEGIN
		DECLARE @End_Date datetime
		-- Get the start and end dates for the report
		SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

		-- Create the date variables for reuse in the database filter.
		DECLARE @End_DateIndex int
		SELECT @End_DateIndex = dbo.dateIndex(@End_Date)


		DECLARE @mCustomer TABLE (DataSourceID int, CustomerID int)
		
		-- Create a list of N-central CustomerID based on a list of dim_CustomerIDs
		INSERT INTO @mCustomer 
		SELECT [DataSourceID], [N-central CustomerID] CustomerID FROM dim_Customer 
		WHERE CustomerID IN (SELECT Value FROM inline_split_me(@CustomerID))

		declare @WindowsLogEventServiceID table ( WinIDs int)
		declare @LogAnalysisServiceID table ( LogIDs int)
		declare @LogAnalysisAppendServiceID table ( LogIDs int)
		
		-- get all windows event log service ids in each datasource
		Insert into @WindowsLogEventServiceID 
		select serviceid
		from dbo.dim_Service
		where datasourceid in (select [DataSourceID] from @mCustomer )
		 and (ServiceName = 'Windows Event Log')

		-- get all log analysis service ids in each datasource 
		Insert into @LogAnalysisServiceID 
		select serviceid
		from dbo.dim_Service
		where datasourceid in (select [DataSourceID] from @mCustomer )
		 and (ServiceName = 'Log Analysis (Batch)')

		-- get all log analysis service (Appended) ids in each datasource 
		Insert into @LogAnalysisAppendServiceID 
		select serviceid
		from dbo.dim_Service
		where datasourceid in (select [DataSourceID] from @mCustomer )
		 and (ServiceName = 'Log Analysis (Appended)')

		-- Check if showing deleted devices
			DECLARE @ShowDeleted nvarchar(30) 	 
			SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')

		
		--
		DECLARE @TotalWindowsDevices int
		DECLARE @TotalAntivirusDevices int
		SET @TotalWindowsDevices = 0
		SET @TotalAntivirusDevices = 0

		-- Get Total Windows Devices Count
		SELECT @TotalWindowsDevices = COUNT(DISTINCT d.DeviceID)
		FROM dim_Device d
		INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
		WHERE (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS FLOAT ) ) AS DATETIME )) OR DeletedOn IS NULL)
			AND [Device Class] IN ('Windows Server', 'Windows Workstation','Windows Laptop') 
			AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
			AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
				 OR @ShowDeleted &lt;&gt; 'False')
			AND [Is Managed Device Indicator] = 'Managed Device'
			AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'

		DECLARE @mAvDevices TABLE (Deviceid int) 		
			
		--Get all custom antivirus devices monitored by windows event log services
		INSERT INTO @mAvDevices 
		SELECT DISTINCT dsp.DeviceID
		FROM 
			dbo.[fact_Device Service Parameters]  dsp
				INNER JOIN 
			dbo.dim_Device d 
					ON d.DeviceID=dsp.DeviceID
		WHERE 
				dsp.serviceid IN (SELECT WinIDs FROM @WindowsLogEventServiceID )
			AND (CAST( FLOOR( CAST( d.CreatedOn AS FLOAT )) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( d.DeletedOn AS FLOAT )) AS DATETIME )) OR d.DeletedOn IS NULL)			
			AND dsp.CustomerID IN  (SELECT [Value] FROM inline_split_me(@CustomerID)) 		
			AND (dsp.[Parameter Name] = 'SourceIncludeFilter')
			AND (dsp.[Parameter Value] IN (SELECT [ParameterValue] FROM config.dbo.[AntivirusProducts] WHERE [ParameterName] = 'Event Source Include Filter' )) 
			AND dsp.DeviceID NOT IN (SELECT deviceid FROM @mAvDevices)

		--Get all custom antivirus devices monitored by log analysis(batch) services

		INSERT INTO @mAvDevices 
			SELECT DISTINCT dsp.DeviceID
			FROM 
				dbo.[fact_Device Service Parameters] dsp
					INNER JOIN 
				dbo.dim_Device d 
						ON d.DeviceID=dsp.DeviceID		
			WHERE 
					dsp.serviceid IN (SELECT LogIDs FROM @LogAnalysisServiceID )
				AND (CAST( FLOOR( CAST( d.CreatedOn AS FLOAT )) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( d.DeletedOn AS FLOAT )) AS DATETIME )) OR d.DeletedOn IS NULL)			
				AND dsp.CustomerID IN  (SELECT [Value] FROM inline_split_me(@CustomerID)) 
				AND (dsp.[Parameter Name] = 'FileSpec') 
				AND (dsp.[Parameter Value] IN (SELECT [ParameterValue] FROM config.dbo.[AntivirusProducts] WHERE [ParameterName] = 'Log File Name and Path' ))
				AND dsp.DeviceID NOT IN (SELECT deviceid FROM @mAvDevices)

		--Get all custom antivirus devices monitored by log analysis(appended) services

		INSERT INTO @mAvDevices 
			SELECT DISTINCT dsp.DeviceID
			FROM 
				dbo.[fact_Device Service Parameters] dsp
					INNER JOIN 
				dbo.dim_Device d 
						ON d.DeviceID=dsp.DeviceID				
			WHERE 
					dsp.serviceid IN (SELECT LogIDs FROM @LogAnalysisAppendServiceID )
				AND (CAST( FLOOR( CAST( d.CreatedOn AS FLOAT )) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( d.DeletedOn AS FLOAT )) AS DATETIME )) OR d.DeletedOn IS NULL)			
				AND dsp.CustomerID IN  (SELECT [Value] FROM inline_split_me(@CustomerID)) 
				AND (dsp.[Parameter Name] = 'FileSpec') 
				AND (dsp.[Parameter Value] IN (SELECT [ParameterValue] FROM config.dbo.[AntivirusProducts] WHERE [ParameterName] = 'Log File Name and Path' ))
				AND dsp.DeviceID NOT IN (SELECT deviceid FROM @mAvDevices)

		-- Get Total Antivirus Devices Count
		
		INSERT INTO @mAvDevices 
		SELECT DISTINCT d.DeviceID
		FROM dim_Device d
		INNER JOIN [fact_Device Services] ON d.DeviceID = [fact_Device Services].DeviceID 
		INNER JOIN [dim_Service] ON [fact_Device Services].ServiceID = dim_Service.ServiceID
		INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
		WHERE (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS FLOAT ) ) AS DATETIME )) OR DeletedOn IS NULL)
			AND [ServiceName] LIKE 'AV %' 
			AND [Device Class] IN ('Windows Server', 'Windows Workstation','Windows Laptop') 
			AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
			AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
				 OR @ShowDeleted &lt;&gt; 'False')
			AND [Date Monitoring Started On] &lt;= @End_Date
			AND [Date Monitoring Stopped On] &gt; @End_Date 
			AND [Is Managed Device Indicator] = 'Managed Device'
			AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'
			and d.DeviceID not in (select deviceid from @mAvDevices)		
			
		INSERT INTO @mAvDevices 
		SELECT DISTINCT d.DeviceID
		FROM dim_Device d
		INNER JOIN [fact_Device Services] ON d.DeviceID = [fact_Device Services].DeviceID 
		INNER JOIN [dim_Service] ON [fact_Device Services].ServiceID = dim_Service.ServiceID
		INNER JOIN	[fact_Device Service Parameters] ON [fact_device Service Parameters].Serviceid = [fact_Device Services].ServiceID 
		INNER JOIN @mCustomer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
		WHERE (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS FLOAT ) ) AS DATETIME )) OR DeletedOn IS NULL)
		AND [ServiceName] LIKE 'Endpoint Security%'
		AND [Device Class] IN ('Windows Server', 'Windows Workstation', 'Windows Laptop')
		AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows') 
		AND [fact_Device Service Parameters].[Parameter Name] = 'Detection_Type_List'
		AND [fact_Device Service Parameters].[Parameter Value] LIKE '%AV%'
		AND ([fact_Device Service Parameters].[Parameter In Effect Started On] &lt;= @End_Date)
		AND ([fact_Device Service Parameters].[Parameter In Effect Ended On] &gt; @End_Date
			OR [fact_Device Service Parameters].[Parameter In Effect Ended On] is null)		
		AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
			 OR @ShowDeleted &lt;&gt; 'False')
		AND [Date Monitoring Started On] &lt;= @End_Date 
		AND [Date Monitoring Stopped On] &gt; @End_Date 
		AND [Is Managed Device Indicator] = 'Managed Device' 
		AND d .[Is Ignored Device Indicator] = 'Not an Ignored Device'
		AND d.deviceID NOT IN (select deviceid from @mAvDevices)	
			
		SELECT @TotalAntivirusDevices = COUNT(*) 
		FROM @mAvDevices a		
		INNER JOIN  dbo.dim_device d
			 ON a.Deviceid=d.DeviceID
			 AND ((@ShowDeleted = 'False' AND d.[status] = 'Active')
				 OR @ShowDeleted &lt;&gt; 'False')
			 AND (d.[Is managed device indicator] = 'Managed Device')
			 AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
					
		IF @TotalWindowsDevices = 0
			SELECT 0 AS [Devices Monitored Percent], 0 AS [Devices Monitored Count], 0 AS [Devices Unmonitored Percent], 0 AS [Devices Unmonitored Count]
		ELSE	
			SELECT CAST(@TotalAntivirusDevices AS FLOAT)/CAST(@TotalWindowsDevices AS FLOAT)*100 AS [Devices Monitored Percent], 
				@TotalAntivirusDevices AS [Devices Monitored Count], 
				(CAST(@TotalWindowsDevices AS FLOAT)-CAST(@TotalAntivirusDevices AS FLOAT))/CAST(@TotalWindowsDevices AS FLOAT)*100 AS [Devices Unmonitored Percent], 
				(@TotalWindowsDevices-@TotalAntivirusDevices) AS [Devices Unmonitored Count]
	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Devices_Monitored_Percent">
          <DataField>Devices Monitored Percent</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Devices_Monitored_Count">
          <DataField>Devices Monitored Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Devices_Unmonitored_Percent">
          <DataField>Devices Unmonitored Percent</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Devices_Unmonitored_Count">
          <DataField>Devices Unmonitored Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPTES_SecurityMonitoring_FirewallDetails">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!FirewallMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PeriodType">
            <Value>=Parameters!PeriodType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @mFirewallDetails TABLE (
			[DeviceID]		int,
			[DeviceName]		nvarchar(100),
			[Severity]		nvarchar(100),
			[Overall Daily Average]	float,
			[Highest Daily Count]	int,
			[Lowest Daily Count]	int,
			[Std Deviation]		float,
			[Trend]			int,
			[Period]		bit, -- Period: 0 - Current, 1 - Previous
			[OrderIndex]		smallint) 
			
-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT [DeviceID], [DeviceName], [Severity], [Overall Daily Average], 
				[Highest Daily Count], [Lowest Daily Count], [Std Deviation], [Trend], [OrderIndex]
			FROM @mFirewallDetails
			WHERE 1=2		
		END
	ELSE
	BEGIN
		DECLARE @Start_Date datetime
		DECLARE @End_Date datetime
		-- Get the start and end dates for the report
		SELECT @Start_Date = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
		SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

		-- Create the date variables for reuse in the database filter.
		DECLARE @Start_DateIndex int, @End_DateIndex int
		SELECT @Start_DateIndex = dbo.dateIndex(@Start_Date)
		SELECT @End_DateIndex = dbo.dateIndex(@End_Date)
		DECLARE @mCustomer TABLE(DataSourceID int, CustomerID int)
		
		-- Create a list of N-central CustomerID based on a list of dim_CustomerIDs
		INSERT INTO @mCustomer 
		SELECT [DataSourceID], [N-central CustomerID] CustomerID FROM dim_Customer 
		WHERE CustomerID IN (SELECT Value FROM inline_split_me(@CustomerID))

		-- Check if showing deleted devices
			DECLARE @ShowDeleted nvarchar(30) 	 
			SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')
		--
		DECLARE @mDailyCount TABLE (
			[Date]			datetime,
			[DeviceID]		int,
			[DeviceName]		nvarchar(100),
			[Emergency]		int,
			[Alert]			int,
			[Critical]		int,
			[Error]			int,
			[Warning]		int)


		DECLARE @Counter smallint
		SET @Counter = 0

	SP_LOOP:	
		-- Clean @mDailyCount table
		DELETE FROM @mDailyCount

		INSERT INTO @mDailyCount
		SELECT dim_Date.Date AS [Date], dim_Device.DeviceID, dim_Device.DeviceName,
			ISNULL(SUM(i.[Emergency Incidents Detected]), 0), 
			ISNULL(SUM(i.[Alert Incidents Detected]), 0), 
			ISNULL(SUM(i.[Critical Incidents Detected]), 0), 
			ISNULL(SUM(i.[Error Incidents Detected]), 0), 
			ISNULL(SUM(i.[Warning Incidents Detected]), 0)
		FROM [fact_SEM_Firewall Incidents] i 
		INNER JOIN [dim_Service] ON i.ServiceID = dim_Service.ServiceID
		INNER JOIN [dim_Device] ON i.DeviceID = dim_Device.DeviceID
		INNER JOIN @mCustomer c ON dim_Device.[N-central CustomerID] = c.CustomerID AND dim_Device.[DataSourceID] = c.[DataSourceID]
		INNER JOIN dim_Date ON i.DateID = dim_Date.DateID
		WHERE dim_Device.[Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows') 
			AND dim_Service.[ServiceName] NOT LIKE 'FW-Chk Point'
			AND ((@ShowDeleted = 'False' AND dim_Device.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
			AND (i.DateID &gt;= @Start_DateIndex) AND (i.DateID &lt;= @End_DateIndex)
			AND (dim_Device.[Is Managed Device Indicator] = 'Managed Device')
			AND (dim_Device.[Is Ignored Device Indicator] = 'Not an Ignored Device')
			 
		GROUP BY dim_Device.DeviceID, dim_Device.DeviceName, dim_Date.Date


		INSERT INTO @mFirewallDetails
		SELECT [DeviceID], [DeviceName], 'Notfall' AS [Emergency], 
			AVG([Emergency]), MAX([Emergency]), MIN([Emergency]), STDEV([Emergency]), 0, @Counter, 0
		FROM @mDailyCount
		GROUP BY DeviceID, DeviceName

		INSERT INTO @mFirewallDetails
		SELECT [DeviceID], [DeviceName], 'Alarm' AS [Alert], 
			AVG([Alert]), MAX([Alert]), MIN([Alert]), STDEV([Alert]), 0, @Counter, 1
		FROM @mDailyCount
		GROUP BY DeviceID, DeviceName

		INSERT INTO @mFirewallDetails
		SELECT [DeviceID], [DeviceName], 'Kritisch' AS [Critical], 
			AVG([Critical]), MAX([Critical]), MIN([Critical]), STDEV([Critical]), 0, @Counter, 2
		FROM @mDailyCount
		GROUP BY DeviceID, DeviceName

		INSERT INTO @mFirewallDetails
		SELECT [DeviceID], [DeviceName], 'Fehler' AS [Error], 
			AVG([Error]), MAX([Error]), MIN([Error]), STDEV([Error]), 0, @Counter, 3
		FROM @mDailyCount
		GROUP BY DeviceID, DeviceName

		INSERT INTO @mFirewallDetails
		SELECT [DeviceID], [DeviceName], 'Warnung' AS [Warning], 
			AVG([Warning]), MAX([Warning]), MIN([Warning]), STDEV([Warning]), 0, @Counter, 4
		FROM @mDailyCount
		GROUP BY DeviceID, DeviceName

		IF @Counter = 0
		BEGIN
			-- Set Previous Date Index
			SELECT @Start_DateIndex = dbo.dateIndex(dbo.GetPreviousDate(@Start_Date, @PeriodType))
			SELECT @End_DateIndex = dbo.dateIndex(dbo.GetPreviousDate(@End_Date, @PeriodType))
			SET @Counter = 1
			GOTO SP_LOOP
		END
		-- Update Trend
		UPDATE f1 SET [Trend] = CASE 
			WHEN f1.[Overall Daily Average] &gt; f2.[Overall Daily Average] THEN 1 
			WHEN f1.[Overall Daily Average] &lt; f2.[Overall Daily Average] THEN -1
			ELSE 0 END 
		FROM @mFirewallDetails f1
		INNER JOIN @mFirewallDetails f2 
		ON f1.DeviceID = f2.DeviceID AND f1.Severity = f2.Severity
		WHERE f1.[Period] = 0 AND f2.[Period] = 1

		-- Return Firewall Details table
		SELECT [DeviceID], [DeviceName], [Severity], [Overall Daily Average], 
			[Highest Daily Count], [Lowest Daily Count], [Std Deviation], [Trend], [OrderIndex]
		FROM @mFirewallDetails
		WHERE [Period] = 0
	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DeviceID">
          <DataField>DeviceID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DeviceName">
          <DataField>DeviceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Severity">
          <DataField>Severity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Overall_Daily_Average">
          <DataField>Overall Daily Average</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Highest_Daily_Count">
          <DataField>Highest Daily Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Lowest_Daily_Count">
          <DataField>Lowest Daily Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Std_Deviation">
          <DataField>Std Deviation</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Trend">
          <DataField>Trend</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="OrderIndex">
          <DataField>OrderIndex</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPTES_SecurityMonitoring_FirewallGraph">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!FirewallMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT 
				NULL AS [Date], 
				NULL AS [Emergency], 
				NULL AS [Alert], 
				NULL AS [Critical], 
				NULL AS [Error], 
				NULL AS [Warning]			
			WHERE 1=2
		END
	ELSE
	BEGIN
	DECLARE  @Start_Date datetime
	DECLARE	 @End_Date datetime
	-- Get the start and end dates for the report
	SELECT @Start_Date = [dbo].GetStartDate(@StartDate, GETDATE(), @Subscription)
	SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

	-- Create the date variables for reuse in the database filter.
	DECLARE @Start_DateIndex int, @End_DateIndex int
	DECLARE @Interval nvarchar(20)
	SELECT @Start_DateIndex = dbo.dateIndex(@Start_Date)
	SELECT @End_DateIndex = dbo.dateIndex(@End_Date)
	SET @Interval = 'Daily'

	DECLARE @mCustomer TABLE (DataSourceID int, CustomerID int)
	
	-- Create a list of N-central CustomerID based on a list of dim_CustomerIDs
	INSERT INTO @mCustomer 
	SELECT [DataSourceID], [N-central CustomerID] CustomerID FROM dim_Customer 
	WHERE CustomerID IN (SELECT Value FROM inline_split_me(@CustomerID))

	-- Check if showing deleted devices
        DECLARE @ShowDeleted nvarchar(30) 	 
        SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')
	--
	DECLARE @RecordCount int
	DECLARE @tempDateTable Table ([Date] datetime)
	
	INSERT INTO @tempDateTable
	SELECT dim_Date.Date
	FROM [fact_SEM_Firewall Incidents] i 
	INNER JOIN [dim_Service] ON i.ServiceID = dim_Service.ServiceID
	INNER JOIN [dim_Device] ON i.DeviceID = dim_Device.DeviceID
	INNER JOIN @mCustomer c ON dim_Device.[N-central CustomerID] = c.CustomerID AND dim_Device.[DataSourceID] = c.[DataSourceID]
	INNER JOIN dim_Date ON i.DateID = dim_Date.DateID
	WHERE dim_Device.[Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows') 
		AND dim_Service.[ServiceName] NOT LIKE 'FW-Chk Point'
		AND ((@ShowDeleted = 'False' AND dim_Device.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
		AND (i.DateID &gt;= @Start_DateIndex) AND (i.DateID &lt;= @End_DateIndex)
		AND (dim_Device.[Is Managed Device Indicator] = 'Managed Device')
		AND (dim_Device.[Is Ignored Device Indicator] = 'Not an Ignored Device')
	GROUP BY dim_Date.Date

	SELECT @RecordCount = COUNT(*) FROM @tempDateTable
	IF (@RecordCount &gt; 30)
		SET @Interval = 'Weekly'
		
	IF (@Interval = 'Weekly')
	BEGIN
		DELETE FROM @tempDateTable
		
		INSERT INTO @tempDateTable
		SELECT MIN(dim_Date.Date)
		FROM [fact_SEM_Firewall Incidents] i 
		INNER JOIN [dim_Service] ON i.ServiceID = dim_Service.ServiceID
		INNER JOIN [dim_Device] ON i.DeviceID = dim_Device.DeviceID
		INNER JOIN @mCustomer c ON dim_Device.[N-central CustomerID] = c.CustomerID AND dim_Device.[DataSourceID] = c.[DataSourceID]
		INNER JOIN dim_Date ON i.DateID = dim_Date.DateID
		WHERE dim_Device.[Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows') 
			AND dim_Service.[ServiceName] NOT LIKE 'FW-Chk Point'
			AND ((@ShowDeleted = 'False' AND dim_Device.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
			AND (i.DateID &gt;= @Start_DateIndex) AND (i.DateID &lt;= @End_DateIndex)
			AND (dim_Device.[Is Managed Device Indicator] = 'Managed Device')
			AND (dim_Device.[Is Ignored Device Indicator] = 'Not an Ignored Device')
		GROUP BY dim_Date.[Calendar Year], dim_Date.[Week Number In Year]
	
		SELECT @RecordCount = COUNT(*) FROM @tempDateTable
		IF (@RecordCount &gt; 30)
			SET @Interval = 'Monthly'
	END
	
	IF (@Interval = 'Weekly')
	BEGIN
		SELECT MIN(dim_Date.Date) AS [Date], 
			SUM(i.[Emergency Incidents Detected]) AS [Emergency], 
			SUM(i.[Alert Incidents Detected]) AS [Alert], 
			SUM(i.[Critical Incidents Detected]) AS [Critical], 
			SUM(i.[Error Incidents Detected]) AS [Error], 
			SUM(i.[Warning Incidents Detected]) AS [Warning]
		FROM [fact_SEM_Firewall Incidents] i 
		INNER JOIN [dim_Service] ON i.ServiceID = dim_Service.ServiceID
		INNER JOIN [dim_Device] ON i.DeviceID = dim_Device.DeviceID
		INNER JOIN @mCustomer c ON dim_Device.[N-central CustomerID] = c.CustomerID AND dim_Device.[DataSourceID] = c.[DataSourceID]
		INNER JOIN dim_Date ON i.DateID = dim_Date.DateID
		WHERE dim_Device.[Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows') 
			AND dim_Service.[ServiceName] NOT LIKE 'FW-Chk Point'
			AND ((@ShowDeleted = 'False' AND dim_Device.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
			AND (i.DateID &gt;= @Start_DateIndex) AND (i.DateID &lt;= @End_DateIndex)
			AND (dim_Device.[Is Managed Device Indicator] = 'Managed Device')
			AND (dim_Device.[Is Ignored Device Indicator] = 'Not an Ignored Device')
		GROUP BY dim_Date.[Calendar Year], dim_Date.[Week Number In Year]
		ORDER BY MIN(dim_Date.[Date]) asc
	END

	ELSE IF (@Interval = 'Monthly')
	BEGIN
		SELECT MIN(dim_Date.[Calendar Month]) + ' ' + CAST(dim_Date.[Calendar Year] AS varchar(10)) AS [Date],
			SUM(i.[Emergency Incidents Detected]) AS [Emergency], 
			SUM(i.[Alert Incidents Detected]) AS [Alert], 
			SUM(i.[Critical Incidents Detected]) AS [Critical], 
			SUM(i.[Error Incidents Detected]) AS [Error], 
			SUM(i.[Warning Incidents Detected]) AS [Warning]
		FROM [fact_SEM_Firewall Incidents] i 
		INNER JOIN [dim_Service] ON i.ServiceID = dim_Service.ServiceID
		INNER JOIN [dim_Device] ON i.DeviceID = dim_Device.DeviceID
		INNER JOIN @mCustomer c ON dim_Device.[N-central CustomerID] = c.CustomerID AND dim_Device.[DataSourceID] = c.[DataSourceID]
		INNER JOIN dim_Date ON i.DateID = dim_Date.DateID
		WHERE dim_Device.[Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows') 
			AND dim_Service.[ServiceName] NOT LIKE 'FW-Chk Point'
			AND ((@ShowDeleted = 'False' AND dim_Device.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
			AND (i.DateID &gt;= @Start_DateIndex) AND (i.DateID &lt;= @End_DateIndex)
			AND (dim_Device.[Is Managed Device Indicator] = 'Managed Device')
			AND (dim_Device.[Is Ignored Device Indicator] = 'Not an Ignored Device')
		GROUP BY dim_Date.[Calendar Year], dim_Date.[Month Number in Year]
		ORDER BY dim_Date.[Calendar Year] asc, dim_Date.[Month Number in Year] asc
	END

	ELSE -- Daily
		SELECT dim_Date.Date AS [Date], 
			SUM(i.[Emergency Incidents Detected]) AS [Emergency], 
			SUM(i.[Alert Incidents Detected]) AS [Alert], 
			SUM(i.[Critical Incidents Detected]) AS [Critical], 
			SUM(i.[Error Incidents Detected]) AS [Error], 
			SUM(i.[Warning Incidents Detected]) AS [Warning]
		FROM [fact_SEM_Firewall Incidents] i 
		INNER JOIN [dim_Service] ON i.ServiceID = dim_Service.ServiceID
		INNER JOIN [dim_Device] ON i.DeviceID = dim_Device.DeviceID
		INNER JOIN @mCustomer c ON dim_Device.[N-central CustomerID] = c.CustomerID AND dim_Device.[DataSourceID] = c.[DataSourceID]
		INNER JOIN dim_Date ON i.DateID = dim_Date.DateID
		WHERE dim_Device.[Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows') 
			AND dim_Service.[ServiceName] NOT LIKE 'FW-Chk Point'
			AND ((@ShowDeleted = 'False' AND dim_Device.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
			AND (i.DateID &gt;= @Start_DateIndex) AND (i.DateID &lt;= @End_DateIndex)
			AND (dim_Device.[Is Managed Device Indicator] = 'Managed Device')
			AND (dim_Device.[Is Ignored Device Indicator] = 'Not an Ignored Device')
		GROUP BY dim_Date.Date
		ORDER BY dim_Date.Date ASC
	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Emergency">
          <DataField>Emergency</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Alert">
          <DataField>Alert</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Critical">
          <DataField>Critical</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Error">
          <DataField>Error</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Warning">
          <DataField>Warning</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RPTES_SecurityMonitoring_PatchDetails">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!FirewallMonitoring.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
		BEGIN
			SELECT d.[Device Class], d.DeviceName, d.URI
			FROM dbo.dim_Device d
			WHERE 1=2
		END
	ELSE
	BEGIN
		DECLARE @End_Date datetime
		-- Get the start and end dates for the report
		SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)

		-- Create the date variables for reuse in the database filter.
		DECLARE @End_DateIndex int
		SELECT @End_DateIndex = dbo.dateIndex(@End_Date)


		DECLARE @Customer TABLE (DataSourceID int, CustomerID int)
		DECLARE @PatchUnappliedDevice TABLE (DeviceID int)	
		/* Bug # 1165 -- Patch Status support added */
		DECLARE @PatchServiceNames TABLE (ServiceName nvarchar(100))
		INSERT INTO @PatchServiceNames (ServiceName) VALUES ('Patch Level')
		INSERT INTO @PatchServiceNames (ServiceName) VALUES ('MBSA 1.2.1')
		INSERT INTO @PatchServiceNames (ServiceName) VALUES ('MBSA 2.0')
		INSERT INTO @PatchServiceNames (ServiceName) VALUES ('Patch Management')
		INSERT INTO @PatchServiceNames (ServiceName) VALUES ('Patch Status')
			
		-- Create a list of N-central CustomerID based on a list of dim_CustomerIDs
		INSERT INTO @Customer 
		SELECT [DataSourceID], [N-central CustomerID] CustomerID FROM dim_Customer 
		WHERE CustomerID IN (SELECT Value FROM inline_split_me(@CustomerID))

		-- Check if showing deleted devices
			DECLARE @ShowDeleted nvarchar(30) 	 
			SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')

		-- Insert ALL Windows Devices
		INSERT INTO @PatchUnappliedDevice
		SELECT DISTINCT d.DeviceID FROM dim_Device d
		INNER JOIN @Customer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
		WHERE (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS FLOAT ) ) AS DATETIME )) OR DeletedOn IS NULL)
			AND [Device Class] IN ('Windows Server', 'Windows Workstation','Windows Laptop') 
			AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
			AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
			AND [Is Managed Device Indicator] = 'Managed Device'
			AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')

		-- Remove Antivirus Devices From PatchUnappliedDevice
		DELETE @PatchUnappliedDevice FROM @PatchUnappliedDevice p
		INNER JOIN dim_Device d ON d.DeviceID = p.DeviceID
		INNER JOIN [fact_Device Services] ON d.DeviceID = [fact_Device Services].DeviceID 
		INNER JOIN [dim_Service] ON [fact_Device Services].ServiceID = dim_Service.ServiceID
		INNER JOIN @Customer c ON d.[N-central CustomerID] = c.CustomerID AND d.[DataSourceID] = c.[DataSourceID]
		WHERE [ServiceName] IN (SELECT ServiceName FROM @PatchServiceNames)
			AND [Device Class] IN ('Windows Server', 'Windows Workstation','Windows Laptop') 
			AND [Appliance Type] NOT IN ('Workgroup - Windows', 'Network - Windows')
			AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
			AND [Date Monitoring Started On] &lt;= @End_Date
			AND [Date Monitoring Stopped On] &gt; @End_Date
			AND [Is Managed Device Indicator] = 'Managed Device'
			AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
			

		SELECT d.[Device Class], d.DeviceName, d.URI
		FROM @PatchUnappliedDevice p
		INNER JOIN dim_Device d ON d.DeviceID = p.DeviceID 
		Where d.[Is Managed Device Indicator] = 'Managed Device'
			AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
		ORDER BY d.[Device Class], d.DeviceName
	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
