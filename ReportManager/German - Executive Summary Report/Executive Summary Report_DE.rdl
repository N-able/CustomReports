        </Field>
        <Field Name="serialnumber">
          <DataField>serialnumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Devices_with_expired_warranties">
          <DataField>Devices with expired warranties</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="OSInstallDate">
          <DataField>OSInstallDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Devices_with_warranties_that_will_expire_soon">
          <DataField>Devices with warranties that will expire soon</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Make_and_Model">
          <DataField>Make and Model</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
