  ELSE
  BEGIN 
  DECLARE @End_Date datetime	
  -- get the true start and end date based on subscription or not 
  SELECT @End_Date = [dbo].GetEndDate(@EndDate,Getdate(),@Subscription) 
   
  -- get the enddateid 
  DECLARE  @End_DateID INTEGER 
   
  SET @End_DateID = CONVERT(INT,CONVERT(CHAR(8),@End_Date,112)) 
   
  DECLARE  @ShowDeleted  nvarchar(30), 
           @DataSourceID int, 
           @ACServiceID  int 
   
  SELECT @ShowDeleted = (SELECT [value] 
                         FROM   config.dbo.config 
                         WHERE  [key] = 'ShowDeletedDevices') 
   
  SELECT @DataSourceID = datasourceid 
  FROM   dim_customer 
  WHERE  customerid = @CustomerID 
                       
  -- Get all the devices which have app compliance monitoring on 
  SELECT @ACServiceID = serviceid 
  FROM   dim_service 
  WHERE  [n-central serviceid] = 141 
         AND @DataSourceID = datasourceid 
   
  -- get all the parameters we care about 
  DECLARE  @mTmpFactReduced  TABLE( 
                                   deviceid        int, 
                                   applicationname nvarchar(1000) 
                                   ) 
   
  INSERT INTO @mTmpFactReduced 
  SELECT deviceid, 
         [application name] 
  FROM   [fact_device license compliance details] 
  WHERE  customerid = @CustomerID 
         AND [date not in compliance] &lt;= (@End_Date + ' 23:59') 
         AND ([date in compliance] IS NULL 
               OR [date in compliance] &gt;= (@End_Date + ' 23:59')) 
         AND deviceid IN (SELECT deviceid 
                          FROM   [fact_device services] 
                          WHERE  customerid = @CustomerID 
                                 AND (serviceid = @ACServiceID) 
                                 AND [date monitoring started on] &lt; (@End_Date + ' 23:59') 
                                 AND [date monitoring stopped on] &gt; (@End_Date + ' 23:59')) 
   
  -- merge with the dim_device 
  SELECT d.devicename       AS [Device_Name], 
         fr.applicationname AS [Application_Name] 
  FROM     @mTmpFactReduced fr 
           INNER JOIN dim_device d 
             ON d.deviceid = fr.deviceid 
  WHERE d.[Is Managed Device Indicator] = 'Managed Device'
		AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'
  ORDER BY d.devicename 
            
  END</CommandText>
