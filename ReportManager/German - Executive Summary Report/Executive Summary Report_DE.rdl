    <DataSet Name="dsWarrantyScore">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!UpdatedEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!UpdatedStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Customer_ID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ShowDeleted nvarchar(30) 
	SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')

SELECT
	CASE WHEN isnull(COUNT(DeviceName), 0) = 0 THEN -1 
		ELSE
		(CAST(Sum(CASE WHEN @EndDate &lt;= d.[Warranty Expiry Date] THEN 1
		ELSE 0 END) as float)/COUNT(DeviceName)) * 100 END AS Score
	,CASE WHEN isnull(COUNT(DeviceName), 0) = 0 THEN -1 
		ELSE
		(CAST(Sum(CASE WHEN DATEADD(day,-1,@StartDate) &lt;= d.[Warranty Expiry Date] THEN 1
		ELSE 0 END) as float)/COUNT(DeviceName)) * 100 END as Previous
FROM dim_Device d
	LEFT JOIN [fact_cim_computer system] cs
		ON cs.customerid = d.CustomerID
		AND cs.deviceid = d.DeviceID
	LEFT JOIN [fact_cim_operating system] os
		ON os.customerid = d.CustomerID
		AND os.deviceid = d.DeviceID
		AND os.installdate between @StartDate and @EndDate
	LEFT JOIN [fact_Device Management History] mh
		ON mh.CustomerID = d.CustomerID
		AND mh.DeviceID = d.DeviceID
WHERE d.CustomerID = @Customer_ID
	AND d.[Is Managed Device Indicator] = 'Managed Device'
	AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False') 
	AND d.[Is Ignored Device Indicator] = 'Not an Ignored Device'
	AND d.[Warranty Expiry Date] is not null
	AND d.[IsProbe Device Indicator] = 'Not a Probe Device'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Score">
          <DataField>Score</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Previous">
          <DataField>Previous</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
