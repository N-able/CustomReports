    <DataSet Name="dsApprovedApplications">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeApplicationLicenseCompliance.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
	BEGIN
		SELECT NULL AS ApplicationName1, NULL AS ApplicationName2, NULL AS ApplicationName3
		WHERE 1=2
	END
	ELSE
	BEGIN
	DECLARE @End_Date datetime
	-- get the true start and end date based on subscription or not
	SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)
	
	-- get the enddateid
	DECLARE @End_DateID INTEGER
	SET @End_DateID = CONVERT(int, CONVERT(char(8),@End_Date, 112))

	-- get the datasourceid
	DECLARE @DataSourceID INTEGER
	SELECT @DataSourceID = DataSourceID FROM dim_Customer WHERE CustomerID = @CustomerID

    DECLARE @ShowDeleted nvarchar(30) 	 
    SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')

	-- get the ServiceID, application compliance = 141
	DECLARE @ServiceID INTEGER
	SELECT @ServiceID = ServiceID FROM dim_Service WHERE [N-central ServiceID] = 141 AND DataSourceID = @DataSourceID

	-- get all the parameters we care about
	DECLARE @mTmpParametersReduced TABLE(
		TaskID INTEGER,
		ParameterName VARCHAR(40),
		ParameterValue VARCHAR(8000),
		Processed BIT
	)
	
	INSERT INTO @mTmpParametersReduced
		SELECT [Original TaskID], [Parameter Name], [Parameter Value], 0 
		FROM [fact_Device Service Parameters] 
		WHERE CustomerID = @CustomerID AND ServiceID = @ServiceID 
		  AND [Parameter Name] LIKE 'Matrix.0.Comparing.%' AND [Parameter Value] IS NOT NULL
		  AND [Parameter In Effect Started On] &lt; @End_Date
		  AND ([Parameter In Effect Ended On] IS NULL OR [Parameter In Effect Ended On] &gt; @End_Date)

	-- go through the list and create an application list
	DECLARE @mTmpApplicationList TABLE(
		ApplicationName VARCHAR(2000)
	)

	-- declare a few variables
	DECLARE @TaskID INTEGER, @ParameterName VARCHAR(40), @ParameterValue VARCHAR(8000), @Processed BIT

	-- go through the records using the cursor	
	SELECT TOP 1 @TaskID = TaskID, @ParameterName = ParameterName, @ParameterValue = ParameterValue, @Processed = Processed FROM @mTmpParametersReduced ORDER BY Processed 
	WHILE @Processed = 0
		BEGIN
			-- for each row, split out the applications
			INSERT INTO @mTmpApplicationList SELECT VALUE FROM inline_split_me(@ParameterValue)
			
			-- get the next record
			UPDATE @mTmpParametersReduced SET Processed = 1 WHERE TaskID = @TaskID AND ParameterName = @ParameterName
			SELECT TOP 1 @TaskID = TaskID, @ParameterName = ParameterName, @ParameterValue = ParameterValue, @Processed = Processed FROM @mTmpParametersReduced ORDER BY Processed
		END

	-- make a nice tmp table to store the distincts
	DECLARE @mTmpDistinctApplicationList TABLE(
		AppID INTEGER identity(1,1),
		ApplicationName VARCHAR(2000)
	)
	-- return our list of apps

	INSERT INTO @mTmpDistinctApplicationList (ApplicationName) 
		SELECT DISTINCT * FROM @mTmpApplicationList WHERE ApplicationName IS NOT NULL AND ApplicationName != '' ORDER BY ApplicationName

	-- now turn this into 3 columns
	DECLARE @mTmpFinalApplicationList TABLE(
		RowID INTEGER,
		ApplicationName1 VARCHAR(2000),
		ApplicationName2 VARCHAR(2000),
		ApplicationName3 VARCHAR(2000)
	)

	DECLARE @AppID INTEGER, @ApplicationName VARCHAR(2000)
	SELECT TOP 1 @AppID = AppID, @ApplicationName = ApplicationName FROM @mTmpDistinctApplicationList ORDER BY AppID

	WHILE @@RowCount != 0
		BEGIN
			IF (@AppID % 3) = 1
				INSERT INTO @mTmpFinalApplicationList (RowID, ApplicationName1) VALUES (@AppID, @ApplicationName)
			ELSE IF (@AppID % 3) = 2
				UPDATE @mTmpFinalApplicationList SET ApplicationName2 = @ApplicationName WHERE RowID = (@AppID - 1)
			ELSE IF (@AppID % 3) = 0
				UPDATE @mTmpFinalApplicationList SET ApplicationName3 = @ApplicationName WHERE RowID = (@AppID - 2)

			SELECT TOP 1 @AppID = AppID, @ApplicationName = ApplicationName FROM @mTmpDistinctApplicationList WHERE AppID = (@AppID + 1) ORDER BY AppID
		END

	SELECT ApplicationName1, ApplicationName2, ApplicationName3 FROM @mTmpFinalApplicationList

	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ApplicationName1">
          <DataField>ApplicationName1</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ApplicationName2">
          <DataField>ApplicationName2</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ApplicationName3">
          <DataField>ApplicationName3</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsNoCompliance">
      <Query>
        <DataSourceName>warehouse_db</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SectionEnabled">
            <Value>=Parameters!IncludeApplicationLicenseCompliance.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subscription">
            <Value>=Parameters!Subscription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerID">
            <Value>=Parameters!Customer_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- check to see if this section is enabled, if not, get out!
	IF (@SectionEnabled = 0)
	BEGIN
		SELECT NULL AS [Device Class], NULL AS [Device Name], NULL AS URI
		WHERE 1=2
	END
	ELSE
	BEGIN
	DECLARE @End_Date datetime
	-- get the true start and end date based on subscription or not
	SELECT @End_Date = [dbo].GetEndDate(@EndDate, GETDATE(), @Subscription)
	
	-- get the enddateid
	DECLARE @End_DateID int
	SET @End_DateID = CONVERT(int, CONVERT(char(8),@End_Date, 112))

	-- get the datasourceid
	DECLARE @DataSourceID int
	SELECT @DataSourceID = DataSourceID FROM dim_Customer WHERE CustomerID = @CustomerID

    DECLARE @ShowDeleted nvarchar(30) 	 
    SELECT @ShowDeleted = (SELECT [Value] FROM config.dbo.Config WHERE [Key]='ShowDeletedDevices')

	-- get the total number of Windows devices the customer had on the end date
	DECLARE @mTmpDeviceReduced TABLE(
		DeviceID int,
		[Device Class] nvarchar(100),
		[DeviceName] nvarchar(100),
		[URI] nvarchar(200)
	)
	INSERT INTO @mTmpDeviceReduced 
		SELECT DeviceID, [Device Class], DeviceName, [URI] FROM dim_Device d
		  WHERE (CAST( FLOOR( CAST( CreatedOn AS FLOAT ) ) AS DATETIME )) &lt;= @End_Date AND (@End_Date &lt;= (CAST( FLOOR( CAST( DeletedOn AS FLOAT ) ) AS DATETIME )) OR DeletedOn IS NULL)
			AND [N-central CustomerID] = (SELECT [N-central CustomerID] FROM dim_Customer WHERE CustomerID = @CustomerID)
			AND [Device Class] IN ('Windows Server', 'Windows Workstation','Windows Laptop')
			AND DataSourceID = @DataSourceID
			AND ((@ShowDeleted = 'False' AND d.[Status] = 'Active') OR @ShowDeleted &lt;&gt; 'False')
			AND [Is Managed Device Indicator] = 'Managed Device'
			AND (d.[Is Ignored Device Indicator] = 'Not an Ignored Device')
			

	-- determine how many of the devices are not being monitored for application compliance (141).
	-- first find out the serviceids, then figure out how many devices are being monitored, then subtract that number from the total 
	--  number of devices for that customer.
	DECLARE @ACServiceID int
	SELECT @ACServiceID = ServiceID FROM dim_Service WHERE [N-central ServiceID] = 141 AND @DataSourceID = DataSourceID

	-- remove from the reduced table the devices which have the services installed.
	DECLARE @TotalDevicesMissingServices int
	DELETE FROM @mTmpDeviceReduced WHERE DeviceID IN 
			(SELECT DeviceID FROM [Fact_Device Services] 
			WHERE CustomerID = @CustomerID AND DeviceID IN (SELECT DeviceID FROM @mTmpDeviceReduced)
				AND (ServiceID = @ACServiceID)
				AND [Date Monitoring Started On] &lt; @End_Date AND [Date Monitoring Stopped On] &gt; @End_Date)

	-- return what we have left
	SELECT [Device Class], [DeviceName] AS [Device Name], URI FROM @mTmpDeviceReduced ORDER BY [Device Class], [Device Name]

	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Device_Class">
          <DataField>Device Class</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Device_Name">
          <DataField>Device Name</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="URI">
          <DataField>URI</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
